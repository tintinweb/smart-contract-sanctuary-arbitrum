pragma solidity ^0.8.13;

import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";
import "../platform_integration/theproxy.sol";
import "../registry/IRegistryController.sol";
import "../registry/IRegistryConsumer.sol";
import "../platform_integration/CommunityRegistry.sol";
import "../platform_integration/community_list.sol";

import "./IAny2EVMMessageReceiver.sol";
import "./OmniSender.sol";


interface ISubscriptionManager {
    function createSubscription(uint32 community_id) external;
    function communitySubscriptions(uint32) external view returns (uint64);
    //function subscriptionStatuses(uint64) external view returns (SubscriptionStatus memory);
}


contract L2OmniReceiver is IERC165 ,IAny2EVMMessageReceiver,AccessControlEnumerable {

    function version() virtual external view returns(uint256) {
        return 20231010;
    }


    struct deferralData  {
        uint32          community_id;
        address         community_admin;
        string          community_name;
        bool            resolved;
    }

    mapping(uint64 => bool)             public valid_chains;
    mapping (uint32 => deferralData)    public bridgeRecords;
    bool                                public deferring;

    address reg = 0x1e8150050A7a4715aad42b905C08df76883f396F;

    event L2communityRegistryCreated(uint32 indexed community_id,address community_proxy);
    event L2communityCreationDeferred(uint32 indexed community_id,address indexed community_admin, string indexed community_name);
    event L2communityAdminCreated(uint32 indexed community_id,address indexed community_admin,string community_name);
    event CompletedOnL2(uint32 indexed community_id,address community_admin,string community_name);
    event MessageReceived(uint32 indexed community_id, address indexed community_admin, string community_name);
    //event L2CreationDeferred(uint32 community_id, address indexed community_admin, string community_name);
    event L2CommunityAlreadyExists(uint32 community_id);
    event RootSenderUpdated(address _old,address _new);
    event DeferringUpdated(bool _deferring);

    event ChainAdded(uint64 chainId,bytes description);

    event SubscriptionCreationFailed(uint32 community, string reason);
    event SubscriptionCreationFailedBadly(uint32 community, bytes reason);
    event SubscriptionCreated(uint32 community_id,uint64 subscription_id);


    error InvalidChain(uint64 chainSelector);

 


    bytes32 public constant COMMUNITY_REGISTRY_ADMIN = keccak256("COMMUNITY_REGISTRY_ADMIN");
    bytes32 public constant RECEIVER_ADMIN = keccak256("RECEIVER_ADMIN");

    address                     public rootSender;

   
    error InvalidSender(address expected, address actual);

    constructor(address _rootCommunityList) {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(RECEIVER_ADMIN,msg.sender);
        rootSender = _rootCommunityList;
    }

    function updateRootSender(address _rootSender) external onlyRole(RECEIVER_ADMIN) {
        emit RootSenderUpdated(rootSender ,_rootSender);
        rootSender = _rootSender;
    }

    function updateDeferralMode(bool _deferring) external onlyRole(RECEIVER_ADMIN) {
        deferring = _deferring;
        emit DeferringUpdated(_deferring);
    }

    function ccipReceive(Client.Any2EVMMessage calldata message) external 
        onlyRouter
        validChain(message.sourceChainSelector)  
    {
        address sender = abi.decode(message.sender,(address));
        require(sender == rootSender,"L2_OmniReceiver : INVALID_ROOT_SENDER");
        require(message.data.length > 24,"invalid data length");
        uint32 community_id            = uint32(bytes4(message.data[0:4]));
        address community_admin         = address(bytes20(message.data[4:24]));
        string  memory community_name   = string(message.data[24:message.data.length]);
        emit MessageReceived(community_id, community_admin, community_name);

        if (bridgeRecords[community_id].resolved) {
            emit L2CommunityAlreadyExists(community_id);
            return;
        } // in case manually created or already resolved;

        if (deferring) {
            bridgeRecords[community_id] = deferralData(community_id,community_admin, community_name, false);
            emit L2communityCreationDeferred(community_id, community_admin, community_name);
        } else {
            bridgeRecords[community_id] = deferralData(community_id,community_admin, community_name, true);
            _createCommunity(community_id,community_admin,community_name);
        }
   }

    function completeCommunityCreation(uint32 _community_id) external {
        require(msg.sender == bridgeRecords[_community_id].community_admin,"Unauthorised access");
        require(!bridgeRecords[_community_id].resolved,"Community already created");
        bridgeRecords[_community_id].resolved = true;
        _createCommunity(_community_id,msg.sender,bridgeRecords[_community_id].community_name);
        emit CompletedOnL2(_community_id, msg.sender, bridgeRecords[_community_id].community_name);
    }

    // @dev this function is provided in case the bridge fails. 
    // deferral record set in case the bridge resumes operation later.

    function createCommunity(
        uint32 community_id,
        address community_admin,
        string memory community_name
    ) external onlyRole(RECEIVER_ADMIN) {
        require(!bridgeRecords[community_id].resolved,"Community already created");
        bridgeRecords[community_id] = deferralData(community_id,community_admin, community_name, true);
        _createCommunity(community_id, community_admin, community_name);
        emit L2communityAdminCreated(community_id, community_admin, community_name);
    }


    function _createCommunity(
        uint32 community_id,
        address community_admin,
        string memory community_name
    ) internal {
        theproxy community_proxy = new theproxy("GOLDEN_COMMUNITY_REGISTRY"); // all golden contracts should start with `GOLDEN_`
        CommunityRegistry cr = CommunityRegistry(address(community_proxy));
        cr.init(community_id,community_admin,community_name);
        cr.setRegistryBool("IS_HOME_CHAIN",true);
        address cl = RegistryConsumer(reg).getRegistryAddress("COMMUNITY_LIST"); // one list so from galaxis registry
        community_list(cl).addCommunity(community_id, community_name,address(community_proxy)); // now the community list holds the registry address
        ISubscriptionManager subMan = ISubscriptionManager(RegistryConsumer(reg).getRegistryAddress("SUBSCRIPTION_MANAGER"));
        if (address(subMan) != address(0)) {
            try subMan.createSubscription(community_id) {

                uint64 id = subMan.communitySubscriptions(community_id);
                emit SubscriptionCreated(community_id,id);
            } catch Error(string memory reason) {
                emit SubscriptionCreationFailed(community_id,reason);
            } catch(bytes memory reason) {
                emit SubscriptionCreationFailedBadly(community_id,reason);
            } 
        }
        emit L2communityRegistryCreated(community_id,address(community_proxy));
    }

    function addIncomingChain(uint64 chainId, bool valid) external onlyRole(RECEIVER_ADMIN) {
        valid_chains[chainId] = valid;
    }

    modifier onlyRouter() {
        require(msg.sender == RegistryConsumer(reg).getRegistryAddress("CCIP_ROUTER"),"L2_OmniReceiver : Unauthorised");
        _;
    }

    modifier validChain(uint64 chainSelector) {
        if (!valid_chains[chainSelector]) revert InvalidChain(chainSelector);
        _;
    }

    function supportsInterface(
        bytes4 interfaceId)
    public pure override (AccessControlEnumerable,IERC165) returns (bool) {
        return 
                interfaceId == type(IAny2EVMMessageReceiver).interfaceId || 
                interfaceId == type(IERC165).interfaceId;
    }

    
}

