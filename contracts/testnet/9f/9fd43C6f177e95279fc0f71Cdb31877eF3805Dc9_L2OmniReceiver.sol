pragma solidity ^0.8.13;

import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";


import "./IRouterClient.sol";
import "../registry/IRegistryConsumer.sol";

contract OmniSender is Initializable, AccessControlEnumerableUpgradeable {

    bytes32 public constant CONTRACT_ADMIN = keccak256("CONTRACT_ADMIN");

    bool                      initialised;

    address constant          regAddress       = 0x1e8150050A7a4715aad42b905C08df76883f396F;
    RegistryConsumer constant galaxisRegistry  = RegistryConsumer(regAddress);

    mapping(uint64 => bytes) public s_chains;

    event MessageSent(bytes32 messageId);
    event ChainAdded(uint64 chainId,bytes description);

    error InvalidChain(uint64 chainSelector);


    function initialize()  public initializer {
        __AccessControlEnumerable_init();
        _init();
    }

    function _init() internal {
        require(!initialised,"Already initialised");
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(CONTRACT_ADMIN,msg.sender);
        initialised = true;
    }

    function sendDataPayFeeToken(
        uint64 destinationChainSelector,
        bytes memory receiver,
        bytes memory data
    ) external onlyRole(CONTRACT_ADMIN) returns (bytes32 messageId) {
        IRouterClient i_router = IRouterClient(galaxisRegistry.getRegistryAddress("CCIP_ROUTER"));
        Client.EVMTokenAmount[] memory tokenAmounts = new Client.EVMTokenAmount[](0);
        Client.EVM2AnyMessage memory message = Client.EVM2AnyMessage({
            receiver: receiver,
            data: data,
            tokenAmounts: tokenAmounts,
            // If extraArgs is left empty (0 length byte array), a default of 200,000 gas will be set and no strict sequencing.
            extraArgs: s_chains[destinationChainSelector],
            feeToken: galaxisRegistry.getRegistryAddress("CCIP_FEE_TOKEN")
        });
        // Optional uint256 fee = i_router.getFee(destChainSelector, message);
        // Can decide if fee is acceptable.
        // address(this) must have sufficient feeToken or the send will revert.
        messageId = i_router.ccipSend(destinationChainSelector, message);
        emit MessageSent(messageId);
    }

    function addChain(uint64 chainId, bytes calldata description) external onlyRole(CONTRACT_ADMIN) {
        s_chains[chainId] = description;
        emit ChainAdded(chainId,description);
    }

    modifier validChain(uint64 chainSelector) {
        if (s_chains[chainSelector].length == 0) revert InvalidChain(chainSelector);
        _;
    }

    function _msgData() internal pure override returns (bytes calldata) {
        return msg.data;
    }

    function _msgSender() internal view override returns (address) {
        return msg.sender;
    }

}

