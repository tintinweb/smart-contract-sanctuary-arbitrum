pragma solidity 0.8.17;

import "forge-std/Test.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@pythnetwork/pyth-sdk-solidity/IPyth.sol";
import "@pythnetwork/pyth-sdk-solidity/PythStructs.sol";
import "../../../interfaces/vault/IVault.sol";
import "../../../interfaces/vault/IReferralStorage.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";

struct Order {
    uint48 col;
    uint48 minPrice;
    uint48 maxPrice;
    uint32 validUntil;
    uint24 nonce;
    address player;
    uint16 lev;
    bool long;
}

struct Position {
  uint48 openPrice;
  uint48 bustPrice;
  uint48 col; // amount of margin in USD
  address player;
  uint32 openTime;
  uint16 lev; // in the asset (ETH or BTC)
  bool long;
  bool close;
}

struct FundFeeConfig {
    uint16 rate;
    uint16 buffer;
    uint16 period;
}

error PriceOutRange();
error MaxExposureReached(bool long);
error Halted();
error NotHalted();
error LiquidatedByFees();
error NotLiquidableByFees();

contract DegenPool {
  using ECDSA for bytes;
  using SafeERC20 for IERC20;

  event PositionExecuted(
    bytes indexed sig,
    Position position
  );
  
  event PositionLiquidated(
    bytes indexed sig,
    uint64 price,
    Position position
  );
  
  event PositionClosed(
    bytes indexed sig,
    uint64 price,
    Position position
  );
  
  event PositionClosedEmergency(
    bytes indexed sig,
    Position position
  );

  IPyth public immutable pyth;
  IVault public immutable vault; /// @notice Vault address
  IERC20 public immutable asset;
  bytes32 public immutable pythAssetId;

  uint96 public constant SCALE = 1e6;
  uint96 public constant minFee = 100000; /// @notice The minimum fee percentage 10%
  uint96 public constant minPriceMove = 100; /// @notice The minimum price move percentage
  uint96 public constant maxPriceMove = 100000; /// @notice The maximum price move percentage 10%, The pnl fee will be fixed after this price move, also 10% after this price move
  uint96 public constant maxFeeAtMaxPs = 820000; /// @notice The maximum fee at the maximum position size, occurring at a 0.01% price move, is 82% of the profit.
  uint96 public constant maxFeeAtMinPs = 500000; /// @notice The maximum fee at the minimum position size, occurring at a 0.01% price move, is 50% of the profit.
  uint96 public constant maxPositionSize = 1000000 * SCALE; /// @notice The maximum position size in dollar value 1m$ * 1e6

  // Exposure config
  uint8 public freshness = 60;
  address public controller; // Controller
  uint96 public maxProfit = 3500e6; // 1% for internal testing
  uint96 public maxLongExposure = 5000000e6;
  uint96 public maxShortExposure = 5000000e6;
  uint96 public totalLongExposure = 5000000e6;
  uint96 public totalShortExposure = 5000000e6;
  uint96 public interestLiquidationFee = 4e4; // 1% for internal testing
  uint256 public budget = 25000 * SCALE; // this configuration sets the max budget for losses before the contract pauses itself partly. it is sort of the credit line the contract has (given by the DAO) that the contract has to stay within. if the contract exceeds this budget it will stop accepting new positions and will only liquidate positions. this is to prevent the contract from going bankrupt suddenly. If the degen game is profitable (and the profits are collected by the vault) the budget will increase. In this way the value set/added to this value act as the 'max amount of losses possible'. The main purpose of this mechanism is to prevent draining of the vault. It is true that degen can still lose incredibly much if the game is profitable for years and suddently all historical profits are lost in a few  hours. To prevetn this the DAO can decrement so that the budget is reset.
  
  /// @notice Referral's rate 0.001
  uint96 public bribeRate = 1e4;
  /// @notice Referral storage address
  uint96 public pendingBribe;

  /// @notice Referral storage address
  IReferralStorage public refStore;
  /// @notice Referral storage address
  uint96 public refAmount;

  mapping(bytes => Position) public positions;
  mapping(address => uint96) public liquidatorFees; /// @notice amount of tokens liquidators can claim as a reward for liquidating positions
  mapping(address => bool) public keepers;
  FundFeeConfig public fundFeeConfig = FundFeeConfig(25, 180, 60);

  uint96 public minPositionSize = 1 * SCALE; /// @notice The minimum position size in dollar value 1$ * 1e8
  uint8 public pythUpdateFee = 1;

  modifier onlyKeeper() {
    require(keepers[msg.sender], "Invalid keeper");
    _;
  }

  modifier onlyPosOwnerAndNotClosed(bytes memory sig) {
    require(msg.sender == positions[sig].player, "Invalid position owner");
    require(!positions[sig].close, "Position already closed");
    _;
  }

  constructor(
    IPyth pyth_,
    IReferralStorage refStore_,
    bytes32 pythAssetId_,
    address asset_,
    address controller_,
    address vault_
  ) {
    pyth = pyth_;
    refStore = refStore_;
    pythAssetId = pythAssetId_;
    controller = controller_;
    asset = IERC20(asset_);
    vault = IVault(vault_);
  }

  function setKeeper(address addr, bool allow) external {
    require(msg.sender == controller, "DegenRouter: INVALID_SENDER");
    keepers[addr] = allow;
  }

  function setExpoConf(uint96 longExp, uint96 shortExp) external {
    require(msg.sender == controller, "DegenRouter: INVALID_SENDER");

    maxLongExposure = longExp;
    maxShortExposure = shortExp;
  }

  function getPosition(bytes memory sig) public view returns(Position memory) {
    return positions[sig];
  }

  function hashOrderToEthSigned(Order calldata order) public view returns (bytes32) {
    require(order.validUntil > block.timestamp, "ValidUntil too early");
    return ECDSA.toEthSignedMessageHash(keccak256(abi.encode(order)));
  }

  function hashOrder(Order calldata order) public view returns (bytes32) {
    require(order.validUntil > block.timestamp, "ValidUntil too early");
    return keccak256(abi.encode(order));
  }

  function verify(Order calldata order, bytes calldata signature_) public view returns(bool) {
    require(order.validUntil > block.timestamp, "Order is expired");
    return ECDSA.recover(ECDSA.toEthSignedMessageHash(keccak256(abi.encode(order))), signature_) == order.player;
  }

  function updatePrice(bytes[] calldata priceUpdateData) internal returns (uint64) {
    pyth.updatePriceFeeds{value: pythUpdateFee}(priceUpdateData);
    PythStructs.Price memory price = pyth.getPriceNoOlderThan(pythAssetId, freshness);
    return uint64(price.price);
  }

  /// @notice lets vault get wager amount from escrowed tokens
  /// @param token one of the whitelisted tokens which is collected in settings
  /// @param amount the amount of token
  function getEscrowedTokens(address token, uint256 amount) public {
    address addr = address(vault);
    require(msg.sender == addr, "DegenPoolManager: only vault");
    IERC20(token).safeTransfer(addr, amount);
  }

  function _increaseExposure(bool long, uint96 size) internal {
    if (long) {
        totalLongExposure += size;

        if (totalLongExposure > maxLongExposure) {
            totalLongExposure = maxLongExposure;
        }
    } else {
        totalShortExposure += size;

        if (totalShortExposure > maxShortExposure) {
            totalShortExposure = maxShortExposure;
        }
    }
  }

  function _decreaseExposure(bool long, uint96 size) internal {
    if (long) {
        if (size > totalLongExposure) revert MaxExposureReached(true);
        totalLongExposure -= size;
    } else {
        if (size > totalShortExposure) revert MaxExposureReached(false);
        totalShortExposure -= size;
    }
  }

  function setRefReward(address player, uint96 amount) internal returns (uint96 reward) {
    if (amount > 0) {
      reward = uint96(refStore.setReward(player, address(asset), uint256(amount / 1e2))) * 1e2;
    }
  }

  function execute(bytes[] calldata priceData, Order calldata order, bytes calldata sig) external onlyKeeper {
    require(verify(order, sig) == true, "Order is not verified");
    require(positions[sig].player == address(0), "Position already created");
    uint48 price = uint48(updatePrice(priceData));
    uint48 size = (order.col * order.lev);
    require(size <= maxPositionSize, "Position size too high");
    require(size >= minPositionSize, "Position size too low");

    // Transfer usdc.e
    IERC20(asset).safeTransferFrom(order.player, address(this), order.col);

    uint48 bustPrice = price;
    uint48 margin = price / order.lev; // TODO: check
    margin -= margin / 10;

    order.long ? bustPrice -= margin : bustPrice += margin;
    _decreaseExposure(order.long, size);

    Position memory pos = Position(
        price,
        bustPrice,
        order.col,
        order.player,
        uint32(block.timestamp),
        order.lev,
        order.long,
        false
    );

    positions[sig] = pos;

    emit PositionExecuted(sig, pos);
  }

  /**
   * @notice internal view returns the P/L of a position
   * @param size size of the position scaled 1e18
   * @param openPrice price when the position was opened in usd, scaled 1e18
   * @param price current price scaled 1e18
   * @param long true if the user is betting on the price going up, if false the user is betting on the price going down
   * @return pnl P/L of the position in the asset, this could be a positive or negative number, scaled 1e18 so pnl +$90 is 90 * 1e18
   */
  function _calcPnl(uint96 size, uint96 openPrice, uint64 price, bool long) internal pure returns (int96 pnl) {
    uint96 amount = (size * SCALE) / openPrice;
    int96 diff = int64(price) - int96(openPrice);

    if (long) {
      pnl = (int96(amount) * diff) / int96(SCALE);
    } else {
      pnl = (int96(amount) * -1 * diff) / int96(SCALE);
    }
  }

  /**
   * @notice internal view returns the amount of funding rate accured
   * @param openTime timestamp when the position was opened
   * @param size size of the position
   */
  function _calcFundingFee(uint32 openTime, uint96 size) internal view returns (uint96 fee) {
    uint32 curTime = uint32(block.timestamp);
    FundFeeConfig memory conf = fundFeeConfig;

    if (curTime >= openTime + conf.buffer) {
      uint32 percent = conf.rate * ((curTime - openTime) / conf.period);

      if (percent > SCALE) {
        percent = uint32(SCALE);
      }

      fee = (size * percent) / SCALE;
    }
  }

  function _calcProfitFee(uint96 priceMove, uint96 size) internal view returns (uint96) {
    // Check if the provided price move is within the specified range
    if (maxPriceMove > priceMove && priceMove >= minPriceMove) {
      // calculate the max fee
      uint96 diff = (maxFeeAtMaxPs - maxFeeAtMinPs) * SCALE ;

      uint96 positionRange = (maxPositionSize - minPositionSize) * SCALE;
      // calculate the fee using linear interpolation
      uint96 maxFee = (diff * (size - minPositionSize)) / (positionRange) + maxFeeAtMinPs;
      // calculate the shift amount based on the position size
      // shift amount will be add to the result of the fee calculation to shift the fee curve
      int96 shift = (-500001 + (int96(size) * 500001)) / int96(10 ** 13);
      // Calculate the fee using linear interpolation

      return uint96(shift) + maxFee - 
        ((maxFee - minFee) * (priceMove - minPriceMove)) / 
        (maxPriceMove - minPriceMove);
    }

    // If the price move is out of the specified range, return the min fee
    // it means the price move is greater than the max price move, it should return the min fee
    return minFee;
  }

  /**
   * @notice Function to calculate the price move percentage.
   * @param openPrice The open price of the position.
   * @param closePrice The close price of the position.
   */
  function _calcPriceMovePerc(
    uint96 openPrice,
    uint96 closePrice
  ) internal pure returns (uint96 pm_) {
    int96 diff = int96(closePrice) - int96(openPrice);
    // if the diff is negative, make it positive
    diff < 0 ? diff = diff * -1 : diff;
    return (uint96(diff) * SCALE) / openPrice;
  }

  function _share(uint96 amount, address player) internal returns (uint96) {
    uint96 bribe = (amount * bribeRate) / SCALE;
    uint96 reward = setRefReward(player, bribe);
    
    if (reward > 0) {
        refAmount += reward;
    }

    pendingBribe += bribe - reward;

    return bribe;
  }

  function calcProfit(uint96 col, uint96 size, uint96 openPrice, uint64 price, uint32 openTime, bool long)
  public view returns (uint96, uint96) {
    int96 profit = _calcPnl(size, openPrice, price, long);
    uint96 pm = _calcPriceMovePerc(openPrice, price);
    uint96 pf = (uint96(profit) * _calcProfitFee(pm, size)) / SCALE;
    uint96 ff = _calcFundingFee(openTime, size);
    uint96 excluded = uint96(profit) - pf;

    if (excluded > maxProfit) {
        excluded = maxProfit;
    }

    uint96 payout = excluded + col;

    payout = ff > payout ? 0 : payout - ff;
    excluded = ff > excluded ? 0 : excluded - ff;

    return (payout, excluded);
  }

  function _liquidate(bytes calldata sig, Position memory pos, uint64 price) internal {
    require(!pos.close, "Position is close");
    uint96 liqFee = (pos.col * interestLiquidationFee) / SCALE;
    uint96 shared = _share(pos.col, pos.player);
    uint96 payin = pos.col - liqFee - shared;

    liquidatorFees[msg.sender] += liqFee;
    budget += payin;
    vault.payin(address(asset), address(this), payin);

    emit PositionLiquidated(sig, price, pos);
  }

  function _close(address player, uint96 col, uint96 payout) internal {
    uint96 payin = col - payout;
    uint96 shared = _share(payin, player);

    budget += payin - shared;

    vault.payout(address(asset), address(this), col - shared, player, payout);
  }

  function _closeInLoss(bytes calldata sig, Position memory pos, uint64 price, uint96 size) internal onlyPosOwnerAndNotClosed(sig) {
    uint96 loss = uint96(_calcPnl(size, pos.openPrice, price, pos.long) * -1);
    uint96 fundFee = _calcFundingFee(pos.openTime, size);

    if (fundFee > 0) {
        loss += fundFee;
    }

    uint96 payout = loss > pos.col ? 0 : pos.col - loss;

    if (payout == 0) {
        revert LiquidatedByFees();
    }

    _close(pos.player, pos.col, payout);

    emit PositionClosed(sig, price, pos);
  }

  function _closeInProfit(bytes calldata sig, Position memory pos, uint64 price, uint96 size) internal onlyPosOwnerAndNotClosed(sig) {
    (uint96 payout, uint96 profit) = calcProfit(pos.col, size, pos.openPrice, price, pos.openTime, pos.long);

    if (profit > budget) {
        revert Halted();
    }

    if (payout > 0 && profit == 0) {
        _close(pos.player, pos.col, payout);
    } else if (payout == 0 && profit == 0) {
        revert LiquidatedByFees();
    } else {
        budget -= profit;
        vault.payout(address(asset), address(this), pos.col, pos.player, payout);
    }

    emit PositionClosed(sig, price, pos);
  }

  function closePosition(bytes[] calldata priceData, bytes calldata sig) external {
    uint64 price = updatePrice(priceData);
    Position memory pos = positions[sig];
    uint96 size = pos.col * pos.lev;

    if (pos.long) {
        if (price <= pos.bustPrice) {
            _liquidate(sig, pos, price);
        } else if (price <= pos.openPrice) {
            _closeInLoss(sig, pos, price, size);
        } else {
            _closeInProfit(sig, pos, price, size);
        }
    } else if (!pos.long) {
        if (price >= pos.bustPrice) {
            _liquidate(sig, pos, price);
        } else if (price >= pos.openPrice) {
            _closeInLoss(sig, pos, price, size);
        } else {
            _closeInProfit(sig, pos, price, size);
        }
    }

    positions[sig].close = true;
    _increaseExposure(pos.long, size);
  }

  function liquidateByFee(bytes[] calldata priceData, bytes calldata sig) external {
    uint64 price = updatePrice(priceData);
    Position memory pos = positions[sig];

    uint96 size = pos.col * pos.lev;
    uint96 payout = pos.col;

    // if the position in loss
    if (pos.long && price <= pos.openPrice || !pos.long && price >= pos.bustPrice) {
        uint96 loss = uint96(_calcPnl(size, pos.openPrice, price, pos.long) * -1);
        uint96 ff = _calcFundingFee(pos.openTime, size);

        payout = loss > payout ? 0 : payout - loss;
        payout = ff > payout ? 0 : payout - ff;
    } else {
        (payout,) = calcProfit(pos.col, size, pos.openPrice, price, pos.openTime, pos.long);
    }

    if (payout > 0) {
        revert NotLiquidableByFees();
    }

    positions[sig].close = true;
    _liquidate(sig, pos, price);
  }

  function emergencyClose(bytes[] calldata priceUpdateData, bytes calldata sig) external onlyPosOwnerAndNotClosed(sig) {
    uint64 price = updatePrice(priceUpdateData);
    Position memory pos = positions[sig];
    uint96 size = pos.col * pos.lev;

    (, uint96 profit_) = calcProfit(pos.col, size, pos.openPrice, price, pos.openTime, pos.long);

    if (profit_ < budget) {
        revert NotHalted();
    }

    positions[sig].close = true;
    asset.safeTransfer(address(pos.player), pos.col);
    _increaseExposure(pos.long, size);

    emit PositionClosedEmergency(sig, pos);
  }

    // function that allows the admin to deposit eth to the contract
  receive() external payable {}

  // function that allows the admin to withdraw eth from the contract
  function withdrawEth(address payable _to, uint256 _amount) external  {
    require(msg.sender == controller, "DegenRouter: INVALID_SENDER");
    _to.transfer(_amount);
  }
}

