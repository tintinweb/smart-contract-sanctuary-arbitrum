pragma solidity ^0.8;

import "@layerzerolabs/solidity-examples/contracts/contracts-upgradable/interfaces/ILayerZeroEndpointUpgradeable.sol";
import "@layerzerolabs/solidity-examples/contracts/contracts-upgradable/interfaces/ILayerZeroReceiverUpgradeable.sol";
import "hardhat/console.sol";

contract LzEndPoint is ILayerZeroEndpointUpgradeable {
    event SendLzMessage(
        uint16 indexed dstChainId,
        bytes indexed destination,
        bytes payload,
        address payable refundAddress,
        address zroPaymentAddress,
        bytes adapterParams
    );

    function receivePayload(
        uint16 _srcChainId,
        bytes calldata _srcAddress,
        address _dstAddress,
        uint64 _nonce,
        uint /*_gasLimit*/,
        bytes calldata _payload
    ) external override {
        ILayerZeroReceiverUpgradeable receiver = ILayerZeroReceiverUpgradeable(
            _dstAddress
        );
        receiver.lzReceive(_srcChainId, _srcAddress, _nonce, _payload);
    }

    function send(
        uint16 _dstChainId,
        bytes calldata _destination,
        bytes calldata _payload,
        address payable _refundAddress,
        address _zroPaymentAddress,
        bytes calldata _adapterParams
    ) external payable override {
        emit SendLzMessage(
            _dstChainId,
            _destination,
            _payload,
            _refundAddress,
            _zroPaymentAddress,
            _adapterParams
        );
    }

    function setConfig(
        uint16 _version,
        uint16 _chainId,
        uint _configType,
        bytes calldata _config
    ) external override {}

    function setSendVersion(uint16 _version) external override {}

    function setReceiveVersion(uint16 _version) external override {}

    function forceResumeReceive(
        uint16 _srcChainId,
        bytes calldata _srcAddress
    ) external override {}

    function getInboundNonce(
        uint16 _srcChainId,
        bytes calldata _srcAddress
    ) external view override returns (uint64) {}

    function getOutboundNonce(
        uint16 _dstChainId,
        address _srcAddress
    ) external view override returns (uint64) {}

    function estimateFees(
        uint16 _dstChainId,
        address _userApplication,
        bytes calldata _payload,
        bool _payInZRO,
        bytes calldata _adapterParam
    ) external view override returns (uint nativeFee, uint zroFee) {}

    function getChainId() external view override returns (uint16) {}

    function retryPayload(
        uint16 _srcChainId,
        bytes calldata _srcAddress,
        bytes calldata _payload
    ) external override {}

    function hasStoredPayload(
        uint16 _srcChainId,
        bytes calldata _srcAddress
    ) external view override returns (bool) {}

    function getSendLibraryAddress(
        address _userApplication
    ) external view override returns (address) {}

    function getReceiveLibraryAddress(
        address _userApplication
    ) external view override returns (address) {}

    function isSendingPayload() external view override returns (bool) {}

    function isReceivingPayload() external view override returns (bool) {}

    function getConfig(
        uint16 _version,
        uint16 _chainId,
        address _userApplication,
        uint _configType
    ) external view override returns (bytes memory) {}

    function getSendVersion(
        address _userApplication
    ) external view override returns (uint16) {}

    function getReceiveVersion(
        address _userApplication
    ) external view override returns (uint16) {}
}

