pragma solidity ^0.8.4;

contract PUNDOGE is ERC20, ERC721Holder, Ownable {
    IERC721 public nft;
    IERC20 public rewardToken; // Declare the new reward token
    mapping(uint256 => address) public tokenOwnerOf;
    mapping(uint256 => uint256) public tokenStakedAt;
    mapping (uint256 => bool) public isStaked;
    mapping(address => uint256) public rewardsEarnedByAddress;
    mapping(uint256 => uint256) public bonusRewards; // Mapping to store bonus reward percentages for each token ID
    mapping(address => uint256[]) public tokensStakedByAddress;
    mapping(uint256 => uint256) public tokenStartTime;
    mapping(uint256 => uint256) public initialTaxPercentage;

    uint256 public EMISSION_RATE = (1 * 10 ** 9) / 1 seconds;
    uint256 constant SECONDS_PER_DAY = 3600; // number of seconds in a day
    uint256 public taxPercentage = 30; // initialize with maximum tax percentage
    uint256 public minimumTaxPercentage = 5; // add minimum tax percentage

    uint256 public constant MAX_TAX_PERCENT = 100;  
    address public constant _nft = 0x2750631792B4F140C91F58CAB4A750b64f8811a0;
    address public constant _rewardToken = 0x391fcc6c6feB1551fbeBaC8796780585aD6CfD33;
    address public taxCollector = 0xb34E8d159872Ae54613f2ac14171cc40DCf50812; // replace with the address you want to use


    constructor() ERC20("PUNDOGE", "PUN") {    
        nft = IERC721(_nft);
        rewardToken = IERC20(_rewardToken); // Add this line to initialize the reward token
    }

   function stake(uint256 tokenId) external {

        isStaked[tokenId] = true;
        nft.safeTransferFrom(msg.sender, address(this), tokenId);
        tokenOwnerOf[tokenId] = msg.sender;
        tokenStakedAt[tokenId] = block.timestamp;

        tokenStartTime[tokenId] = block.timestamp; // store the start time of the staked token
        initialTaxPercentage[tokenId] = taxPercentage; // store the initial tax percentage of the staked token

       // Add the staked token ID to the array of tokens staked by the sender's address
         tokensStakedByAddress[msg.sender].push(tokenId);
    }


    function getCurrentTaxPercentage(uint256 tokenId) public view returns (uint256) {
        uint256 daysPassed = (block.timestamp - tokenStartTime[tokenId]) / SECONDS_PER_DAY;
        uint256 currentTaxPercentage = taxPercentage;
        if (daysPassed > 0) {
            uint256 taxReduction = daysPassed * 5;
            
            if (taxReduction > taxPercentage - minimumTaxPercentage) {
                currentTaxPercentage = minimumTaxPercentage;
            } else {
                currentTaxPercentage -= taxReduction;
            }
        }
        return currentTaxPercentage;
    }





    function unstakeWithoutReward(uint256 tokenId) external {
        require(tokenOwnerOf[tokenId] == msg.sender, "You can't unstake");
        nft.transferFrom(address(this), msg.sender, tokenId);
        delete tokenOwnerOf[tokenId];
        delete tokenStakedAt[tokenId];
         // Remove the unstaked token ID from the array of tokens staked by the sender's address
        uint256[] storage tokensStaked = tokensStakedByAddress[msg.sender];
        for (uint256 i = 0; i < tokensStaked.length; i++) {
            if (tokensStaked[i] == tokenId) {
                tokensStaked[i] = tokensStaked[tokensStaked.length - 1];
                tokensStaked.pop();
                break;
            }
        }
    }


    function unstake(uint256 tokenId) external {

        require(tokenOwnerOf[tokenId] == msg.sender, "You can't unstake");
        uint256 rewardAmount = calculateTokens(tokenId);
        uint256 currentTaxPercentage = getCurrentTaxPercentage(tokenId); // calculate the current tax percentage
        uint256 taxAmount = (rewardAmount * currentTaxPercentage) / MAX_TAX_PERCENT;
        uint256 amountAfterTax = rewardAmount - taxAmount;
        uint256 netRewardAmount = rewardAmount - taxAmount;
        require(rewardToken.balanceOf(address(this)) >= amountAfterTax, "Not enough reward tokens in contract balance");

        rewardToken.transfer(msg.sender, amountAfterTax); // Transfer the staker's reward tokens after deducting tax
        rewardToken.transfer(taxCollector, taxAmount); // Transfer the tax to the tax collector's address

        nft.transferFrom(address(this), msg.sender, tokenId);
        delete tokenOwnerOf[tokenId];
        delete tokenStakedAt[tokenId];
        isStaked[tokenId] = false;
    

    // Remove the unstaked token ID from the array of tokens staked by the sender's address
    uint256[] storage tokensStaked = tokensStakedByAddress[msg.sender];
    for (uint256 i = 0; i < tokensStaked.length; i++) {
        if (tokensStaked[i] == tokenId) {
            tokensStaked[i] = tokensStaked[tokensStaked.length - 1];
            tokensStaked.pop();
            break;
        }
    }

     // Update the rewardsEarnedByAddress mapping for the staker's address
    rewardsEarnedByAddress[msg.sender] += netRewardAmount;
}


function calculateTokens(uint256 tokenId) public view returns (uint256) {

        uint256 timeElapsed = block.timestamp - tokenStakedAt[tokenId];
        uint256 baseReward = timeElapsed * EMISSION_RATE;
        uint256 bonusReward = 0;

    if (bonusRewards[tokenId] > 0) {
        bonusReward = (baseReward * bonusRewards[tokenId]) / 100; // Calculate the bonus reward based on the bonus percentage for the token ID
    }

        return baseReward + bonusReward;
}


function removeFromTokensStakedByAddress(uint256 tokenId) internal {
    uint256[] storage tokensStaked = tokensStakedByAddress[msg.sender];
    for (uint256 i = 0; i < tokensStaked.length; i++) {
        if (tokensStaked[i] == tokenId) {
            tokensStaked[i] = tokensStaked[tokensStaked.length - 1];
            tokensStaked.pop();
            return;
        }
    }
}


    function getRewardsEarnedByAddress(address staker) external view returns (uint256) {

        return rewardsEarnedByAddress[staker];
    }

    function setBonusReward(uint256 tokenId, uint256 bonusPercentage) external onlyOwner {

        require(bonusPercentage <= 100, "Bonus percentage must be less than or equal to 100");
        bonusRewards[tokenId] = bonusPercentage;

    }

    function setEmissionRate(uint256 _emissionRate) external onlyOwner {
        EMISSION_RATE = _emissionRate;
    }

    function setUnstakeTax(uint256 _taxPercentage) external onlyOwner {
        taxPercentage = _taxPercentage;
    }

    function setMinimumTaxPercentage(uint256 _minimumTaxPercentage) external onlyOwner {
        minimumTaxPercentage = _minimumTaxPercentage;
    }

    function getTokensByOwner(address owner) external view returns (uint256[] memory) {
        uint256[] storage tokensStaked = tokensStakedByAddress[owner];
        uint256[] memory result = new uint256[](tokensStaked.length);
        for (uint256 i = 0; i < tokensStaked.length; i++) {
            result[i] = tokensStaked[i];
        }
        return result;
    }

    function getRewardTokenBalance() public view returns (uint256) {
        
        return rewardToken.balanceOf(address(this));
    }


    function getBonusReward(uint256 tokenId) public view returns (uint256) {
        return bonusRewards[tokenId];
    }


    
}