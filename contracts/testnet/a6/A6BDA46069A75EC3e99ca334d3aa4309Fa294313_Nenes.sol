pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/ERC20Burnable.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/ERC20Capped.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "../utils/Math.sol";
import "../PoposMinting.sol";

contract Nenes is ERC20Burnable, ERC20Capped, Ownable {
    using Math for uint256;

    bool private initialized;
    bool public isMint;
    bool public mintOver;
    uint256 public mintSst;
    uint256 public mintEst;
    uint256 public burnedCurrency;
    uint256 public mintedCurrency;
    uint256 public mintAddressCount;
    uint256 public mintFee = 25;
    uint256 public constant CAP_TOTAL_AMOUNT = 55_000_000_000_000_000e9;
    uint256 public constant MAX_MINT_AMOUNT = (CAP_TOTAL_AMOUNT * 20) / 100;
    uint256 public constant MAX_MINT_REWARDS = (CAP_TOTAL_AMOUNT * 35) / 100;
    uint256 public constant MAX_STAKE_REWARDS = (CAP_TOTAL_AMOUNT * 10) / 100;
    uint256 public constant MAX_ARIDROP_AMOUNT = (CAP_TOTAL_AMOUNT * 5) / 100;
    uint256 public constant MAX_LP_TOKENS = (CAP_TOTAL_AMOUNT * 30) / 100;
    uint256 public mintRewards;
    uint256 public stakeRewards;
    uint8 public taxFee = 3;
    address payable founAddress;
    address mintStakeAddress;
    address public poposPool;

    mapping(address => uint8) public userMints;

    event StartMint(uint256 mintStartTimestamp, uint256 mintEndTimestamp);
    event Mint(
        address indexed user,
        uint256 mintSeq,
        uint256 amount,
        uint256 depositedTimestamp,
        uint256 stakeDay
    );

    constructor() ERC20("NENES", "NENES") ERC20Capped(CAP_TOTAL_AMOUNT) {}

    function setFounAddress(address payable founAddr) public onlyOwner {
        founAddress = founAddr;
    }

    function setMintFee(uint256 mFee) public onlyOwner {
        mintFee = mFee;
    }

    function initMintStakeAddress(address mintStakeAddr) public onlyOwner {
        require(
            mintStakeAddr != address(0),
            "InitMintStakeAddress:Address is the zero address"
        );
        mintStakeAddress = mintStakeAddr;
        super._mint(mintStakeAddress, MAX_MINT_REWARDS);
    }

    function initPool(address _poposPool) public onlyOwner {
        require(!initialized, "Already initialized");
        require(
            _poposPool != address(0),
            "InitializePair:Cannot set with zero address"
        );
        poposPool = _poposPool;
        super._mint(_poposPool, MAX_LP_TOKENS);
        initialized = true;
    }

    function transfer(
        address to,
        uint256 amount
    ) public virtual override returns (bool) {
        return _poTransfer(_msgSender(), to, amount);
    }

    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) public virtual override returns (bool) {
        address spender = _msgSender();
        _spendAllowance(sender, spender, amount);
        return _poTransfer(sender, recipient, amount);
    }

    function _poTransfer(
        address sender,
        address recipient,
        uint256 amount
    ) internal returns (bool) {
        require(amount > 0, "PoTransfer:Cannot transfer with zero");
        uint8 txFee = calCurrentTaxFee();
        if (taxFee > 0) {
            amount = amount - (amount * txFee) / 100;
            _burn(sender, (amount * txFee) / 100);
            burnedCurrency = burnedCurrency + (amount * txFee) / 100;
        }
        _transfer(sender, recipient, amount);
        return true;
    }

    function allotMintEth(uint256 taxFeeCurrency) internal {
        require(taxFeeCurrency > 0, "AllotFeeCurrency:Cannot allot with zero");
        founAddress.transfer((taxFeeCurrency * 50) / 100);
    }

    function calCurrentTaxFee() public returns (uint8) {
        if (totalSupply() == 0) {
            return taxFee;
        }
        if ((burnedCurrency * 100) / totalSupply() >= 75) {
            burnedCurrency = 0;
            taxFee = taxFee - 1;
        }
        return taxFee;
    }

    function decimals() public pure override returns (uint8) {
        return 9;
    }

    function startMint() public onlyOwner {
        require(isMint == false, "StartMint:Mint is already opened");
        isMint = true;
        mintSst = block.timestamp;
        mintEst = mintSst + 31 days;
        emit StartMint(mintSst, mintEst);
    }

    function burnMintUnspent() public onlyOwner {
        require(block.timestamp > mintEst, "BurnMintUnspent:Illegal operation");
        require(
            MAX_MINT_AMOUNT - mintedCurrency > 0,
            "BurnMintUnspent:Remaining quantity is 0"
        );
        burn(MAX_MINT_AMOUNT - mintedCurrency);
    }

    function mint(uint256 stakeDay) public payable returns (bool) {
        require(isMint, "Mint:Mint is not available");
        require(!mintOver, "Mint:MintOver");
        require(stakeDay > 0 && stakeDay <= 365, "Mint:Invalid StakeDay");
        require(block.timestamp <= mintEst, "Mint:Mint is ended");
        require(
            userMints[msg.sender] < 10,
            "Mint:Address already minted 10 times"
        );
        require(msg.value * 10000 >= mintFee, "Mint:Insufficient amount");
        uint256 amount = (MAX_MINT_AMOUNT *
            (100_000 - mintAddressCount / 100)) / 10_000_000_000;
        require(amount > 0, "Mint: Cannot mint with zero");
        require(
            mintedCurrency + amount <= MAX_MINT_AMOUNT,
            "Mint:Exceeded the maximum mint"
        );
        mintedCurrency = mintedCurrency + amount;
        uint256 amountNext = (MAX_MINT_AMOUNT *
            (100_000 - (mintAddressCount + 1) / 100)) / 10_000_000_000;
        if(mintedCurrency + amountNext > MAX_MINT_AMOUNT){
            mintOver = true;
        }
        userMints[msg.sender] = userMints[msg.sender] + 1;
        _mint(mintStakeAddress, amount);
        emit Mint(
            msg.sender,
            mintAddressCount,
            amount,
            block.timestamp,
            stakeDay
        );
        PoposMinting(mintStakeAddress).mintStake(
            msg.sender,
            mintAddressCount,
            amount,
            stakeDay
        );
        mintAddressCount = mintAddressCount + 1;
        return true;
    }

    function _mint(
        address to,
        uint256 amount
    ) internal override(ERC20, ERC20Capped) {
        super._mint(to, amount);
    }

    event Received(address Sender, uint Value);

    receive() external payable {
        emit Received(msg.sender, msg.value);
    }
}

