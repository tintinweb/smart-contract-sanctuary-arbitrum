pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/IERC20Metadata.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";


contract PoposMinting is Ownable {

    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    using EnumerableSet for EnumerableSet.AddressSet;

    struct UserInfo {
        address user;
        uint256 amount;     // How many tokens the user has provided.
        uint256 depositedReward; // Deposited reward
        uint256 depositedTime; // Deposited timestamp
        uint256 depositedDays; // Deposited days
    }

    mapping(uint256 => UserInfo) public mintInfo;
    address public stakeToken;
    uint256 public stakeId = 0;
    uint256 public curRewardAmount;
    uint256 public constant ALL_REWARD_AMOUNT = 55_000_000_000_000_000 * 35 / 100;

    event MintStake(address indexed user, uint256 amount, uint256 depositedTimestamp, uint256 depositedDays);
    event Withdraw(address indexed user, uint256 mintSeq, uint256 amount,uint256 timestamp);
    event UpdateReward(string uid);

    function setStakeToken(address stoken) public onlyOwner {
        require(stoken != address(0), "PoposMinting:StakeToken cannot be zero address");
        stakeToken = address(stoken);
    }

    function mintStake(address user, uint256 mintSeq, uint256 amount, uint256 depositedDays) public returns(bool) {
        require(msg.sender == stakeToken, "PoposMinting:Illegal operation");
        require(stakeToken != address(0), "PoposMinting:StakeToken cannot be zero address");
        require(user != address(0), "PoposMinting:User cannot be zero address");
        require(amount > 0, "PoposMinting:Amount must be large than zero");
        require(mintInfo[mintSeq].amount == 0, "PoposMinting:Address already minted");
        IERC20(stakeToken).safeTransferFrom(user, address(this),amount);
        mintInfo[mintSeq] = UserInfo(user, amount, 0, block.timestamp, depositedDays);
        emit MintStake(user,mintInfo[mintSeq].amount, mintInfo[mintSeq].depositedTime, mintInfo[mintSeq].depositedDays);
        return true;
    }

    function updateReward(uint256[] memory mintSeqs, uint256[] memory rewards,string memory uid) public onlyOwner returns(bool){
        require(mintSeqs.length > 0 && mintSeqs.length == rewards.length, "PoposMinting:Invalid update datas");
        for(uint256 i = 0 ; i < mintSeqs.length ; i++){
            require(curRewardAmount + rewards[i] <= ALL_REWARD_AMOUNT, "PoposMinting:Mint stake reward exceeds the maximum");
            curRewardAmount = curRewardAmount + rewards[i];
            mintInfo[mintSeqs[i]].depositedReward = mintInfo[mintSeqs[i]].depositedReward + rewards[i];
        }
        emit UpdateReward(uid);
        return true;
    }

    function withdraw(uint256 mintSeq) public {
        require(mintInfo[mintSeq].user == msg.sender, "PoposMinting:Illegal operation");
        require(mintInfo[mintSeq].amount > 0, "PoposMinting:Withdraw with insufficient balance");
        require(block.timestamp >= mintInfo[mintSeq].depositedTime + mintInfo[mintSeq].depositedDays * 1 days, "PoposMinting:The pledge time has not been reached");
        uint256 amount = mintInfo[mintSeq].amount;
        uint256 depositedReward = mintInfo[mintSeq].depositedReward - calOvertimeDeduction(mintSeq);
        IERC20 token = IERC20(stakeToken);
        require(token.balanceOf(address(this)) >= amount + depositedReward, "PoposMinting:Reward pool insufficient balance");
        mintInfo[mintSeq].amount = 0;
        mintInfo[mintSeq].depositedReward = 0;
        token.transfer(msg.sender, amount + depositedReward);
        emit Withdraw(msg.sender, mintSeq, amount + depositedReward, block.timestamp);
    }

    function calOvertimeDeduction(uint256 mintSeq) view internal returns(uint256){
        UserInfo memory userInfo = mintInfo[mintSeq];
        uint256 expirationTime = userInfo.depositedTime + userInfo.depositedDays * 1 days;
        uint256 overtime = block.timestamp - expirationTime;
        uint256 overDays = (overtime - 1) / 1 days;
        return userInfo.depositedReward * overDays / 100;
    }

    function getMintInfo(uint256 mintSeq) view public returns(UserInfo memory){
        return mintInfo[mintSeq];
    }

}

