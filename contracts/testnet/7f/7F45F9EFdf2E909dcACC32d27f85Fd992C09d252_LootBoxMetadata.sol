   * @dev Returns a newly allocated string representing a JSON array with a dictionary-like structure.
   * Similar to how metadata is expected by Opensea for example.
   * @param array Array of strings to concatenate.
   * @return The concatenation of all strings.
   */
  function buildJsonKeyMapArray(
    string memory objectStart,
    string memory objectMid,
    string memory objectEnd,
    string[] memory keys,
    string[] memory values
  ) internal pure returns (string memory) {
    require(keys.length >= 0, "strings: Invalid metadata length");
    require(keys.length == values.length, "strings: Invalid metadata length");

    slice memory JSON_ARRAY_START = toSlice(JSON_ARRAY_START_STRING);
    slice memory JSON_ARRAY_END = toSlice(JSON_ARRAY_END_STRING);
    slice memory JSON_KEY_SEPARATOR = toSlice(JSON_KEY_SEPARATOR_STRING);

    slice memory start = toSlice(objectStart);
    slice memory mid = toSlice(objectMid);
    slice memory end = toSlice(objectEnd);

    uint baseObjLength = start._len + mid._len + end._len;
    uint separatorsLength = keys.length > 1
      ? (JSON_KEY_SEPARATOR._len * (keys.length - 1))
      : 0;
    uint totalLength = JSON_ARRAY_START._len +
      JSON_ARRAY_END._len +
      (baseObjLength * keys.length) +
      separatorsLength;
    slice[] memory keySlices = new slice[](keys.length);
    slice[] memory valueSlices = new slice[](values.length);
    for (uint i = 0; i < keys.length; i++) {
      keySlices[i] = toSlice(keys[i]);
      valueSlices[i] = toSlice(values[i]);
      totalLength += keySlices[i]._len;
      totalLength += valueSlices[i]._len;
    }

    string memory ret = new string(totalLength);
    uint retptr;
    assembly {
      retptr := add(ret, 32)
    }
    memcpy(retptr, JSON_ARRAY_START._ptr, JSON_ARRAY_START._len);
    retptr += JSON_ARRAY_START._len;
    for (uint i = 0; i < keySlices.length; i++) {
      memcpy(retptr, start._ptr, start._len);
      retptr += start._len;
      memcpy(retptr, keySlices[i]._ptr, keySlices[i]._len);
      retptr += keySlices[i]._len;
      memcpy(retptr, mid._ptr, mid._len);
      retptr += mid._len;
      memcpy(retptr, valueSlices[i]._ptr, valueSlices[i]._len);
      retptr += valueSlices[i]._len;
      memcpy(retptr, end._ptr, end._len);
      retptr += end._len;
      if (i < keySlices.length - 1) {
        memcpy(retptr, JSON_KEY_SEPARATOR._ptr, JSON_KEY_SEPARATOR._len);
        retptr += JSON_KEY_SEPARATOR._len;
      }
    }
    memcpy(retptr, JSON_ARRAY_END._ptr, JSON_ARRAY_END._len);
    retptr += JSON_ARRAY_END._len;
    return ret;
  }

  /*
   * @dev Joins an array of slices, using `self` as a delimiter, returning a
   *      newly allocated string.
   * @param self The delimiter to use.
   * @param parts A list of slices to join.
   * @return A newly allocated string containing all the slices in `parts`,
   *         joined with `self`.
   */
  function join(
    slice memory self,
    slice[] memory parts
  ) internal pure returns (string memory) {
    if (parts.length == 0) return "";

    uint length = self._len * (parts.length - 1);
    for (uint i = 0; i < parts.length; i++) length += parts[i]._len;

    string memory ret = new string(length);
    uint retptr;
    assembly {
      retptr := add(ret, 32)
    }

    for (uint i = 0; i < parts.length; i++) {
      memcpy(retptr, parts[i]._ptr, parts[i]._len);
      retptr += parts[i]._len;
      if (i < parts.length - 1) {
        memcpy(retptr, self._ptr, self._len);
        retptr += self._len;
      }
    }

    return ret;
  }
}

