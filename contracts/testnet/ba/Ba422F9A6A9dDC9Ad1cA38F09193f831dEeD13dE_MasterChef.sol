// File contracts/Masterchef.sol

pragma solidity ^0.8.0;
pragma experimental ABIEncoderV2;

contract MasterChef is IMasterChef, Ownable, ReentrancyGuard {
    using SafeMath for uint256;
    using SafeCast for uint256;
    using SafeERC20 for IERC20;
    using SafeERC20 for IERC20Mintable;
    using SignedSafeMath for int256;
    using SafeCast for int256;
    using AddressLib for bytes;

    /// @notice Info of each MCV user.
    /// `amount` LP token amount the user has provided.
    /// `rewardDebt` The amount of MDUST entitled to the user.
    struct UserInfo {
        uint256 amount;
        int256 rewardDebt;
    }

    /// @notice Info of each MCV pool.
    /// `allocPoint` The amount of allocation points assigned to the pool.
    /// Also known as the amount of MDUST to distribute per block.
    struct PoolInfo {
        uint256 accMDustPerShare;
        uint256 rewardPerBlock;
        uint64 lastRewardBlock;
        uint64 startBlock;
        uint64 endBlock;
        uint64 unlockBlock;
    }

    struct ClaimRewarder {
        bytes rewarders;
        uint256 pid;
        uint256 pendingReward;
        uint256 amount;
        address account;
        address to;
    }

    /// @notice Address of MDUST contract.
    IERC20Mintable public MDUST;

    // @notice The migrator contract. It has a lot of power. Can only be set through governance (owner).
    IMigratorChef public migrator;

    // MDUST tokens created per block.
    // uint256 public mdustPerBlock;

    /// @notice Info of each MCV pool.
    PoolInfo[] public override poolInfo;

    /// @notice Address of the LP token for each MCV pool.
    IERC20[] public lpToken;

    /// @notice Address of each `IRewarder` contract in MCV.
    bytes[] public rewarders;

    /// @notice Info of each user that stakes LP tokens.
    mapping(uint256 => mapping(address => UserInfo)) public override userInfo;

    // keeps track of total amount staked in each pool.
    mapping(uint256 => uint256) public override totalStaked;

    /// @dev Total allocation points. Must be the sum of all allocation points in all pools.
    // uint256 public totalAllocPoint;

    uint256 private constant ACC_MDUST_PRECISION = 1e12;

    event Deposit(address indexed user, uint256 indexed pid, uint256 amount, address indexed to);
    event Withdraw(address indexed user, uint256 indexed pid, uint256 amount, address indexed to);
    event EmergencyWithdraw(address indexed user, uint256 indexed pid, uint256 amount, address indexed to);
    event Harvest(address indexed user, uint256 indexed pid, uint256 amount);
    event LogPoolAddition(
        uint256 indexed pid,
        uint64 startBlock,
        uint64 endBlock,
        uint64 unlockBlock,
        uint256 rewardPerBlock,
        IERC20 indexed lpToken,
        bytes indexed rewarders
    );
    event LogSetPool(uint256 indexed pid, uint256 rewardPerBlock, bytes indexed rewarders, bool overwrite);
    event LogSetPoolBlocks(uint256 indexed pid, uint64 startBlock, uint64 endBlock);
    event LogUpdatePool(uint256 indexed pid, uint64 lastRewardBlock, uint256 lpSupply, uint256 accMDustPerShare);

    /// @param _mdust The MDUST token contract address.
    constructor(IERC20Mintable _mdust) {
        MDUST = _mdust;
    }

    function setMDust(IERC20Mintable _mdust) external onlyOwner {
        require(address(_mdust) != address(0), "invalid address");
        MDUST = _mdust;
    }

    /// @notice Returns the number of MCV pools.
    function poolLength() public view returns (uint256 pools) {
        pools = poolInfo.length;
    }

    // lock and withdraw reward after the reward start block.

    /// @notice Add a new LP to the pool. Can only be called by the owner.
    /// DO NOT add the same LP token more than once. Rewards will be messed up if you do.
    /// @param startBlock startBlock of pool.
    /// @param endBlock endBlock of pool.
    /// @param rewardPerBlock rewardPerBlock of pool.
    /// @param _lpToken Address of the LP ERC-20 token.
    /// @param _rewarders Address of the rewarder delegate.
    function add(
        uint64 startBlock,
        uint64 endBlock,
        uint64 unlockBlock,
        uint256 rewardPerBlock,
        IERC20 _lpToken,
        bytes calldata _rewarders
    ) public onlyOwner {
        require(!_rewarders.moreThanTwo(), "Rewarder is more than two");
        require(endBlock > startBlock && startBlock > block.number, "invalid start and end block");
        require(unlockBlock >= startBlock && unlockBlock <= endBlock, "invalid unlock block");
        uint256 lastRewardBlock = block.number;
        lpToken.push(_lpToken);
        rewarders.push(_rewarders);

        poolInfo.push(
            PoolInfo({
                startBlock: startBlock,
                endBlock: endBlock,
                unlockBlock: unlockBlock,
                rewardPerBlock: rewardPerBlock,
                lastRewardBlock: lastRewardBlock.toUint64(),
                accMDustPerShare: 0
            })
        );
        emit LogPoolAddition(
            lpToken.length.sub(1),
            startBlock,
            endBlock,
            unlockBlock,
            rewardPerBlock,
            _lpToken,
            _rewarders
        );
    }

    /// @notice Update the given pool's MDUST allocation point and `IRewarder` contract. Can only be called by the owner.
    /// @param _pid The index of the pool. See `poolInfo`.
    /// @param _rewardPerBlock New rewardPerBlock of the pool.
    /// @param _rewarders Address of the rewarder delegate.
    /// @param overwrite True if _rewarder should be `set`. Otherwise `_rewarder` is ignored.
    function set(uint256 _pid, uint256 _rewardPerBlock, bytes memory _rewarders, bool overwrite) public onlyOwner {
        poolInfo[_pid].rewardPerBlock = _rewardPerBlock;
        if (overwrite) {
            require(!_rewarders.moreThanTwo(), "Rewarder is more than two");
            rewarders[_pid] = _rewarders;
        }
        emit LogSetPool(_pid, _rewardPerBlock, overwrite ? _rewarders : abi.encodePacked(_rewarders[_pid]), overwrite);
    }

    //// @notice Update the given pools start and end times
    /// @param pid The index of the pool.
    /// @param startBlock new Startblock of the pool.
    /// @param endBlock new Endblock of the pool.
    /// @param unlockBlock new unlockBlock of the pool.
    function setPoolBlocks(uint256 pid, uint64 startBlock, uint64 endBlock, uint64 unlockBlock) public onlyOwner {
        require(poolInfo[pid].startBlock > block.number, "Pool already started");
        require(endBlock > startBlock && startBlock > block.number, "invalid block numbers");
        require(unlockBlock >= startBlock && unlockBlock <= endBlock, "invalid unlock block");
        poolInfo[pid].startBlock = startBlock;
        poolInfo[pid].endBlock = endBlock;
        poolInfo[pid].unlockBlock = unlockBlock;
        emit LogSetPoolBlocks(pid, startBlock, endBlock);
    }

    /// @notice Set the `migrator` contract. Can only be called by the owner.
    /// @param _migrator The contract address to set.
    function setMigrator(IMigratorChef _migrator) public onlyOwner {
        migrator = _migrator;
    }

    /// @notice Migrate LP token to another LP contract through the `migrator` contract.
    /// @param _pid The index of the pool. See `poolInfo`.
    function migrate(uint256 _pid) public {
        require(address(migrator) != address(0), "MasterChefV2: no migrator set");
        IERC20 _lpToken = lpToken[_pid];
        uint256 bal = _lpToken.balanceOf(address(this));
        _lpToken.approve(address(migrator), bal);
        IERC20 newLpToken = migrator.migrate(_lpToken);
        require(bal == newLpToken.balanceOf(address(this)), "MasterChefV2: migrated balance must match");
        lpToken[_pid] = newLpToken;
    }

    /// @notice View function to see pending MDUST on frontend.
    /// @param _pid The index of the pool. See `poolInfo`.
    /// @param _user Address of user.
    /// @return pending MDUST reward for a given user.
    function pendingMDust(uint256 _pid, address _user) external view returns (uint256 pending) {
        PoolInfo memory pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_user];
        uint256 accMDustPerShare = pool.accMDustPerShare;
        uint256 lpSupply = lpToken[_pid].balanceOf(address(this));
        if (block.number > pool.lastRewardBlock && lpSupply != 0) {
            uint256 blocks = block.number.sub(pool.lastRewardBlock);
            uint256 mdustReward = blocks.mul(pool.rewardPerBlock);
            accMDustPerShare = accMDustPerShare.add(mdustReward.mul(ACC_MDUST_PRECISION) / lpSupply);
        }
        pending = int256(user.amount.mul(accMDustPerShare) / ACC_MDUST_PRECISION).sub(user.rewardDebt).toUint256();
    }

    /// @notice Update reward variables for all pools. Be careful of gas spending!
    /// @param pids Pool IDs of all to be updated. Make sure to update all active pools.
    function massUpdatePools(uint256[] calldata pids) external nonReentrant {
        uint256 len = pids.length;
        for (uint256 i = 0; i < len; ++i) {
            updatePool(pids[i]);
        }
    }

    /// @notice Update reward variables of the given pool.
    /// @param pid The index of the pool. See `poolInfo`.
    /// @return pool Returns the pool that was updated.
    function updatePool(uint256 pid) public returns (PoolInfo memory pool) {
        pool = poolInfo[pid];
        if (
            block.number >= pool.startBlock &&
            pool.lastRewardBlock < pool.endBlock &&
            block.number > pool.lastRewardBlock
        ) {
            uint256 lpSupply = totalStaked[pid];
            uint256 currentblock = Math.min(block.number, pool.endBlock);
            if (lpSupply > 0) {
                uint256 blocks = currentblock.sub(pool.lastRewardBlock);
                uint256 mdustReward = blocks.mul(pool.rewardPerBlock);
                pool.accMDustPerShare = pool.accMDustPerShare.add(
                    (mdustReward.mul(ACC_MDUST_PRECISION) / lpSupply).toUint128()
                );
                if (!MDUST.mintingFinished()) {
                    MDUST.mint(address(this), mdustReward);
                }
            }
            pool.lastRewardBlock = currentblock.toUint64();
            poolInfo[pid] = pool;
            emit LogUpdatePool(pid, pool.lastRewardBlock, lpSupply, pool.accMDustPerShare);
        }
    }

    /// @notice Deposit LP tokens to MCV for MDUST allocation.
    /// @param pid The index of the pool. See `poolInfo`.
    /// @param amount LP token amount to deposit.
    /// @param to The receiver of `amount` deposit benefit.
    function deposit(uint256 pid, uint256 amount, address to) public nonReentrant {
        PoolInfo memory pool = updatePool(pid);
        UserInfo storage user = userInfo[pid][to];
        require(block.number >= pool.startBlock, "Pool not started");
        require(block.number <= pool.endBlock, "Pool ended");

        // Effects
        user.amount = user.amount.add(amount);
        user.rewardDebt = user.rewardDebt.add(int256(amount.mul(pool.accMDustPerShare) / ACC_MDUST_PRECISION));

        // Interactions
        _claimRewarders(
            ClaimRewarder({
                rewarders: rewarders[pid],
                pid: pid,
                pendingReward: 0,
                amount: user.amount,
                account: msg.sender,
                to: to
            })
        );

        totalStaked[pid] = totalStaked[pid].add(amount);

        lpToken[pid].safeTransferFrom(msg.sender, address(this), amount);

        emit Deposit(msg.sender, pid, amount, to);
    }

    /// @notice Withdraw LP tokens from MCV.
    /// @param pid The index of the pool. See `poolInfo`.
    /// @param amount LP token amount to withdraw.
    /// @param to Receiver of the LP tokens.
    function withdraw(uint256 pid, uint256 amount, address to) public nonReentrant {
        PoolInfo memory pool = updatePool(pid);
        UserInfo storage user = userInfo[pid][msg.sender];
        require(block.number >= pool.unlockBlock, "unlock block not reached");
        require(user.amount > 0, "bad withdraw");
        // Effects
        user.rewardDebt = user.rewardDebt.sub(int256(amount.mul(pool.accMDustPerShare) / ACC_MDUST_PRECISION));
        user.amount = user.amount.sub(amount);

        // Interactions
        _claimRewarders(
            ClaimRewarder({
                rewarders: rewarders[pid],
                pid: pid,
                pendingReward: 0,
                amount: user.amount,
                account: msg.sender,
                to: to
            })
        );

        totalStaked[pid] = totalStaked[pid].sub(amount);

        lpToken[pid].safeTransfer(to, amount);

        emit Withdraw(msg.sender, pid, amount, to);
    }

    /// @notice Harvest proceeds for transaction sender to `to`.
    /// @param pid The index of the pool. See `poolInfo`.
    /// @param to Receiver of MDUST rewards.
    function harvest(uint256 pid, address to) public nonReentrant {
        PoolInfo memory pool = updatePool(pid);
        UserInfo storage user = userInfo[pid][msg.sender];
        require(block.number >= pool.unlockBlock, "unlock block not reached");

        int256 accumulatedMDust = int256(user.amount.mul(pool.accMDustPerShare) / ACC_MDUST_PRECISION);
        uint256 _pendingMDust = accumulatedMDust.sub(user.rewardDebt).toUint256();

        // Effects
        user.rewardDebt = accumulatedMDust;

        // Interactions
        if (_pendingMDust != 0) {
            safeMDustTransfer(to, _pendingMDust);
        }
        _claimRewarders(
            ClaimRewarder({
                rewarders: rewarders[pid],
                pid: pid,
                pendingReward: _pendingMDust,
                amount: user.amount,
                account: msg.sender,
                to: to
            })
        );

        emit Harvest(msg.sender, pid, _pendingMDust);
    }

    /// @notice Withdraw LP tokens from MCV and harvest proceeds for transaction sender to `to`.
    /// @param pid The index of the pool. See `poolInfo`.
    /// @param amount LP token amount to withdraw.
    /// @param to Receiver of the LP tokens and MDUST rewards.
    function withdrawAndHarvest(uint256 pid, uint256 amount, address to) public nonReentrant {
        PoolInfo memory pool = updatePool(pid);
        UserInfo storage user = userInfo[pid][msg.sender];
        require(block.number >= pool.unlockBlock, "unlock block not reached");
        require(user.amount > 0, "bad withdraw");

        int256 accumulatedMDust = int256(user.amount.mul(pool.accMDustPerShare) / ACC_MDUST_PRECISION);
        uint256 _pendingMDust = accumulatedMDust.sub(user.rewardDebt).toUint256();

        // Effects
        user.rewardDebt = accumulatedMDust.sub(int256(amount.mul(pool.accMDustPerShare) / ACC_MDUST_PRECISION));
        user.amount = user.amount.sub(amount);

        // Interactions
        safeMDustTransfer(to, _pendingMDust);

        _claimRewarders(
            ClaimRewarder({
                rewarders: rewarders[pid],
                pid: pid,
                pendingReward: _pendingMDust,
                amount: user.amount,
                account: msg.sender,
                to: to
            })
        );

        totalStaked[pid] = totalStaked[pid].sub(amount);

        lpToken[pid].safeTransfer(to, amount);

        emit Withdraw(msg.sender, pid, amount, to);
        emit Harvest(msg.sender, pid, _pendingMDust);
    }

    // Safe mdust transfer function, just in case if rounding error causes pool to not have enough MDUSTs.
    function safeMDustTransfer(address _to, uint256 _amount) internal {
        uint256 mdustBal = MDUST.balanceOf(address(this));
        if (_amount > mdustBal) {
            MDUST.safeTransfer(_to, mdustBal);
        } else {
            MDUST.safeTransfer(_to, _amount);
        }
    }

    /**
     * When tokens are sent to the sale by mistake: withdraw the specified token.
     */
    function withdrawToken(address token, uint256 amount) external onlyOwner nonReentrant {
        require(amount > 0, "Withdrawable: amount should be greater than zero");
        IERC20(token).safeTransfer(owner(), amount);
    }

    /// @notice Withdraw without caring about rewards. EMERGENCY ONLY.
    /// @param pid The index of the pool. See `poolInfo`.
    /// @param to Receiver of the LP tokens.
    function emergencyWithdraw(uint256 pid, address to) public nonReentrant {
        UserInfo storage user = userInfo[pid][msg.sender];
        uint256 amount = user.amount;
        user.amount = 0;
        user.rewardDebt = 0;

        _claimRewarders(
            ClaimRewarder({
                rewarders: rewarders[pid],
                pid: pid,
                pendingReward: 0,
                amount: 0,
                account: msg.sender,
                to: to
            })
        );

        // Note: transfer can fail or succeed if `amount` is zero.
        lpToken[pid].safeTransfer(to, amount);
        emit EmergencyWithdraw(msg.sender, pid, amount, to);
    }

    function _claimRewarders(ClaimRewarder memory params) internal {
        address rewarder = params.rewarders.decodeFirstAddress();
        if (rewarder != address(0)) {
            IRewarder(rewarder).onMDustReward(
                params.pid,
                params.account,
                params.to,
                params.pendingReward,
                params.amount
            );
            if (params.rewarders.hasMultipleAddresses()) {
                params.rewarders = params.rewarders.skipToken();
                _claimRewarders(params);
            }
        }
    }
}