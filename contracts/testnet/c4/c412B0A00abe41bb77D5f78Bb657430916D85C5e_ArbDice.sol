pragma solidity ^0.8.17;

import { Initializable } from "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import { OwnableUpgradeable } from "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import { ReentrancyGuardUpgradeable } from "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import { ERC20Upgradeable } from "@openzeppelin/contracts-upgradeable/token/ERC20/ERC20Upgradeable.sol";
import { SafeERC20 } from "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import { IERC20 } from "@openzeppelin/contracts/interfaces/IERC20.sol";

contract ArbPool is Initializable, OwnableUpgradeable, ReentrancyGuardUpgradeable, ERC20Upgradeable {
    using SafeERC20 for IERC20;

    uint256 constant PRECISION = 1e10;
    uint256 constant MAX_FEE = 1e8; // 1%
    uint256 constant LP_INITIAL = 1e3;

    address public collateralToken;
    uint256 public fee;
    uint256 public feeReserve;
    address public feeDistributor;
    address public payeer;
    uint256 public paidAmount;

    /* ======== INITIALIZERS ========= */
    function initialize(address _collateralToken, uint256 _fee, address _feeDistributor) external initializer {
        __Ownable_init();
        __ReentrancyGuard_init();
        __ERC20_init("ARB-LP", "ALP");
        require(_fee < MAX_FEE, "fee > MAX_FEE");
        require(_feeDistributor != address(0), "InvalidAddress");
        require(_collateralToken != address(0), "InvalidAddress");
        collateralToken = _collateralToken;
        fee = _fee;
        feeDistributor = _feeDistributor;
    }

    function setCollateralToken(address _collateralToken) external onlyOwner {
        require(_collateralToken != address(0), "InvalidAddress");
        collateralToken = _collateralToken;
        emit CollateralTokenUpdate(collateralToken);
    }

    function setFeeDistributor(address _feeDistributor) external onlyOwner {
        require(_feeDistributor != address(0), "InvalidAddress");
        feeDistributor = _feeDistributor;
        emit FeeDistributorUpdate(_feeDistributor);
    }

    function setFee(uint256 _fee) external onlyOwner {
        require(_fee <= MAX_FEE, "fee > MAX_FEE");
        fee = _fee;
        emit FeeUpdate(_fee);
    }

    function setPayeer(address _payeer) external onlyOwner {
        require(_payeer != address(0), "InvalidAddress");
        payeer = _payeer;
        emit PayeerUpdate(payeer);
    }

    function deposit(address to, uint256 amountIn) external nonReentrant {
        if (amountIn == 0) {
            return;
        }

        (uint256 daoFee, uint256 lpAmount) = _calAddLp(amountIn);

        feeReserve += daoFee;
        IERC20(collateralToken).safeTransferFrom(msg.sender, address(this), amountIn);
        if (totalSupply() == 0) {
            lpAmount = lpAmount - LP_INITIAL;
            _mint(address(this), LP_INITIAL);
        }
        require(totalSupply() > 0, "INSUFFICIENT_LP");
        _mint(to, lpAmount);

        emit Deposit(to, amountIn, lpAmount);
    }

    function withdraw(address to, uint256 lpAmount) external nonReentrant {
        if (lpAmount == 0) {
            return;
        }

        (uint256 daoFee, uint256 amountOutAferFee) = _calRemoveLp(lpAmount);

        feeReserve += daoFee;
        _burn(msg.sender, lpAmount);
        IERC20(collateralToken).safeTransfer(to, amountOutAferFee);

        emit Withdraw(to, lpAmount, amountOutAferFee);
    }

    function payout(uint256 rewardAmount) external nonReentrant {
        require(msg.sender == payeer, "Only Payeer");

        require(rewardAmount <= poolBalance(), "Not enough reward");

        paidAmount += rewardAmount;
        IERC20(collateralToken).safeTransfer(msg.sender, rewardAmount);

        emit Payout(rewardAmount);
    }

    function poolBalance() public view returns (uint256) {
        return IERC20(collateralToken).balanceOf(address(this)) - feeReserve;
    }

    // ======== internal functions =========

    function _calAddLp(uint256 _amountIn) internal view returns (uint256 daoFee, uint256 lpAmount) {
        daoFee = (_amountIn * fee) / PRECISION;
        uint256 lpSupply = totalSupply();
        if (lpSupply == 0) {
            lpAmount = _amountIn * LP_INITIAL;
        } else {
            lpAmount = (_amountIn * lpSupply) / poolBalance();
        }
    }

    function _calRemoveLp(uint256 _amountLp) internal view returns (uint256 daoFee, uint256 amountOutAferFee) {
        uint256 lpSupply = totalSupply();
        uint256 amountOut = (_amountLp * poolBalance()) / lpSupply;
        daoFee = (amountOut * fee) / PRECISION;
        amountOutAferFee = amountOut - daoFee;
    }

    // ======== Event =========

    event Deposit(address investor, uint256 amountIn, uint256 amountLp);
    event Withdraw(address investor, uint256 amountLp, uint256 amountOut);
    event Payout(uint256 reward);
    event CollateralTokenUpdate(address);
    event FeeUpdate(uint256);
    event FeeDistributorUpdate(address);
    event PayeerUpdate(address);
}

