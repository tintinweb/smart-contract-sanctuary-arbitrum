pragma solidity ^0.8.19;

import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

import "../NFTBridgeData.sol";

contract IndividualURI is ERC721Enumerable, Ownable {

    bool        _initialised;
    string      _name;
    string      _symbol;

    address     _minter;

    mapping(uint256 => string) _uris;

    modifier onlyMinter() {
        require(msg.sender == _minter,"unauthorised");
        _;
    }

    constructor() ERC721("GOLDEN","BOY") Ownable(msg.sender) {
        _initialised = true;
    }

    function name() public override view returns (string memory) {
        return _name;
    }

    function symbol() public override view returns (string memory) {
        return _symbol;
    }

    function init(TokenStorageData calldata data) external {
        require(!_initialised,"BaseURI : contract already initialised");
        _name = data.name;
        _symbol = data.symbol;
        _transferOwnership(data.owner);
        _minter = msg.sender;
        _initialised = true;
    }

    function mintWithURI(address owner, uint256 tokenId, string calldata _uri) external onlyMinter {
        _mint(owner,tokenId);
        _uris[tokenId] = _uri;
    }

    function burn(uint256 tokenId) external onlyMinter {
        _burn(tokenId);
    }


}