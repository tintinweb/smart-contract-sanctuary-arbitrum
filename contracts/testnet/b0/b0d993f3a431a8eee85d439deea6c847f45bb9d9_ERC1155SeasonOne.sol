        _setRoleAdmin(Roles.FINANCIAL_CONTROLLER_PROTOCOL_ROLE, Roles.ADMIN);
        _setRoleAdmin(Roles.LOCKER_PROTOCOL_ROLE, Roles.ADMIN);
