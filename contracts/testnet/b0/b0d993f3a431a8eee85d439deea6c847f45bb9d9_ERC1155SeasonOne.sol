        uint256[] memory tokenIds,
        address seasonContract
    ) external onlyRole(Roles.REGISTRY_OPERATOR_PROTOCOL_ROLE) whenNotPaused {
        for (uint256 i = 0; i < tokenIds.length; i++) {
            _register(tokenIds[i], seasonContract);
        }
    }
}

