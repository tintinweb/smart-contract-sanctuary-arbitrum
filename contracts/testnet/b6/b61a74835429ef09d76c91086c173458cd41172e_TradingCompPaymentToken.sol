/**
   @title TradingCompPaymentToken
   @author float
   @notice Mimics a real payment token (like DAI) but does not allow transfer
   to anyone but the market contract.
*/
contract TradingCompPaymentToken is AccessControlledAndUpgradeable, ERC20Upgradeable, ERC20PermitUpgradeable, ITradingCompPaymentToken {
  /// @notice Role that is assigned to the single entity that is allowed to call mint
  bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

  /// @notice Role that is assigned to the single entity that is allowed to transfer the token
  bytes32 public constant RECIPIENT_ROLE = keccak256("RECIPIENT_ROLE");

  uint256[50] private __gap;
  // TODO: make the trading comp payment token compatible structurally with the other payment tokens we have in this repo
  bool public isPublic;

  /// @notice Creates an instance of the contract.
  /// @dev Should only be called by TokenFactory.sol for our system.
  /// @param _name ERC20 name
  /// @param _symbol ERC20 symbol
  /// @param _admin Contract upgrader and admin role
  /// @param _minter Only address allowed to mint tokens
  function initialize(
    string memory _name,
    string memory _symbol,
    address _admin,
    address _minter
  ) external initializer {
    _AccessControlledAndUpgradeable_init(_admin);
    __ERC20_init(_name, _symbol);
    __ERC20Permit_init(_name);

    _setupRole(MINTER_ROLE, _minter);
    _setupRole(RECIPIENT_ROLE, _admin);
  }

  /// @notice Add transfer roles to market and liquidity manager
  /// @param _market Address of the market
  /// @param _liquidityManager Address of the liquidity manager associated with the market
  function addMarket(address _market, address _liquidityManager) external override onlyRole(ADMIN_ROLE) {
    _setupRole(RECIPIENT_ROLE, _market);
    _setupRole(RECIPIENT_ROLE, _liquidityManager);
  }

  /// @notice Mints a number of payment tokens for an address.
  /// @dev Can only be called by addresses with a minter role.
  /// @param to The address for which to mint the tokens for.
  /// @param amount Amount of pool tokens to mint in wei.
  function mint(address to, uint256 amount) external override onlyRole(MINTER_ROLE) {
    _mint(to, amount);
  }

  function transferFrom(
    address from,
    address to,
    uint256 amount
  ) public virtual override(ERC20Upgradeable, IERC20Upgradeable) returns (bool) {
    if (hasRole(RECIPIENT_ROLE, to)) {
      // Skip requirement for aprroval
      _transfer(from, to, amount);
      return true;
    } else {
      super.transferFrom(from, to, amount);
    }
  }

  modifier onlyMinterOrRecipientRole(address from, address to) {
    require(
      isPublic ||
        hasRole(RECIPIENT_ROLE, to) ||
        hasRole(RECIPIENT_ROLE, from) ||
        from == /* for minting */
        address(0) ||
        hasRole(ADMIN_ROLE, msg.sender),
      "not-greenlisted for transfer"
    );
    _;
  }

  function _beforeTokenTransfer(
    address from,
    address to,
    uint256 amount
  ) internal virtual override onlyMinterOrRecipientRole(from, to) {
    super._beforeTokenTransfer(from, to, amount);
  }

  function setPublic(bool _isPublic) external onlyRole(ADMIN_ROLE) {
    isPublic = _isPublic;
  }

  /// Public mint functions for when the token is not being used for a trading competition:
  // Minting is public for easy testing on the mock.
  function mint(uint256 amount) public {
    require(isPublic, "disabled for trading comp");
    require(amount <= 10000e18, "Amount too high");
    super._mint(msg.sender, amount);
  }

  // Minting is public for easy testing on the mock.
  function mintFor(uint256 amount, address user) external {
    require(isPublic, "disabled for trading comp");
    require(amount <= 10000e18, "Amount too high");
    super._mint(user, amount);
  }

  // quick hack before start of trading competition to override these
  function name() public view virtual override returns (string memory) {
    return "Arctic Fake Coin 2";
  }

  function symbol() public view virtual override returns (string memory) {
    return "ARC2";
  }

  // hack: function to allow burning of payment tokens on the trading comp (to fix mistakes etc)
  //       For example we can burn the tokens of a user who has not participated in the trading comp
  function burnFor(address user, uint256 amount) public onlyRole(ADMIN_ROLE) {
    _burn(user, amount);
  }
}

