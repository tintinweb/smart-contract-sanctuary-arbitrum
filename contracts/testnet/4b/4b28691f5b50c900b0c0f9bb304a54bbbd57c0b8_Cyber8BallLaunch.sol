import "forge-std/Test.sol";
import {ERC721Holder} from "openzeppelin/token/ERC721/utils/ERC721Holder.sol";
import {Cyber8Ball} from "../src/Cyber8Ball.sol";
import {ICyber8BallLaunch} from "../src/ICyber8BallLaunch.sol";
import {BaseTest} from "./helper/BaseTest.sol";
import {Cyber8BallLaunchMock} from "./mock/Cyber8BallLaunchMock.sol";

contract Cyber8BallTest is BaseTest, ERC721Holder {
    event TokenBaseURIUpdated(string uri);

    Cyber8BallLaunchMock public cyber8Ball;

    function setUp() public {
        cyber8Ball = new Cyber8BallLaunchMock("Cyber8Ball", "C8B", 10000, address(0));
    }

    function testSetTokenBaseURIChangeTokenURI() public {
        cyber8Ball.__testMint(2);

        assertEq(cyber8Ball.tokenURI(0), "");
        assertEq(cyber8Ball.tokenURI(1), "");

        string memory uri = "ipfs://abc/";
        vm.expectEmit(true, true, true, true);
        emit TokenBaseURIUpdated(uri);
        cyber8Ball.setBaseTokenURI(uri);

        assertEq(cyber8Ball.tokenURI(0), string.concat(uri, "0"));
        assertEq(cyber8Ball.tokenURI(1), string.concat(uri, "1"));

        string memory uri2 = "ipfs://abc2/";
        vm.expectEmit(true, true, true, true);
        emit TokenBaseURIUpdated(uri2);
        cyber8Ball.setBaseTokenURI(uri2);

        assertEq(cyber8Ball.tokenURI(0), string.concat(uri2, "0"));
        assertEq(cyber8Ball.tokenURI(1), string.concat(uri2, "1"));
    }

    function testPublicMint() public {
        ICyber8BallLaunch.MintConfig memory config = ICyber8BallLaunch.MintConfig({
            priorOpen: false,
            whitelistOpen: false,
            publicOpen: true,
            publicMaxMintQuantityPerWallet: 10,
            signer: address(0)
        });
        cyber8Ball.setMintConfig(config);

        vm.prank(users[0], users[0]);
        cyber8Ball.publicMint(2);

        assertEq(cyber8Ball.totalSupply(), 2);
        assertEq(cyber8Ball.balanceOf(users[0]), 2);
        (uint256 priorMint, uint256 whitelist, uint256 publicMint) = cyber8Ball.walletMintCount(
            users[0]
        );
        assertEq(priorMint, 0);
        assertEq(whitelist, 0);
        assertEq(publicMint, 2);

        vm.prank(users[0], users[0]);
        vm.expectRevert(ICyber8BallLaunch.ExceedMaxMintQuantity.selector);
        cyber8Ball.publicMint(9);
    }

    function testPriorMint() public {
        {
            bytes memory signature = _signPriorMint(users[1], 2, userKeys[0]);
            vm.prank(users[1], users[1]);
            vm.expectRevert(ICyber8BallLaunch.MintNotOpen.selector);
            cyber8Ball.priorMint(1, 2, signature);
        }

        ICyber8BallLaunch.MintConfig memory config = ICyber8BallLaunch.MintConfig({
            priorOpen: true,
            whitelistOpen: false,
            publicOpen: false,
            publicMaxMintQuantityPerWallet: 0,
            signer: users[0]
        });
        cyber8Ball.setMintConfig(config);

        vm.startPrank(users[1], users[1]);
        {
            bytes memory signature = _signPriorMint(users[1], 2, userKeys[1]);
            vm.expectRevert(ICyber8BallLaunch.SignatureInvalid.selector);
            cyber8Ball.priorMint(2, 2, signature);
        }
        {
            bytes memory signature = _signPriorMint(users[1], 2, userKeys[0]);
            cyber8Ball.priorMint(1, 2, signature);
            assertEq(cyber8Ball.totalSupply(), 1);
            assertEq(cyber8Ball.balanceOf(users[1]), 1);
            (uint256 priorMint, , ) = cyber8Ball.walletMintCount(users[1]);
            assertEq(priorMint, 1);

            cyber8Ball.priorMint(1, 2, signature);
            assertEq(cyber8Ball.totalSupply(), 2);
            assertEq(cyber8Ball.balanceOf(users[1]), 2);
            (priorMint, , ) = cyber8Ball.walletMintCount(users[1]);
            assertEq(priorMint, 2);

            vm.expectRevert(ICyber8BallLaunch.ExceedMaxMintQuantity.selector);
            cyber8Ball.priorMint(1, 2, signature);
        }
    }

    function testWhitelistMint() public {
        {
            bytes memory signature = _signWhitelistMint(users[1], 2, userKeys[0]);
            vm.prank(users[1], users[1]);
            vm.expectRevert(ICyber8BallLaunch.MintNotOpen.selector);
            cyber8Ball.whitelistMint(1, 2, signature);
        }

        ICyber8BallLaunch.MintConfig memory config = ICyber8BallLaunch.MintConfig({
            priorOpen: false,
            whitelistOpen: true,
            publicOpen: false,
            publicMaxMintQuantityPerWallet: 0,
            signer: users[0]
        });
        cyber8Ball.setMintConfig(config);

        vm.startPrank(users[1], users[1]);
        {
            bytes memory signature = _signWhitelistMint(users[1], 2, userKeys[1]);
            vm.expectRevert(ICyber8BallLaunch.SignatureInvalid.selector);
            cyber8Ball.whitelistMint(2, 2, signature);
        }
        {
            bytes memory signature = _signWhitelistMint(users[1], 2, userKeys[0]);
            cyber8Ball.whitelistMint(1, 2, signature);
            assertEq(cyber8Ball.totalSupply(), 1);
            assertEq(cyber8Ball.balanceOf(users[1]), 1);
            (, uint256 whitelistMint, ) = cyber8Ball.walletMintCount(users[1]);
            assertEq(whitelistMint, 1);

            cyber8Ball.whitelistMint(1, 2, signature);
            assertEq(cyber8Ball.totalSupply(), 2);
            assertEq(cyber8Ball.balanceOf(users[1]), 2);
            (, whitelistMint, ) = cyber8Ball.walletMintCount(users[1]);
            assertEq(whitelistMint, 2);

            vm.expectRevert(ICyber8BallLaunch.ExceedMaxMintQuantity.selector);
            cyber8Ball.whitelistMint(1, 2, signature);
        }
    }

    function testTreasuryMint() public {
        vm.prank(users[0], users[0]);
        vm.expectRevert("Ownable: caller is not the owner");
        cyber8Ball.treasuryMint(20);

        cyber8Ball.treasuryMint(100);
        assertEq(cyber8Ball.totalSupply(), 100);
        assertEq(cyber8Ball.balanceOf(address(this)), 100);

        cyber8Ball.treasuryMint(100);
        assertEq(cyber8Ball.balanceOf(address(this)), 200);

        vm.expectRevert(ICyber8BallLaunch.ExceedMaxMintQuantity.selector);
        cyber8Ball.treasuryMint(1);
    }

    function testTreasuryMintExceedMaxSupply() public {
        ICyber8BallLaunch.MintConfig memory config = ICyber8BallLaunch.MintConfig({
            priorOpen: false,
            whitelistOpen: false,
            publicOpen: true,
            publicMaxMintQuantityPerWallet: 10000,
            signer: address(this)
        });
        cyber8Ball.setMintConfig(config);
        cyber8Ball.publicMint(9900);

        cyber8Ball.treasuryMint(100);
        assertEq(cyber8Ball.totalSupply(), 10000);

        vm.expectRevert(ICyber8BallLaunch.ExceedMaxSupply.selector);
        cyber8Ball.treasuryMint(1);
    }

    function _signPriorMint(
        address wallet,
        uint256 maxQuantity,
        uint256 signerPrivateKey
    ) private view returns (bytes memory) {
        bytes32 digest = cyber8Ball.getPriorMintSigDigest(wallet, maxQuantity);
        (uint8 v, bytes32 r, bytes32 s) = vm.sign(signerPrivateKey, digest);
        bytes memory signature = abi.encodePacked(r, s, v);
        return signature;
    }

    function _signWhitelistMint(
        address wallet,
        uint256 maxQuantity,
        uint256 signerPrivateKey
    ) private view returns (bytes memory) {
        bytes32 digest = cyber8Ball.getWhitelistMintSigDigest(wallet, maxQuantity);
        (uint8 v, bytes32 r, bytes32 s) = vm.sign(signerPrivateKey, digest);
        bytes memory signature = abi.encodePacked(r, s, v);
        return signature;
    }
}

