// SPDX-License-Identifier: MIT
pragma solidity 0.8.18;

import "forge-std/Test.sol";
import {Cyber8BallLaunch} from "../../src/Cyber8BallLaunch.sol";

contract Cyber8BallLaunchMock is Cyber8BallLaunch {
    bytes32 private constant _PRIOR_MINT_TYPE_HASH =
        keccak256("PriorMint(address wallet,uint256 maxQuantity)");
    bytes32 private constant _WHITELIST_MINT_TYPE_HASH =
        keccak256("WhitelistMint(address wallet,uint256 maxQuantity)");

    constructor(
        string memory name_,
        string memory symbol_,
        uint256 minGasToTransfer_,
        address lzEndpoint_
    ) Cyber8BallLaunch(name_, symbol_, minGasToTransfer_, lzEndpoint_) {}

    function __testMint(uint256 quantity) public {
        for (uint256 i = 0; i < quantity; i++) {
            _safeMint(_msgSender(), totalSupply());
        }
    }

    function getPriorMintSigDigest(
        address wallet,
        uint256 maxQuantity
    ) public view returns (bytes32) {
        return _hashTypedDataV4(keccak256(abi.encode(_PRIOR_MINT_TYPE_HASH, wallet, maxQuantity)));
    }

    function getWhitelistMintSigDigest(
        address wallet,
        uint256 maxQuantity
    ) public view returns (bytes32) {
        return
            _hashTypedDataV4(keccak256(abi.encode(_WHITELIST_MINT_TYPE_HASH, wallet, maxQuantity)));
    }
}