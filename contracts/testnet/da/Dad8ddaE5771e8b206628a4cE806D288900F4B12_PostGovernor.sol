pragma solidity ^0.8.2;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/governance/extensions/GovernorTimelockControl.sol";
import "@openzeppelin/contracts/utils/math/Math.sol";
import "../../interfaces/IVePost.sol";
import "./PostGovernorCounting.sol";

import "hardhat/console.sol";

contract PostGovernor is PostGovernorCounting, GovernorTimelockControl {
    using Math for uint256;
    uint256 private _delay = 60 * 60 * 4; // 4 hours
    uint256 private _period = 60 * 60 * 24; // ~ 1 day
    uint256 private _threshold = 0;
    uint256 private _quorum = 1000000 * 10 ** 18;
    // Dev has right to propose a proposal only
    address public dev;
    address public timelockController;

    IVePost public vePost;

    error NotSupportFunction();

    struct ProposalReward {
        uint256 totalReward;
        uint256 totalCountedReward;
        uint256 maxAmount;
        uint256 totalDistributed;
    }

    mapping(uint256 => ProposalReward) public proposalReward;

    // only dev can propose a proposal
    modifier onlyDev() {
        require(_msgSender() == dev, "Only dev");
        _;
    }

    event DevChanged(address indexed oldDev, address indexed newDev);
    event DelayChanged(uint256 indexed oldDelay, uint256 indexed newDelay);
    event PeriodChanged(uint256 indexed oldPeriod, uint256 indexed newPeriod);
    event ThresholdChanged(
        uint256 indexed oldThreshold,
        uint256 indexed newThreshold
    );
    event QuorumChanged(uint256 indexed oldQuorum, uint256 indexed newQuorum);

    event VoteDelegated(
        address indexed delegator,
        address indexed delegatee,
        uint256 indexed proposalId,
        uint256 weight
    );

    constructor(
        TimelockController _timelock,
        IVePost _vePost
    ) Governor("Post Tech Governor") GovernorTimelockControl(_timelock) {
        timelockController = address(_timelock);
        dev = _msgSender();
        vePost = _vePost;
    }

    // only dev can change new dev
    function changeDev(address newDev) public onlyDev {
        emit DevChanged(dev, newDev);
        dev = newDev;
    }

    function setVePost(address _vePost) public onlyDev {
        vePost = IVePost(_vePost);
    }

    function changeDelay(uint256 delay) public onlyGovernance {
        emit DelayChanged(_delay, delay);
        _delay = delay;
    }

    function changePeriod(uint256 period) public onlyGovernance {
        emit PeriodChanged(_period, period);
        _period = period;
    }

    function changeThreshold(uint256 threshold) public onlyGovernance {
        emit ThresholdChanged(_threshold, threshold);
        _threshold = threshold;
    }

    function changeQuorum(uint256 quorum_) public onlyGovernance {
        emit QuorumChanged(_quorum, quorum_);
        _quorum = quorum_;
    }

    function votingDelay() public view override(Governor) returns (uint256) {
        return _delay;
    }

    function votingPeriod() public view override returns (uint256) {
        return _period;
    }

    function proposalThreshold() public view override returns (uint256) {
        return _threshold;
    }

    // The functions below are overrides required by Solidity.

    function quorum(
        uint256 /*blockNumber*/
    ) public view override(Governor) returns (uint256) {
        return _quorum;
    }

    function state(
        uint256 proposalId
    )
        public
        view
        override(Governor, GovernorTimelockControl)
        returns (ProposalState)
    {
        return super.state(proposalId);
    }

    // only dev can propose a proposal
    function propose(
        address[] memory targets,
        uint256[] memory values,
        bytes[] memory calldatas,
        string memory description
    ) public virtual override(Governor) onlyDev returns (uint256) {
        return super.propose(targets, values, calldatas, description);
    }

    /// delegate vote to delegatee with proposalId
    function delegateVote(address delegatee, uint256 proposalId) external {
        address delegator = msg.sender;
        /// Check can not delegate yourself and delegate to delegate
        require(
            delegatee != delegator && !isDelegatee[delegator][proposalId],
            "Can not delegate loop"
        );
        // Check delegatee not vote before
        require(
            !hasVoted(proposalId, delegatee) &&
                !hasVoted(proposalId, delegator),
            "Delegatee or voter must not vote before"
        );

        /// Voter must not delegate before
        require(
            mappingDelegatee[delegator][proposalId].delegator == address(0),
            "You already delegate to someone"
        );
        /// Check proposal in active
        require(
            state(proposalId) == ProposalState.Active,
            "Proposal not active"
        );

        /// Get weight of delegator
        uint256 weight = vePost.getTotalWeightOfOwner(
            delegator,
            proposalSnapshot(proposalId)
        );

        isDelegatee[delegatee][proposalId] = true;

        mappingWeight[delegatee][proposalId] += weight;

        mappingDelegatee[delegator][proposalId] = DelegateVote({
            delegator: delegatee,
            weightDelegate: weight
        });

        emit VoteDelegated(delegator, delegatee, proposalId, weight);
    }

    function _cancel(
        address[] memory targets,
        uint256[] memory values,
        bytes[] memory calldatas,
        bytes32 descriptionHash
    ) internal override(Governor, GovernorTimelockControl) returns (uint256) {
        return super._cancel(targets, values, calldatas, descriptionHash);
    }

    function _executor()
        internal
        view
        override(Governor, GovernorTimelockControl)
        returns (address)
    {
        return timelockController;
    }

    function supportsInterface(
        bytes4 interfaceId
    ) public view override(Governor) returns (bool) {
        return super.supportsInterface(interfaceId);
    }

    function _castVote(
        uint256 proposalId,
        address account,
        uint8 support,
        string memory reason
    ) internal virtual override(Governor) returns (uint256) {
        require(mappingDelegatee[account][proposalId].delegator == address(0), "Can not cast vote when delegated");
        uint256 _weight = super._castVote(proposalId, account, support, reason);

        uint256 totalWeightDelegate = 0;
        /// If delegatee vote, we will add weight to delegatee
        if (isDelegatee[account][proposalId]) {
            totalWeightDelegate = mappingWeight[account][proposalId];

            _countDelegateVote(
                proposalId,
                account,
                support,
                totalWeightDelegate,
                ""
            );
        }
        return _weight + totalWeightDelegate;
    }

    function _getVotes(
        address account,
        uint256 timepoint,
        bytes memory /*params*/
    ) internal view virtual override(Governor) returns (uint256) {
        return vePost.getTotalWeightOfOwner(account, timepoint);
    }

    function clock() public view override(Governor) returns (uint48) {
        return uint48(block.timestamp);
    }

    function CLOCK_MODE()
        public
        view
        virtual
        override(Governor)
        returns (string memory)
    {
        return "CLOCK_MODE";
    }

    function proposalNeedsQueuing(
        uint256
    )
        public
        view
        virtual
        override(GovernorTimelockControl, Governor)
        returns (bool)
    {
        return GovernorTimelockControl.proposalNeedsQueuing(0);
    }

    function _queueOperations(
        uint256 proposalId,
        address[] memory targets,
        uint256[] memory values,
        bytes[] memory calldatas,
        bytes32 descriptionHash
    )
        internal
        virtual
        override(GovernorTimelockControl, Governor)
        returns (uint48)
    {
        return
            GovernorTimelockControl._queueOperations(
                proposalId,
                targets,
                values,
                calldatas,
                descriptionHash
            );
    }

    function _executeOperations(
        uint256 proposalId,
        address[] memory targets,
        uint256[] memory values,
        bytes[] memory calldatas,
        bytes32 descriptionHash
    ) internal virtual override(GovernorTimelockControl, Governor) {
        GovernorTimelockControl._executeOperations(
            proposalId,
            targets,
            values,
            calldatas,
            descriptionHash
        );
    }
}

