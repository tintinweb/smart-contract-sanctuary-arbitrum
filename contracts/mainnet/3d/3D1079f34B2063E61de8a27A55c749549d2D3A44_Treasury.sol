/**
 * @title Treasury - A contract that manages deposit and withdrawal to/from itself
 */
contract Treasury is Ownable, SignatureChecker {
    using SafeERC20 for IERC20;

    address constant ETH_ADDRESS = 0xEeeeeEeeeEeEeeEeEeEeeEEEeeeeEeeeeeeeEEeE;
    mapping(address => uint256) public nonces;
    address public signer;

    event WithdrawnFromTreasury(
        address indexed to,
        address indexed tokenAddress,
        uint256 amount,
        uint256 nonce
    );

    event DepositToTreasury(
        address indexed from,
        address indexed tokenAddress,
        uint256 amount
    );

    event EventFutureActions(address from, bytes extraData);

    event NewSignerSet(address signer);

    event withdrawByOwner(
        address indexed to,
        address indexed tokenAddress,
        uint256 amount
    );

    /**
     * @dev initialize the contract
     * @param _owner owner address
     * @param _name name of the contract
     * @param _version version of the contract
     */
    constructor(
        address _owner,
        string memory _name,
        string memory _version
    ) Ownable(_owner) SignatureChecker(_name, _version) {}

    /**
     * @dev deposit ETH to treasury and emit executable API data only if funds sent by account owner
     * @param _onBehalfOf account owner address
     * @param _extradata extra calldata to send
     */
    function depositETH(
        address _onBehalfOf,
        bytes calldata _extradata
    ) public payable {
        if (msg.value != 0)
            emit DepositToTreasury(_onBehalfOf, ETH_ADDRESS, msg.value);
        if (msg.sender == _onBehalfOf)
            emit EventFutureActions(_onBehalfOf, _extradata);
    }

    /**
     * @dev deposit ERC20 to treasury and emit executable API data only if sent by account owner
     * @param _onBehalfOf account owner address
     * @param _token erc20 token address that is deposited
     * @param _amount token amount that is deposited
     * @param _extradata extra calldata to send
     */
    function depositERC20(
        address _onBehalfOf,
        IERC20 _token,
        uint256 _amount,
        bytes calldata _extradata
    ) external payable {
        if (_amount != 0) {
            uint256 balanceBefore = _token.balanceOf(address(this));
            _token.safeTransferFrom(_onBehalfOf, address(this), _amount);
            uint tokenDeposited = _token.balanceOf(address(this)) -
                balanceBefore;
            if (tokenDeposited != 0)
                emit DepositToTreasury(
                    _onBehalfOf,
                    address(_token),
                    tokenDeposited
                );
        }

        if (msg.sender == _onBehalfOf)
            emit EventFutureActions(_onBehalfOf, _extradata);
    }

    /**
     * @dev Withdraw ETH from treasury
     * @param _to to address to withdraw funds to
     * @param _amount amount to withdraw
     * @param _nonce nonce of _to, starting from 1
     * @param _sig signature
     * @param _sigType signature type 0 for EIP712TYPE and 1 for EIP191TYPE and 2 for EIP1271TYPE
     */
    function withdrawFromTreasuryETH(
        address _to,
        uint256 _amount,
        uint256 _nonce,
        bytes memory _sig,
        Enum.signatureType _sigType
    ) external {
        if (_nonce != ++nonces[_to]) revert Errors.INVALID_NONCE();

        if (_sigType == Enum.signatureType.EIP191TYPE)
            _verifyEIP191(_to, _amount, _nonce, _sig, signer);
        else if (_sigType == Enum.signatureType.EIP712TYPE)
            _verifyEIP712(_to, _amount, _nonce, _sig, signer);
        else if (_sigType == Enum.signatureType.EIP1271TYPE)
            _verifyEIP1271(_to, _amount, _nonce, _sig, signer);

        (bool success, ) = _to.call{value: _amount}("");
        if (!success) revert Errors.TX_FAILED();
        emit WithdrawnFromTreasury(_to, ETH_ADDRESS, _amount, _nonce);
    }

    /**
     * @dev Withdraw ERC20 from treasury
     * @param _to to address to withdraw funds to
     * @param _amount amount to withdraw
     * @param _nonce nonce of _to, starting from 1
     * @param _token token address that is withdrawn
     * @param _sig signature
     * @param _sigType signature type 0 for EIP712TYPE and 1 for EIP191TYPE and 2 for EIP1271TYPE
     */
    function withdrawFromTreasuryERC20(
        address _to,
        uint256 _amount,
        uint256 _nonce,
        IERC20 _token,
        bytes memory _sig,
        Enum.signatureType _sigType
    ) external {
        if (_nonce != ++nonces[_to]) revert Errors.INVALID_NONCE();

        if (_sigType == Enum.signatureType.EIP191TYPE)
            _verifyEIP191(_to, _amount, _nonce, _sig, signer);
        else if (_sigType == Enum.signatureType.EIP712TYPE)
            _verifyEIP712(_to, _amount, _nonce, _sig, signer);
        else if (_sigType == Enum.signatureType.EIP1271TYPE)
            _verifyEIP1271(_to, _amount, _nonce, _sig, signer);

        bool success = _token.transfer(_to, _amount);
        if (!success) revert Errors.TX_FAILED();
        emit WithdrawnFromTreasury(_to, address(_token), _amount, _nonce);
    }

    /**
     * @dev set the signer address
     * @param _signer address of the signer
     */
    function setSigner(address _signer) external onlyOwner {
        signer = _signer;
        emit NewSignerSet(_signer);
    }

    /**
     * @dev withdraw funds from treasury by treasury owner
     * @param _token token address to withdraw
     * @param _amount amount to withdraw
     */
    function ownerWithdraw(address _token, uint256 _amount) external onlyOwner {
        if (_token == ETH_ADDRESS) {
            (bool success, ) = msg.sender.call{value: _amount}("");
            if (!success) revert Errors.TX_FAILED();
        } else {
            IERC20(_token).safeTransfer(msg.sender, _amount);
        }

        emit withdrawByOwner(msg.sender, _token, _amount);
    }

    /**
     * @dev deposit funds to treasury by treasury owner
     * @param _token token address to deposit
     * @param _amount amount to deposit
     */
    function ownerDeposit(
        address _token,
        uint256 _amount
    ) external payable onlyOwner {
        if (_token == ETH_ADDRESS) {
            emit DepositToTreasury(msg.sender, ETH_ADDRESS, msg.value);
        } else {
            IERC20(_token).safeTransferFrom(msg.sender, address(this), _amount);
            emit DepositToTreasury(msg.sender, _token, _amount);
        }
    }
}

