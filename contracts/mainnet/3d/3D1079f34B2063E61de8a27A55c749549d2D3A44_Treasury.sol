        0x8b73c3c69bb8fe3d512ecc4cf759cc79239f7b179b0ffacaa9a75d522b39400f;
    // keccak256('Transfer(address from,address to,uint256 amount,uint256 nonce)')
    bytes32 internal constant EIP712_SCHEMA_HASH =
        0x286ecc3171aaa74d0fc76977f77dba38ee9f4f740604e0b628c1fcd79b69c958;

    constructor(string memory _name, string memory _version) {
        HASH_NAME = keccak256(bytes(_name));
        HASH_VERSION = keccak256(bytes(_version));
    }

    /**
     * verify EIP191 signature
     * @param _to to address
     * @param _amount amount
     * @param _nonce nonce of to address passed from signer
     * @param _sig signature
     * @param _requiredSigner required signer
     */
    function _verifyEIP191(
        address _to,
        uint256 _amount,
        uint256 _nonce,
        bytes memory _sig,
        address _requiredSigner
    ) internal view {
        bytes32 txHash = keccak256(
            abi.encodePacked(address(this), _to, _amount, _nonce)
        );
        bytes32 _hash = MessageHashUtils.toEthSignedMessageHash(txHash);
        address recoveredSigner = ECDSA.recover(_hash, _sig);
        if (recoveredSigner != _requiredSigner)
            revert Errors.INVALID_SIGNATURE();
    }

    /**
     * verify EIP712 signature
     * @param _to to address
     * @param _amount amount
     * @param _nonce nonce of to address passed from signer
     * @param _sig signature
     * @param _requiredSigner required signer
     */
    function _verifyEIP712(
        address _to,
        uint256 _amount,
        uint256 _nonce,
        bytes memory _sig,
        address _requiredSigner
    ) internal view {
        bytes32 _hash = MessageHashUtils.toTypedDataHash(
            keccak256(
                abi.encode(
                    TYPE_HASH,
                    HASH_NAME,
                    HASH_VERSION,
                    bytes32(block.chainid),
                    address(this)
                )
            ),
            keccak256(
                abi.encode(
                    EIP712_SCHEMA_HASH,
                    address(this),
                    _to,
                    _amount,
                    _nonce
                )
            )
        );
        address recoveredSigner = ECDSA.recover(_hash, _sig);
        if (recoveredSigner != _requiredSigner)
            revert Errors.INVALID_SIGNATURE();
    }

    /**
     * verify EIP1271 signature
     * @param _to to address
     * @param _amount amount
     * @param _nonce nonce of to address passed from signer
     * @param _sig signature
     * @param _requiredSigner required signer
     */
    function _verifyEIP1271(
        address _to,
        uint256 _amount,
        uint256 _nonce,
        bytes memory _sig,
        address _requiredSigner
    ) internal view {
        bytes32 txHash = keccak256(
            abi.encodePacked(address(this), _to, _amount, _nonce, block.chainid)
        );

        (bool success, bytes memory result) = _requiredSigner.staticcall(
            abi.encodeCall(IERC1271.isValidSignature, (txHash, _sig))
        );

        if (!success) revert Errors.TX_FAILED();

        if (abi.decode(result, (bytes4)) != MAGIC_VALUE)
            revert Errors.INVALID_SIGNATURE();
    }
}

