pragma solidity 0.8.19;

import {Ownable} from "@openzeppelin/contracts/access/Ownable.sol";

import {IReduxWhitelist} from "redux/interface/IReduxWhitelist.sol";

contract ReduxWhitelist is IReduxWhitelist, Ownable {
    mapping(address => bool) public whitelist;

    function adjustWhitelist(address[] memory addressList) external onlyOwner {
        for (uint256 i = 0; i < addressList.length; ++i) {
            if (whitelist[addressList[i]]) {
                delete whitelist[addressList[i]];
            } else {
                whitelist[addressList[i]] = true;
            }
        }
    }

    function updateWhitelist(address[] memory addresses, bool[] memory entries) external onlyOwner {
        for (uint256 i = 0; i < addresses.length; i++) {
            if (entries[i]) {
                whitelist[addresses[i]] = true;
            } else {
                delete whitelist[addresses[i]];
            }
        }
    }
}

