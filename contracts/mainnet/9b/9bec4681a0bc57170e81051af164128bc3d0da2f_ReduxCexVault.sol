pragma solidity 0.8.19;

import {AccessControl} from "@openzeppelin/contracts/access/AccessControl.sol";
import {Ownable} from "@openzeppelin/contracts/access/Ownable.sol";
import {ERC20, SafeTransferLib} from "@solmate/mixins/ERC4626.sol";

import {ReduxWhitelist} from "redux/ReduxWhitelist.sol";

import {ConversionHelper} from "redux/library/ConversionHelper.sol";
import {QueueLib} from "redux/library/QueueLib.sol";

/**
 * @title ReduxCexVault
 * @author Resonate Finance [Thanh Trinh, Nikola Malic]
 * @notice A vault that allows users to deposit assets and redeem shares
 * This vault allows depositing into Redux AI trading strategy system which trades on CEXs.
 * The vault will mint shares to the depositors and burn shares from the redeemers.
 * The vault will transfer assets to the deposit address which will be used for trading operations.
 */
contract ReduxCexVault is ERC20, AccessControl, Ownable {
    using SafeTransferLib for ERC20;
    using ConversionHelper for uint256;
    using QueueLib for QueueLib.Queue;

    bool public whitelistMode;

    // Minimum amount of shares that can be redeemed from the vault
    uint256 public constant MIN_REDEEM_AMOUNT = 1e6;
    // Minimum amount of assets that can be deposited into the vault
    uint256 public constant MIN_DEPOSIT_AMOUNT = 10e6;
    // The latest ratio of assets to shares.
    // It is useful for tracking the historical strategy performance but doesn't provide actual ratio
    uint256 public latestRatio = 1;
    // The maximum amount of assets that can be deposited into the vault
    uint256 public depositCap = 100_000_000e18;
    // The total amount of assets that have been deposited into the vault
    // This is an approximation and may not be accurate due to trading factor
    uint256 public totalValueLocked;

    // Role responsible for processing deposits and redeems
    bytes32 public constant TRADER_ROLE = keccak256("TRADER_ROLE");

    // The address that the vault deposits assets to
    address public depositAddress;
    // The asset that the vault holds
    ERC20 public immutable asset;
    // The whitelist contract that checks if an address is whitelisted
    ReduxWhitelist public immutable reduxWhitelist;

    // The queue of all incomming deposit and redeem requests
    QueueLib.Queue public queue;

    event DepositRequested(address indexed owner, address indexed receiver, uint256 indexed assets);
    event RedeemRequested(address indexed owner, address indexed receiver, uint256 shares);

    event DepositRequestProcessed(address indexed owner, uint256 indexed assets);
    event RedeemRequestProcessed(address indexed receiver, uint256 indexed shares, uint256 indexed assets);

    event LatestRatioUpdated(uint256 indexed newRatio, uint256 indexed totalValueLocked);
    event DepositCapChanged(uint256 newDepositCap);
    event DepositAddressChanged(address newDepositAddress);
    event WhitelistModeChanged(bool enabled);
    event TraderRoleChanged(address newTraderAddress);
    event GovernanceControllerChanged(address newGovController);

    error DepositCapReached(uint256 depositCap);
    error NotEnoughAssets(uint256 assets, uint256 balance);
    error NotWhitelisted(address caller);

    /**
     * @notice Checks if the amount is greater than the minimum amount
     */
    modifier sufficientAmount(uint256 amount, uint256 minAmount) {
        require(amount >= minAmount, "Amount must be greater than minimum amount");
        _;
    }

    /**
     * @notice In case whitelist mode is enabled, only whitelisted addresses can interact with the vault
     */
    modifier onlyWhitelisted() {
        if (whitelistMode && !reduxWhitelist.whitelist(msg.sender)) revert NotWhitelisted(msg.sender);
        _;
    }

    constructor(ERC20 _asset, address _depositAddress, address _whitelist)
        ERC20("Redux_Vault", "REDUX", _asset.decimals())
        Ownable()
    {
        require(_depositAddress != address(0), "Invalid deposit address");
        require(_whitelist != address(0), "Invalid whitelist contract");

        asset = _asset;
        depositAddress = _depositAddress;
        reduxWhitelist = ReduxWhitelist(_whitelist);

        emit DepositCapChanged(depositCap);
    }

    /**
     * @notice Returns total number of assets required to redeem all requests
     * @dev This is the minimum amount that the TRADER must have to process all requests
     * @param totalAssets The total amount of assets that the protocol has
     */
    function requestedRedeemAssets(uint256 totalAssets) external view returns (uint256 requiredAssets) {
        uint256 totalShares = totalSupply;
        uint256 originalTotalAssets = totalAssets;
        for (uint256 i = 0; i < queue.length(); ++i) {
            QueueLib.QueueElement memory element = queue.at(i);
            if (element.redeem) {
                totalAssets -= _previewRedeem(element.amount, totalAssets, totalShares);
                totalShares -= element.amount;
            }
        }

        return originalTotalAssets - totalAssets;
    }

    /**
     * @notice Returns total number of assets which will be deployed after requests are processed
     */
    function requestedDepositAssets() external view returns (uint256 totalDepositedAssets) {
        for (uint256 i = 0; i < queue.length(); ++i) {
            QueueLib.QueueElement memory element = queue.at(i);
            if (!element.redeem) {
                totalDepositedAssets += element.amount;
            }
        }
    }

    /**
     * @notice Allows a user to deposit assets into the vault.
     *  The user will have to wait for the assets to be processed before they can receive their share.
     * @param assets The amount of assets to deposit
     * @param receiver The address that will receive the shares
     */
    function requestDeposit(uint256 assets, address receiver)
        external
        onlyWhitelisted
        sufficientAmount(assets, MIN_DEPOSIT_AMOUNT)
    {
        require(receiver != address(0), "Invalid receiver");

        if (assets + totalValueLocked > depositCap) {
            revert DepositCapReached(depositCap);
        }

        queue.pushBack(QueueLib.QueueElement(receiver, assets, false));
        asset.safeTransferFrom(msg.sender, address(this), assets);

        emit DepositRequested(msg.sender, receiver, assets);
    }

    /**
     * @notice Allows a user to redeem their shares for assets.
     *  The user will have to wait for the assets to be processed before they can receive their assets.
     * @param shares The amount of shares to redeem
     * @param receiver The address that will receive the assets
     */
    function requestRedeem(uint256 shares, address receiver) external sufficientAmount(shares, MIN_REDEEM_AMOUNT) {
        require(receiver != address(0), "Invalid receiver");

        queue.pushBack(QueueLib.QueueElement(receiver, shares, true));
        ERC20(this).safeTransferFrom(msg.sender, address(this), shares);

        emit RedeemRequested(msg.sender, receiver, shares);
    }

    /**
     * @notice Processes all the requests in the queue
     * @param totalAssets The total amount of assets that the vault has
     */
    function processRequests(uint256 totalAssets) external onlyRole(TRADER_ROLE) {
        uint256 queueLength = queue.length();
        require(queueLength > 0, "Request queue is empty");
        _processRequests(totalAssets, queueLength);
    }

    /**
     * @notice processRequests with additional parameter to process only a specific number of requests
     * @dev While not used primarily it is a mechanism to avoid high gas costs for single transactions
     * @param totalAssets The total amount of assets that the vault has
     * @param requestsToProcess The number of requests to process
     */
    function processRequests(uint256 totalAssets, uint256 requestsToProcess) external onlyRole(TRADER_ROLE) {
        uint256 queueLength = queue.length();
        require(queueLength >= requestsToProcess, "Not enough requests to process");
        _processRequests(totalAssets, requestsToProcess);
    }

    function _processRequests(uint256 totalAssets, uint256 requestsToProcess) internal {
        require(requestsToProcess > 0, "No requests to process");

        uint256 totalDepositedAssets = 0;
        // Used to track the share amount change during these operations to maintain correct ratio
        uint256 totalShares = totalSupply;

        while (queue.length() > 0 && requestsToProcess-- > 0) {
            QueueLib.QueueElement memory queueElement = queue.popFront();

            if (queueElement.redeem) {
                totalAssets -= _processRedeem(queueElement, totalAssets, totalShares);
                totalShares -= queueElement.amount;
            } else {
                (uint256 assets, uint256 shares) = _processDeposit(queueElement, totalAssets, totalShares);
                totalShares += shares;
                totalDepositedAssets += assets;
                totalAssets += assets;
            }
        }
        _postProcessRequests(totalDepositedAssets, totalShares);

        latestRatio = totalAssets.getRatio(totalSupply);
        totalValueLocked = totalAssets;

        emit LatestRatioUpdated(latestRatio, totalValueLocked);
    }

    /**
     * @notice This function is an optimization to reduce the number of transfers
     * Deposits will require assets to be transferred to the deposit address
     * Redeems will require shares to be burned from the vault
     * @param assets The amount of assets to transfer to the deposit address
     * @param shares The amount of shares to burn from the vault
     */
    function _postProcessRequests(uint256 assets, uint256 shares) internal {
        if (assets > 0) {
            asset.safeTransfer(depositAddress, assets);
        }

        if (totalSupply > shares) {
            _burn(address(this), totalSupply - shares);
        }
    }

    /**
     * @notice Allows the owner to set the deposit address
     * @param newDepositAddress The new deposit address
     */
    function setDepositAddress(address newDepositAddress) external onlyOwner {
        require(newDepositAddress != address(0), "Invalid deposit address");
        require(newDepositAddress != depositAddress, "Same deposit address");
        depositAddress = newDepositAddress;
        emit DepositAddressChanged(newDepositAddress);
    }

    /**
     * @notice Allows the owner to set whitelist mode
     */
    function setWhitelistMode(bool enabled) external onlyOwner {
        whitelistMode = enabled;
        emit WhitelistModeChanged(enabled);
    }

    /**
     * @notice Allows the owner to set the deposit cap
     * @param newDepositCap The new deposit cap
     */
    function setDepositCap(uint256 newDepositCap) external onlyOwner {
        require(newDepositCap > totalValueLocked, "New cap must be greater than total deposited amount");
        depositCap = newDepositCap;
        emit DepositCapChanged(newDepositCap);
    }

    /**
     * @notice Should be invoked on initalization to set the governance controller
     */
    function setGovernanceController(address newGovController) external onlyOwner {
        require(newGovController != address(0), "Invalid governance controller");
        _transferOwnership(newGovController);
        emit GovernanceControllerChanged(newGovController);
    }

    /**
     * @notice Allows the owner to extract any ERC20 token from the vault
     * @dev This is useful for extracting any tokens that were accidentally sent to the vault
     * @param receiver The address that will receive the token
     * @param token The token address
     * @param amount The amount of token to extract
     */
    function extractToken(address receiver, address token, uint256 amount) external onlyOwner {
        require(receiver != address(0), "Invalid receiver address");
        require(token != address(0), "Invalid token address");
        ERC20(token).safeTransfer(receiver, amount);
    }

    /**
     * @notice Allows the owner to set the trader role
     * param newTraderAddress The new trader address
     */
    function setTraderRole(address newTraderAddress) external onlyOwner {
        require(newTraderAddress != address(0), "Invalid trader address");
        _grantRole(TRADER_ROLE, newTraderAddress);
        emit TraderRoleChanged(newTraderAddress);
    }

    function _processDeposit(QueueLib.QueueElement memory element, uint256 totalAssets, uint256 totalShares)
        internal
        returns (uint256 assets, uint256 shares)
    {
        address requestOwner = element.recipient;
        assets = element.amount;

        uint256 rate = totalAssets.getRatio(totalShares);
        shares = assets.convertFromBase(rate);

        _mint(requestOwner, shares);

        emit DepositRequestProcessed(requestOwner, assets);
    }

    function _processRedeem(QueueLib.QueueElement memory element, uint256 totalAssets, uint256 totalShares)
        internal
        returns (uint256 redeemedAssets)
    {
        address redeemReceiver = element.recipient;
        uint256 requestRedeemShare = element.amount;
        uint256 rate = totalAssets.getRatio(totalShares);
        redeemedAssets = requestRedeemShare.convertToBase(rate);

        asset.safeTransferFrom(msg.sender, redeemReceiver, redeemedAssets);

        emit RedeemRequestProcessed(redeemReceiver, redeemedAssets, requestRedeemShare);
    }

    function _previewRedeem(uint256 shares, uint256 totalAssets, uint256 totalShares) private pure returns (uint256) {
        uint256 rate = totalAssets.getRatio(totalShares);
        return shares.convertToBase(rate);
    }

    receive() external payable {
        revert("Cannot receive ETH");
    }

    fallback() external payable {
        revert("Cannot receive ETH");
    }
}

