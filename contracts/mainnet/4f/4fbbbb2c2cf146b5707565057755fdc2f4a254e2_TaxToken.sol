pragma solidity ^0.8.26;

import {ERC20} from "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import {Ownable} from "@openzeppelin/contracts/access/Ownable.sol";
import {console} from "forge-std/console.sol";

/**
    Considerations:
        - 
        - Who will the tax go to, a hard-coded address or a specific tax collector?
            - Tax Collector (we will set this with a setter function w/ onlyOwner: modifier)
        - How does Buy tax work?
            - When user buys tokens from the DEX - LP takes token X and provides TaxToken to the user.
              The tax will be applied to the amount of tokens the user receives.
        - How does Sell tax work?
            - When the user sells tokens to the DEX - LP takes TaxToken and provides token X to the user.
              The tax is applied to the amount of tokens the user sells. 
        - How do we ensure transferring tokens to LP is untaxed
            - We define mapping(address => bool) and ensure the LP contract address is exempt.
        - How do I get my token on to a DEX?
                - Requires creating a Uniswap Liquidity Pool and then providing the token pairs to that pool.
        - How will a DEX interact with my token?
        - Can I use any DEX or would it have to be a specific DEX with support for Tax Tokens?
 */

contract TaxToken is ERC20, Ownable {
    // Initialise these
    uint8 buyTax;
    uint8 sellTax;

    address public taxCollector;
    address immutable TREASURY;

    uint256 constant INITIAL_SUPPLY = 1e14;

    string constant NAME = "Tax";
    string constant SYMBOL = "TX";

    // We would want the DEX's LP to be exempt from any fees and also our treasury address.
    mapping(address => bool) public isExcludedFromFee;
    mapping(address => bool) public knownLps;

    constructor(address treasury) ERC20(NAME, SYMBOL) Ownable(msg.sender) {
        TREASURY = treasury;
        _mint(TREASURY, INITIAL_SUPPLY);
    }

    // Only owner can set these functions (add modifier).
    function setSellTax(uint8 tax) public onlyOwner {
        sellTax = tax;
    }

    function setBuyTax(uint8 tax) public onlyOwner {
        buyTax = tax;
    }

    function setTaxCollector(address _taxCollector) external onlyOwner {
        taxCollector = _taxCollector;
    }

    function addExemptAddress(address addr) external onlyOwner {
        isExcludedFromFee[addr] = true;
    }

    function removeExemptAddress(address addr) external onlyOwner {
        delete isExcludedFromFee[addr];
    }

    function addKnownLp(address addr) external onlyOwner {
        knownLps[addr] = true;
    }

    function removeKnownLp(address addr) external onlyOwner {
        delete knownLps[addr];
    }

    function getInitialSupply() public pure returns (uint256) {
        return INITIAL_SUPPLY;
    }

    function decimals() public pure override returns (uint8) {
        return 6;
    }

    function _update(
        address from,
        address to,
        uint256 value
    ) internal override {
        // Buy Txn: When tokens are transferred from the LP to a user.
        // Sell Txn: When tokens are transferred from the user to the LP.
        bool isSenderExcluded = isExcludedFromFee[from];
        bool isReceiverExcluded = isExcludedFromFee[to];

        if (isSenderExcluded && isReceiverExcluded) {
            // Perform normal transfer if both sender and receiver are exempt from fee.
            super._update(from, to, value);
        } else {
            if (knownLps[from]) {
                // Buy Txn
                uint256 tax = (value / 100) * buyTax;
                super._update(from, to, value - tax);
                super._update(from, taxCollector, tax);
            } else if (knownLps[to]) {
                uint256 tax = (value / 100) * sellTax;
                super._update(from, to, value - tax);
                super._update(from, taxCollector, tax);
            } else {
                super._update(from, to, value);
            }
        }
    }
}

