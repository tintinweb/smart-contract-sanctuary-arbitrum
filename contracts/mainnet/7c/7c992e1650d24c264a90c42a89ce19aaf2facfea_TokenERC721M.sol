        address receiver = _req.to;

        tokenIdMinted = _mintTo(receiver, _req.uri);

        if (_req.royaltyRecipient != address(0)) {
            royaltyInfoForToken[tokenIdMinted] = RoyaltyInfo({
                recipient: _req.royaltyRecipient,
                bps: _req.royaltyBps
