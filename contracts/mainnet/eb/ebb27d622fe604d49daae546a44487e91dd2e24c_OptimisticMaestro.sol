        int256 cashflow,
        Currency cashflowCcy,
        uint256 deadline,
        OrderType orderType,
        address placedBy
    );

    event OrderCancelled(OrderId indexed orderId);
    event OrderExecuted(OrderId indexed orderId, PositionId indexed positionId, uint256 keeperReward);
