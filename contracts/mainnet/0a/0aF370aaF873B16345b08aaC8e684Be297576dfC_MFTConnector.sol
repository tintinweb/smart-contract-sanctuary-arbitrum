pragma solidity ^0.8.0;

import './ERC-1155/Messina1155NonUpgradable.sol';
import './interfaces/IMFT.sol';
import 'hardhat/console.sol';

contract MFTConnector is Messina1155 {
  uint256 public MFT_ID;
  address public MFTAddress;

  constructor(
    uint256 MFT_ID_,
    address MFTAddress_,
    address owner_,
    address nftBridge_,
    uint16 chainId_,
    bytes32 nativeContract_,
    address royaltyReceiver_,
    uint96 royaltyFeesInBips_,
    uint16 standardID_,
    bytes memory data_
  ) Messina1155(
      '',
      owner_,
      nftBridge_,
      chainId_,
      nativeContract_,
      royaltyReceiver_,
      royaltyFeesInBips_,
      standardID_,
      data_
    ) 
  {
    require(MFTAddress_ != address(0), 'MFTAddress cannot be 0');
    require(MFT_ID_ != 0, 'MFT_ID cannot be 0');
    MFT_ID = MFT_ID_;
    MFTAddress = MFTAddress_;
  }

  function setMFTAddress(address MFTAddress_) external onlyOwner {
    require(MFTAddress_ != address(0), 'MFTAddress cannot be 0');
    MFTAddress = MFTAddress_;
  }

  function setMFTID(uint256 MFT_ID_) external onlyOwner {
    require(MFT_ID_ != 0, 'MFT_ID cannot be 0');
    MFT_ID = MFT_ID_;
  }

  function _mint(address to, uint256, uint256 amount, bytes memory data) internal override {
    IMFT(MFTAddress).connectorMint(to, MFT_ID, amount, data);
  }

  function _mintBatch(
    address to,
    uint256[] memory ids,
    uint256[] memory amounts,
    bytes memory data
  ) internal override {
    uint256[] memory MFT_IDs = new uint256[](ids.length);
    for (uint256 i = 0; i < ids.length; i++) {
      MFT_IDs[i] = MFT_ID;
    }
    IMFT(MFTAddress).connectorMintBatch(to, MFT_IDs, amounts, data);
  }
}

