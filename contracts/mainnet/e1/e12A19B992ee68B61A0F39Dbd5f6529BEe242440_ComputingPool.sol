pragma solidity ^0.8.19;

import '@openzeppelin/contracts/token/ERC20/ERC20.sol';
import '@openzeppelin/contracts/utils/math/SafeMath.sol';
import '@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol';
import './lib/Constants.sol';
import './lib/TransferHelper.sol';
import './lib/FullMath.sol';
import './interface/IWhitelist.sol';
import './interface/INark.sol';

contract ComputingPool is OwnableUpgradeable {
    using SafeMath for uint256;

    struct userComputing {
        uint256 amount;
        uint256 lastWithdraw;
    }

    struct Comp {
        uint256 lpAmount;
        uint256 receivedLpAmount;
        uint256 totalComp;
    }

    address public operator;
    address public narkAddress;
    address public whitelistAddress;
    address public lpAddress;
    address public valuationCoin;

    uint8 public lpDecimal;

    uint256 public computingPool;
    uint256 public whitelistComp;
    uint256 public compTimestamp;

    mapping(address => userComputing) public userComputingPow;
    mapping(address => bool) public isUserSync;
    mapping(uint256 => Comp) public dailyLp;

    mapping(address => mapping(uint256 => uint256)) public userDailyComp;
    event Commission(
        address triggerAddress,
        address awardAddress,
        address coinAddress,
        uint256 coinAmount,
        uint8 coinDecimal,
        uint8 fromType
    );
    event PowerChange(address triggerAddress, uint256 changeAmount, uint256 nowAmount, uint8 compType);

    modifier onlyNarkOrOperate() {
        require(msg.sender == narkAddress || msg.sender == operator || msg.sender == owner(), 'only Nark or operate');
        _;
    }

    modifier onlyOperator() {
        require(msg.sender == operator, 'Only operator');
        _;
    }

    function initialize(address _lpAddress) external initializer {
        __Ownable_init();
        lpAddress = _lpAddress;
        lpDecimal = ERC20(_lpAddress).decimals();
        operator = msg.sender;
    }

    modifier onlyOperatorOrOwner() {
        require(msg.sender == operator || msg.sender == owner(), 'Only operator or owner');
        _;
    }

    function setWhitelistComp(uint256 power) external onlyOperatorOrOwner {
        whitelistComp = power;
    }

    function setNarkAddress(address _narkAddress) external onlyOperatorOrOwner {
        narkAddress = _narkAddress;
    }

    function setWhitelistAddress(address _whitelistAddress) external onlyOperatorOrOwner {
        whitelistAddress = _whitelistAddress;
    }

    function depositComp(address userAddr, uint256 power) external onlyNarkOrOperate {
        uint256 curDate = _getCurrentDate();
        userComputing storage user = userComputingPow[userAddr];
        user.amount = user.amount.add(power);
        if(user.lastWithdraw == 0){
            user.lastWithdraw = curDate;
        }
        userDailyComp[userAddr][curDate] = user.amount;
        if (compTimestamp == 0) {
            compTimestamp = curDate;
        }
        if (compTimestamp != curDate) {
            for (uint i = compTimestamp; i < curDate; i++) {
                if (computingPool == 0) {
                    break;
                }
                if (dailyLp[i].totalComp == 0) {
                    dailyLp[i].totalComp = computingPool;
                }
            }
            compTimestamp = curDate;
        }
        computingPool = computingPool.add(power);
        emit PowerChange(userAddr, power, user.amount, Constants.COMP_UPGRADE);
    }

    function recordsDailyLp(uint256 amount) external onlyNarkOrOperate {
        TransferHelper.safeTransferFrom(lpAddress, msg.sender, address(this), amount);
        uint256 curDate = _getCurrentDate();
        Comp storage comp = dailyLp[curDate];
        comp.lpAmount = comp.lpAmount.add(amount);
    }

    function syncWhitelist(address[] calldata whitelist) external onlyOwner {
        require(whitelistAddress != address(0) && whitelistComp != 0, 'invaild node addr');
 
        for (uint i = 0; i < whitelist.length; i++) {
            bool flag = IWhitelist(whitelistAddress).whiteAlready(whitelist[i]);
            if (isUserSync[whitelist[i]] || !flag) {
                continue;
            }
            userComputing storage user = userComputingPow[whitelist[i]];
            user.amount = user.amount.add(whitelistComp);
            if(user.lastWithdraw == 0){
            user.lastWithdraw = _getCurrentDate();
            }
            userDailyComp[whitelist[i]][_getCurrentDate()] = user.amount;
            computingPool = computingPool.add(whitelistComp);
            isUserSync[whitelist[i]] = true;
            emit PowerChange(whitelist[i], whitelistComp, user.amount, Constants.COMP_UPGRADE);
        }
    }

    function withdraw() external {
        userComputing storage user = userComputingPow[msg.sender];
        uint256 date = _getCurrentDate();

        require(user.lastWithdraw < date, 'deposit power or already withdraw');
        require(user.amount > 0, 'must be have comp');

        uint needReceived;
        uint removeComp;
        for (uint i = user.lastWithdraw; i < date; i++) {
            uint256 tomorrow = i.add(Constants.ONE);
            Comp storage comp = dailyLp[i];
            if (comp.totalComp == 0) {
                comp.totalComp = computingPool;
            }
            uint256 userCurComp = userDailyComp[msg.sender][i];
            if (comp.lpAmount == 0 || comp.totalComp == 0) {
                if (userDailyComp[msg.sender][tomorrow] == 0) {
                    userDailyComp[msg.sender][tomorrow] = userCurComp;
                }
                continue;
            }
            if(userCurComp == 0){
               continue;
            }
            uint256 shouldAmount = FullMath.mulDiv(userCurComp, comp.lpAmount, comp.totalComp);
            uint256 needComp;
            if (comp.receivedLpAmount.add(shouldAmount) <= comp.lpAmount) {
                (, , needComp) = INark(narkAddress).computedLPToU(shouldAmount);
            } else {
                uint256 remainingLP = comp.lpAmount.sub(comp.receivedLpAmount);
                (, , needComp) = INark(narkAddress).computedLPToU(remainingLP);
                shouldAmount = remainingLP;
            }

            if (userCurComp >= removeComp.add(needComp)) {
                needReceived = needReceived.add(shouldAmount);
                comp.receivedLpAmount = comp.receivedLpAmount.add(shouldAmount);
            } else {
                uint remainingComp = userCurComp.sub(removeComp);
                uint singleDepositToLiq = FullMath.mulDiv(shouldAmount, Constants.KEEP_VAULE, needComp);
                uint actualAmount = remainingComp.mul(singleDepositToLiq);
                needReceived = needReceived.add(actualAmount);
                comp.receivedLpAmount = comp.receivedLpAmount.add(actualAmount);                             
                needComp = remainingComp;
                userCurComp = 0;
            }
            removeComp = removeComp.add(needComp);   
            
            if (userDailyComp[msg.sender][tomorrow] == 0) {
                userDailyComp[msg.sender][tomorrow] = userCurComp;
            }
        }
        user.amount = user.amount.sub(removeComp);
        computingPool = computingPool.sub(removeComp);
        userDailyComp[msg.sender][date] = user.amount;

        user.lastWithdraw = date;
        compTimestamp = date;
        TransferHelper.safeTransfer(lpAddress, msg.sender, needReceived);
        emit PowerChange(msg.sender, removeComp, user.amount, Constants.COMP_AWARD);
        emit Commission(msg.sender, msg.sender, lpAddress, needReceived, lpDecimal, Constants.COMMISSION_COM_POOL);
    }

    function computedAward() external view returns (uint256) {
        userComputing memory user = userComputingPow[msg.sender];

        uint256 date = _getCurrentDate();
        uint needReceived;
        uint tomorrowComp;
        uint removeComp;
        if (user.lastWithdraw >= date || user.amount <= 0) {
            return needReceived;
        }
        for (uint i = user.lastWithdraw; i < date; i++) {
            uint256 tomorrow = i.add(Constants.ONE);
            Comp memory comp = dailyLp[i];
            if (comp.totalComp == 0) {
                comp.totalComp = computingPool;
            }
            uint256 userCurComp = userDailyComp[msg.sender][i];
            if (comp.lpAmount == 0 || comp.totalComp == 0) {
                if (tomorrowComp == 0) {
                   tomorrowComp = userCurComp;
                } 
                continue;
            }
            if (userCurComp == 0) {
                userCurComp = tomorrowComp;
                if(userCurComp == 0){
                    continue;
                }
            }
            uint256 shouldAmount = FullMath.mulDiv(userCurComp, comp.lpAmount, comp.totalComp);
            uint256 needComp;
            if (comp.receivedLpAmount.add(shouldAmount) <= comp.lpAmount) {
                (, , needComp) = INark(narkAddress).computedLPToU(shouldAmount);
            } else {
                uint256 remainingLP = comp.lpAmount.sub(comp.receivedLpAmount);
                (, , needComp) = INark(narkAddress).computedLPToU(remainingLP);
                shouldAmount = remainingLP;
            }

            if (userCurComp >= removeComp.add(needComp)) {
                needReceived = needReceived.add(shouldAmount);
            } else {
                uint remainingComp = userCurComp.sub(removeComp);
                uint singleDepositToLiq = FullMath.mulDiv(shouldAmount, Constants.KEEP_VAULE, needComp);
                uint actualAmount = remainingComp.mul(singleDepositToLiq);
                needReceived = needReceived.add(actualAmount);
                needComp = remainingComp;
                userCurComp = 0;
                tomorrowComp = 0;
            }
            removeComp = removeComp.add(needComp);
            if (userDailyComp[msg.sender][tomorrow] == 0) {
                tomorrowComp = userCurComp;
            }
        }
        return needReceived;
    }

    function _getCurrentDate() internal view returns (uint256) {
        return block.timestamp / 1 days;
    }

}

