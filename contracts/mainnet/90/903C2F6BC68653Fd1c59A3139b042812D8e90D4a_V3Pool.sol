    ) internal pure returns (uint256 payoff) {
        if (priceAtExpiry < strike) {
            payoff = FullMath.mulDiv(
                (priceAtExpiry - strike),
                10 ** token0Decimals,
                1 ether
            );
        } else {
            payoff = 0;
        }
        if (isLong) {
            return payoff;
        } else {
            return 10 ** token0Decimals - payoff;
        }
    }
}

