pragma solidity ^0.8.17;

import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "./IAnimaChamber.sol";
import "./IAnimaChamberTransferHandler.sol";
import "../ERC721/AbstractERC721Metadata.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "../Manager/ManagerModifier.sol";

contract AnimaChamber is
  IAnimaChamber,
  ERC721Enumerable,
  ReentrancyGuard,
  ManagerModifier
{
  AbstractERC721Metadata public METADATA;
  IAnimaChamberTransferHandler public TRANSFER_HANDLER;

  uint256 public LAST_TOKEN_ID;

  constructor(
    address _manager,
    address _metadata,
    address _transferHandler
  ) ManagerModifier(_manager) ERC721("Anima Chamber", "ACBR") {
    METADATA = AbstractERC721Metadata(_metadata);
    TRANSFER_HANDLER = IAnimaChamberTransferHandler(_transferHandler);
  }

  function tokensOfOwner(
    address _owner
  ) external view returns (uint256[] memory) {
    uint256 numberOwned = ERC721.balanceOf(_owner);
    uint256[] memory _tokens = new uint256[](numberOwned);
    for (uint256 i = 0; i < numberOwned; i++) {
      _tokens[i] = tokenOfOwnerByIndex(_owner, i);
    }
    return _tokens;
  }

  function burn(uint256 _tokenId) external onlyMinter nonReentrant {
    _burn(_tokenId);
  }

  function mintFor(
    address _for
  ) external onlyMinter nonReentrant returns (uint256 tokenId) {
    // Mint
    tokenId = ++LAST_TOKEN_ID;
    _safeMint(_for, tokenId);
  }

  function tokenURI(
    uint256 _tokenId
  ) public view override returns (string memory) {
    return METADATA.uri(address(this), _tokenId);
  }

  //=======================================
  // Internal
  //=======================================
  function _beforeTokenTransfer(
    address _from,
    address _to,
    uint256 _startTokenId,
    uint256 _quantity
  ) internal override {
    TRANSFER_HANDLER.handleBeforeTransfer(
      msg.sender,
      _from,
      _to,
      _startTokenId,
      _quantity
    );
    super._beforeTokenTransfer(_from, _to, _startTokenId, _quantity);
  }

  //=======================================
  // Admin
  //=======================================

  function updateTransferHandler(address _addr) external onlyAdmin {
    TRANSFER_HANDLER = IAnimaChamberTransferHandler(_addr);
  }

  function updateMetadata(address _addr) external onlyAdmin {
    METADATA = AbstractERC721Metadata(_addr);
  }
}

