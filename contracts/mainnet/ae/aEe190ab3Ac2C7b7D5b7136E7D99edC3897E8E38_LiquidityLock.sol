        address deployer,
        uint256 supply
    ) ERC20("SpatialComputing", "CMPT", 18) Owned(deployer) {
        _mint(deployer, supply);
    }

    function renounceOwnership() external onlyOwner {
        transferOwnership(address(0));
    }

