// File: ComputeArb.sol


pragma solidity ^0.8.13;










contract ComputeArb is Owned {
    INonfungiblePositionManager public uniswapV3PositionManager;
    IUniswapV3Pool public pool;
    SpatialComputing public spatialComputing;
    LiquidityLock public lock;

    // Arbitrum 
    address public weth = 0x82aF49447D8a07e3bd95BD0d56f35241523fBab1;
    // Arbitrum
    address public positionManager = 0xC36442b4a4522E871399CD717aBDD847Ab11FE88;
    // Arbitrum
    address public factoryAddress = 0x1F98431c8aD98523631AE4a59f267346ea31F984;
    address public clearingAddress;

    uint256 public validityChecks = 6;
    uint256 public validityCap = 1 ether;
    uint256 public epochLength = 8 hours;
    uint256 public lastCompute;
    uint16 private mevFee = 1000; // default fee 0.1% for low cost network

    uint24 private poolFee = 10_000;
    uint256 private tokenSupply = 1_000_000e18;
    uint256 private benchmarkSupply = tokenSupply / 100;
    uint256 private lastBenchmark;
    address private deployer;
    address private token0;
    bool public canCompute = false;

    /// @dev The minimum value that can be returned from #getSqrtRatioAtTick. Equivalent to getSqrtRatioAtTick(MIN_TICK)
    uint160 internal constant MIN_SQRT_RATIO = 4295128739;
    /// @dev The maximum value that can be returned from #getSqrtRatioAtTick. Equivalent to getSqrtRatioAtTick(MAX_TICK)
    uint160 internal constant MAX_SQRT_RATIO =
        1461446703485210103287273052203988822378723970342;

    constructor() Owned(msg.sender) {
        deployer = msg.sender;
        spatialComputing = SpatialComputing(0x1883aF6b51fb7F69577880004a82c9d68582022C);
        lock = new LiquidityLock(msg.sender);
        clearingAddress = msg.sender;

        ERC20(spatialComputing).approve(
            address(positionManager),
            type(uint256).max
        );
        WETH(payable(weth)).approve(
            address(positionManager),
            type(uint256).max
        );
    }

    receive() external payable {
        WETH(payable(weth)).deposit{value: msg.value}();
    }

    function setCanCompute() external onlyOwner {
        canCompute = true;
    }

    function claimAndCompute() external {
        require(canCompute, "Computing not yet enabled");
        require(
            block.timestamp > lastCompute + epochLength,
            "epoch in progress, try again later"
        );
        lastCompute = block.timestamp;
        // prevent re-entrancy
        claimFees();
        runCompute();
    }

    function claimFees() internal {
        uint256 wethBalance = WETH(payable(weth)).balanceOf(address(this));
        lock.claimFees();
        uint256 wethBalanceAfter = WETH(payable(weth)).balanceOf(address(this));
        uint256 wethGain = (wethBalanceAfter - wethBalance) / mevFee;
        // help pay the validity checks gas bill and compute
        if (wethGain > 0) {
            WETH(payable(weth)).transfer(msg.sender, wethGain);
        }
    }

    function adjustDeployerAddress(
        address _newDeployerAddress
    ) external onlyOwner {
        require(_newDeployerAddress != address(0), "Cannot be zero address");
        deployer = _newDeployerAddress;
    }

    function adjustClearingAddress(
        address _newClearingAddress
    ) external onlyOwner {
        require(_newClearingAddress != address(0), "Cannot be zero address");
        clearingAddress = _newClearingAddress;
    }

    function adjustEpoch(uint256 _newEpochDuration) external onlyOwner {
        require(
            _newEpochDuration >= 3600 && _newEpochDuration <= 28800,
            "new epoch duration is invalid"
        );
        epochLength = _newEpochDuration;
    }

    function adjustMevFeePercentage(uint16 _newMevFee) external onlyOwner {
        require(_newMevFee >= 10, "maximum Mev Fee is 10%");
        require(_newMevFee != 0, "value cannot be zero");
        mevFee = _newMevFee;
    }

    function setValidityChecks(uint256 _newCheckCount) external onlyOwner {
        validityChecks = _newCheckCount;
    }

    function ownerClaimFees() external onlyOwner {
        claimFees();
    }

    function ownerRunCompute() external onlyOwner {
        runCompute();
    }

    function runCompute() internal {
        if (lock.nftId() == 0) {
            return;
        } else if (address(pool) == address(0)) {
            IUniswapV3Factory factory = IUniswapV3Factory(factoryAddress);
            pool = IUniswapV3Pool(
                factory.getPool(weth, address(spatialComputing), poolFee)
            );
            ERC20(spatialComputing).approve(address(pool), type(uint256).max);
            WETH(payable(weth)).approve(address(pool), type(uint256).max);
            require(address(pool) != address(0));
            token0 = pool.token0();
        }

        uint256 spatialComputingBalance = spatialComputing.balanceOf(
            address(this)
        );

        if (spatialComputingBalance > 0) {
            spatialComputing.transfer(address(0), spatialComputingBalance);
        }

        uint256 wethBalance = WETH(payable(weth)).balanceOf(address(this));

        address tokenIn;
        address tokenOut;
        uint256 swapBalance;
        for (uint256 i = 0; i < validityChecks; i++) {
            tokenIn = i % 2 == 0 ? weth : address(spatialComputing);
            tokenOut = i % 2 == 0 ? address(spatialComputing) : weth;
            swapBalance = i % 2 == 0 ? wethBalance : spatialComputingBalance;
            if (swapBalance > 0) {
                uint256 toSwap = ((swapBalance * (i + 1)) * 10) /
                    ((i + 2) * 10);
                swap(tokenIn, tokenOut, toSwap);
            }
            if (i % 2 == 0) {
                wethBalance = WETH(payable(weth)).balanceOf(address(this));
            } else {
                spatialComputingBalance = spatialComputing.balanceOf(
                    address(this)
                );
            }
        }
        spatialComputingBalance = spatialComputing.balanceOf(address(this));
        swap(address(spatialComputing), weth, spatialComputingBalance);

        uint256 burnedSupply = spatialComputing.balanceOf(address(0));
        uint256 benchmarkLevel = burnedSupply / benchmarkSupply;
        if (benchmarkLevel == 5 && lastBenchmark != 5) {
            validityCap += 2 ether;
        }
        if (benchmarkLevel == 10 && lastBenchmark != 10) {
            validityCap += 2 ether;
        }
        if (benchmarkLevel == 15 && lastBenchmark != 15) {
            validityCap += 2 ether;
        }
        if (benchmarkLevel == 30 && lastBenchmark != 30) {
            validityCap += 2 ether;
        }
        if (benchmarkLevel == 50 && lastBenchmark != 50) {
            validityCap += 2 ether;
        }

        wethBalance = WETH(payable(weth)).balanceOf(address(this));
        if (wethBalance > validityCap) {
            uint256 toClear = wethBalance - validityCap;
            WETH(payable(weth)).transfer(clearingAddress, toClear);
        }

        lastBenchmark = benchmarkLevel;
    }

    function swap(address tokeknIn, address tokenOut, uint256 amount) internal {
        if (amount == 0) {
            return;
        }

        bool zeroForOne = tokeknIn < tokenOut;
        if ((address(spatialComputing) > weth)) {
            zeroForOne = !zeroForOne;
        }

        uint160 sqrt = zeroForOne ? MIN_SQRT_RATIO + 1 : MAX_SQRT_RATIO - 1;
        try
            pool.swap(
                address(this),
                zeroForOne,
                int256(amount),
                sqrt,
                zeroForOne ? bytes("1") : bytes("")
            )
        {} catch {}
    }

    function uniswapV3SwapCallback(
        int amount0,
        int amount1,
        bytes calldata data
    ) external {
        require(
            msg.sender == address(pool) && tx.origin != address(this),
            "not authorized"
        );
        bool zeroForOne = data.length > 0;

        address tokenOut = zeroForOne ? token0 : token0 == weth
            ? address(spatialComputing)
            : weth;

        uint amountOut;
        if (zeroForOne) {
            amountOut = uint(amount0);
        } else {
            amountOut = uint(amount1);
        }

        ERC20(tokenOut).transfer(address(pool), amountOut);
    }
}