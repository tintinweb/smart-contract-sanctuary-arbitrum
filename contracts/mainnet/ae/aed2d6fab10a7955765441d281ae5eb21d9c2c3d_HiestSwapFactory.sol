pragma solidity ^0.8.20;

import "../interface/IUniswapV2Router01.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@solmate/src/utils/ReentrancyGuard.sol";

    struct HiestMember{
    address parent;
    uint256 amt;
}


contract HiestSwap is ReentrancyGuard{
    using SafeERC20 for IERC20;

    IUniswapV2Router01 public uniRouter;

    IERC20 public HiestTokens;

    IERC20 public ProfToken;

    uint256[] public rate;

    uint256 public depth;

    mapping (address => HiestMember ) public HiestFamily;

    uint256 public net;

    constructor( address _uniRouter , address _ProfToken , address _HiestTokens , uint256[] memory _rate ){
        HiestTokens = IERC20(_HiestTokens);
        ProfToken = IERC20(_ProfToken);
        uniRouter = IUniswapV2Router01(_uniRouter);
        HiestFamily[address(this)].parent = address(this);
        HiestFamily[address(this)].amt = 0;
        rate = _rate;
        depth = rate.length;
    }

    function iSwap (
        uint amountOut,
        uint amountIn,
        address[] calldata path,
        address to,
        uint deadline
    ) internal returns (uint[] memory amounts){
        IERC20(path[0]).safeTransferFrom(msg.sender , address(this) , amountIn );
        IERC20(path[0]).safeIncreaseAllowance( address(uniRouter) , amountIn );
        amounts = uniRouter.swapTokensForExactTokens(amountOut , amountIn , path , to , deadline);
        if ( amounts[1] == IERC20(path[1]).balanceOf(msg.sender)) {
            IERC20(path[1]).safeTransfer( msg.sender , amounts[1] );
        }else{
            IERC20(path[1]).safeTransfer( msg.sender , IERC20(path[1]).balanceOf(msg.sender)) ;
        }
        return amounts;
    }

    function iSendLoot( address _parent , uint256 _amt , uint256 _rank  ) internal {
        uint256 loot = _amt * rate[_rank];
        if ( ProfToken.balanceOf(address(this)) >= loot ){
            ProfToken.safeTransfer(msg.sender , loot);
            HiestFamily[_parent].amt = HiestFamily[_parent].amt + loot;
        }
    }

    function Swap(
        address referer,
        uint amountOut,
        uint amountIn,
        address[] calldata path,
        address to,
        uint deadline
    ) external nonReentrant returns(uint[] memory amounts) {
        address parent = referer;
        require( path.length == 2 );
        if ( path[1] == address(HiestTokens)){
            uint[] memory _amounts = iSwap(amountOut , amountIn , path , to , deadline);
            for ( uint256 i ; i < depth ; i++ ){
                if( HiestFamily[parent].parent != address(0)){
                    iSendLoot(parent , _amounts[1], i);
                    net = net + _amounts[1];
                    parent = HiestFamily[parent].parent;
                }
            }

            return _amounts;
        }else{
            return iSwap(amountOut , amountIn , path , to , deadline);

        }
    }

}

