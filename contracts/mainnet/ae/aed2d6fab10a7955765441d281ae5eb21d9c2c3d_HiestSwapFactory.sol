pragma solidity ^0.8.20;

import "@openzeppelin/contracts/utils/math/SafeCast.sol";
import "@openzeppelin/contracts/utils/Address.sol";

import "./HiestSwap.sol";

contract HiestSwapFactory {
    using SafeCast for *;
    using Address for address;

    uint256 private ctr;

    mapping(uint256 => address) private SwapList;

    event createSwapEvent( address indexed nft);

    constructor() {
        ctr = 0;
    }

    //Factory Fx
    function createFarm( address _uniRouter , address _ProfToken , address _HiestTokens , uint256[] memory _rate ) external {
        ctr = ctr + 1;
        HiestSwap HS = new HiestSwap( _uniRouter , _ProfToken , _HiestTokens , _rate );
        SwapList[ctr] = address(HS);
        emit createSwapEvent(address(HS));
    }

    //Factory Fx

    function getSwap(uint256 _ctr) external view returns (address) {
        return SwapList[_ctr];
    }

    function getCtr() external view returns (uint256) {
        return ctr;
    }
}

