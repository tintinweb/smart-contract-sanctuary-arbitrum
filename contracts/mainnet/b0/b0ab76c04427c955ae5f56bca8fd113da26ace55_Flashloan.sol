      vars.success = IWETH(WETH).transfer(fld.wallet, vars.amountReceived - vars.amountOwed);
      require(vars.success, "Failed to transfer ETH token profit to my Crypto.com wallet");
      console.log("Successfully transferred", vars.amountReceived - vars.amountOwed, "ETH token profit to my Crypto.com wallet -", vars.success);
    } else {
      //vars.success = vars.outputToken.transfer(fld.wallet, vars.amountReceived - vars.amountOwed);
      vars.success = vars.inputToken.transfer(fld.wallet, vars.amountReceived - vars.amountOwed);
      require(vars.success, "Failed to transfer OTHER token profit to my Crypto.com wallet");
      console.log("Successfully transferred", vars.amountReceived - vars.amountOwed, "OTHER token profit to my Crypto.com wallet -", vars.success);
    }

    emit LogHeader("");
    emit LogHeader("SMART CONTRACT BALANCE (swap)");
    emit LogValue("bal                        : ", vars.amountReceived);
    emit LogValue("repay                      : ", vars.amountOwed);
    emit LogValue("bal - repay                : ", vars.amountReceived - vars.amountOwed);
    emit LogHeader("");

    console.log("WALLET BALANCE (AFTER)");
    console.log("osp.fromTokenAddress       ", address(vars.inputToken));
    console.log("balanceOf                  ", IERC20(vars.inputToken).balanceOf(address(fld.wallet)));
    console.log("osp.toTokenAddress         ", address(vars.outputToken));
    console.log("balanceOf                  ", IERC20(vars.outputToken).balanceOf(address(fld.wallet)));

    emit LogHeader("WALLET BALANCE (AFTER)");
    emit LogAddress("osp.fromTokenAddress       : ", address(vars.inputToken));
    emit LogValue("balanceOf                  : ", IERC20(vars.inputToken).balanceOf(address(fld.wallet)));
    emit LogAddress("osp.toTokenAddress         : ", address(vars.outputToken));
    emit LogValue("balanceOf                  : ", IERC20(vars.outputToken).balanceOf(address(fld.wallet)));

    emit NewArbitrage(vars.amountReceived - vars.amountOwed, block.timestamp);

    return true;
  }

  function tradeUniswapToUniswap(bytes memory data)
  public returns (uint) {
    // Decode the params as a tuple containing an address and bytes array
    FlashLoanData memory fld;
    (fld.weth, fld.wallet, fld.route) = abi.decode(data, (address, address, bytes[]));

    // Now you can decode each SwapData struct inside the fld.route array
    SwapData[] memory swapParams = new SwapData[](fld.route.length);

    swapParams[0] = abi.decode(fld.route[0], (SwapData));

    uint256 amountIn;
    uint256 amountOut;

    for (uint256 i = 0; i < fld.route.length; i++) {
      swapParams[i] = abi.decode(fld.route[i], (SwapData));

      // Accessing individual elements of the struct at index i
      if (i == 0) {
        amountIn = swapParams[i].amountIn;
      } else {
        amountIn = amountOut;
      }

      if (swapParams[i].version == 2) {
        amountOut = uniSwapV2(amountIn, 
                  swapParams[i].token0Address, 
                  swapParams[i].token1Address, 
                  swapParams[i].router,
                  swapParams[i].name);
      }
      if (swapParams[i].version == 3) {
        amountOut = uniSwapV3(amountIn, 
                  swapParams[i].token0Address, 
                  swapParams[i].token1Address, 
                  swapParams[i].fee,
                  swapParams[i].router,
                  swapParams[i].name);
      }
    } // End for loop

    return amountOut;
  }

  struct Uniswap {
      IUniswapV2Router02 uniRouter;
      ISwapRouter swapRouter;
      IERC20 fromIERC20;
      IERC20 toIERC20;
      bool success;
      bytes data;
      uint256 amountReceived;
      uint256 feeTier;
      uint256 feeAmount;
      uint256 amount0Min;
  }
  // Swap tokens on Uniswap V2 FORK exchange
  function uniSwapV2(uint amount, 
                     address token0Address, 
                     address token1Address, 
                     address router,
                     string memory name) internal returns (uint256) {
    Uniswap memory uni;

    uni.uniRouter = IUniswapV2Router02(router);
    uni.fromIERC20 = IERC20(token0Address);
    uni.toIERC20 = IERC20(token1Address);

    uni.fromIERC20.safeIncreaseAllowance(address(uni.uniRouter), amount);

    address[] memory path = new address[](2);
    path[0] = address(uni.fromIERC20);
    path[1] = address(uni.toIERC20);

    uint[] memory minOuts;

    minOuts = uni.uniRouter.getAmountsOut(amount, path);
    minOuts[1] = minOuts[1].mul(99).div(100); //TODO reduced %slippage to fix Uniswap K error (98% orig value)

    uni.amountReceived = uni.uniRouter.swapExactTokensForTokens(
                              amount,
                              minOuts[1], 
                              path, 
                              address(this),
                              block.timestamp
    )[1];

    uni.fromIERC20.safeDecreaseAllowance(address(uni.uniRouter), 0);

    return uni.amountReceived;
  }

  // Swap tokens on Uniswap V3 FORK exchange
  function uniSwapV3(uint amount, 
                     address token0Address, 
                     address token1Address,
                     uint fee,
                     address router,
                     string memory name) internal returns (uint256) {
    Uniswap memory uni;

    uni.swapRouter = ISwapRouter(router);        // Uniswap V3 fork router
    uni.fromIERC20 = IERC20(token0Address);
    uni.toIERC20 = IERC20(token1Address);

    require(fee <= type(uint24).max, "uniSwapV3: 'fee' exceeds uint24 range");

    uni.fromIERC20.safeIncreaseAllowance(address(uni.swapRouter), amount);

    address[] memory path = new address[](2);
    path[0] = address(uni.fromIERC20);
    path[1] = address(uni.toIERC20);

    uni.amount0Min = 0;

    // call exactInputSingle for swapping token1 for token0 in pool w/fee2
    uni.amountReceived =
      uni.swapRouter.exactInputSingle(
          ISwapRouter.ExactInputSingleParams({
              tokenIn: path[0],
              tokenOut: path[1],
              fee: uint24(fee),
              recipient: address(this),
              deadline: block.timestamp,
              amountIn: amount,
              amountOutMinimum: uni.amount0Min,
              sqrtPriceLimitX96: 0
          })
    );

    uni.fromIERC20.safeDecreaseAllowance(address(uni.swapRouter), 0);

    return uni.amountReceived;
  }
