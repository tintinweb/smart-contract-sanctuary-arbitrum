    function totalAccounts() public view override returns (uint256) {
        return _hasAccountsStore().totalAccounts;
    }

    function _incrementTotalAccounts() internal {
        _update(true);
    }

    function _decrementTotalAccounts() internal {
        _update(false);
    }

    function _update(bool add) private {
        HasAccountsStore storage $ = _hasAccountsStore();

        uint256 _totalAccounts = $.totalAccounts;

        unchecked {
            if (add) {
                _totalAccounts++;
            } else {
                _totalAccounts--;
            }
        }

        $.totalAccounts = _totalAccounts;

        emit TotalAccountsUpdate(_totalAccounts);
    }
}

