pragma solidity =0.8.20;

import {AccessControlUpgradeable} from "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import {MulticallUpgradeable} from "@openzeppelin/contracts-upgradeable/utils/MulticallUpgradeable.sol";
import {HasConfig} from "@kei.fi/config-contracts/extensions/HasConfig.sol";

import {ITradingFees} from "./lib/TradingFees/ITradingFees.sol";
import {BaseAsset} from "./lib/BaseAsset/BaseAsset.sol";

import {UniswapV3Module} from "./modules/UniswapV3/UniswapV3Module.sol";
import {IAssetManager} from "./IAssetManager.sol";

contract AssetManager is IAssetManager, AccessControlUpgradeable, UniswapV3Module, MulticallUpgradeable {
    bytes32 public constant EXECUTE_ROLE = keccak256("EXECUTE_ROLE");

    constructor(address config, address uniswapV3Router, address uniswapV3Quoter)
        UniswapV3Module(uniswapV3Router, uniswapV3Quoter)
        HasConfig(config)
    {}

    function deposit(address account, uint256 amount) external override {
        _deposit(account, amount);
    }

    function withdraw(uint256 shares, uint256[][] memory positionIds, bytes[] memory data) external override {
        _withdraw(shares, positionIds, data);
    }

    function initialise(
        address baseAsset,
        string memory name,
        string memory symbol,
        address executor,
        bytes[] memory paths
    ) external initializer {
        __BaseAsset_init(baseAsset);
        __HasAssets_init();

        __AccessControl_init();
        __Multicall_init();
        __ERC20_init(name, symbol);

        _grantRole(EXECUTE_ROLE, executor);

        for (uint256 i; i < paths.length; i++) {
            _addUniswapV3Path(paths[i]);
        }
    }

    function transferRole(bytes32 role, address newUser) external onlyRole(role) {
        _revokeRole(role, msg.sender);
        _grantRole(role, newUser);
    }

    function execute(ExecuteTradeParams[] calldata commands) external onlyRole(EXECUTE_ROLE) returns (Trade[] memory) {
        return _execute(commands);
    }

    function estimate(ExecuteTradeParams[] calldata commands)
        external
        returns (Trade[] memory trades, int256[] memory profit, ITradingFees.FeeAmount[][] memory fees)
    {
        return _estimate(commands);
    }
}

