    using SafeERC20 for IERC20;

    function _deposit(address account, uint256 amount) internal {
        if (account == address(0)) {
            revert TradingModuleZeroAddress();
        }

        if (amount == 0) {
            revert TradingModuleZeroAmount();
        }

        address _baseAsset = baseAsset();

        uint256 _prevBalance = IERC20(_baseAsset).balanceOf(address(this));
        IERC20(_baseAsset).safeTransferFrom(_msgSender(), address(this), amount);
        uint256 _received = IERC20(_baseAsset).balanceOf(address(this)) - _prevBalance;

        if (_received == 0) {
            revert TradingModuleZeroAmount();
        }

        // lets calculate the total value contained in this contract
        uint256 _totalBaseValue = _calculateTotalValue(_baseAsset) - _received;
        uint256 _shares = _calculateShares(_totalBaseValue, amount);

        if (_shares == 0) {
            revert TradingModuleNoSharesSupplied();
        }

        if (balanceOf(account) == _shares) {
            _incrementTotalAccounts();
        }

        _mint(account, _shares);

        emit Deposit(account, amount, _shares);
    }

    function _withdraw(uint256 shares, uint256[][] memory positionIds, bytes[] memory data) internal {
        address _account = msg.sender;
        uint256 _currentShares = balanceOf(_account);
        address _baseAsset = baseAsset();

        if (_currentShares < shares) {
            revert TradingModuleInsufficientShares();
        }

        uint256 _amount = _convertSharesToBase(_baseAsset, shares, positionIds, data, _assetManagerConfig());

        if (_currentShares == shares) {
            _decrementTotalAccounts();
        }

        _burn(_account, shares);

        IERC20(_baseAsset).safeTransfer(_account, _amount);

        emit Withdraw(_account, _amount, shares);
    }

    function tradingData(address fromAsset, address toAsset) public view returns (TradingData memory) {
        return _tradingModuleStore().tradingData[keccak256(abi.encodePacked(fromAsset, toAsset))];
    }

    function _execute(ExecuteTradeParams[] memory commands) internal virtual returns (Trade[] memory trades) {
        trades = new Trade[](commands.length);
        address _baseAsset = baseAsset();
        IKEIConfig.AssetManagerConfig memory _config = _assetManagerConfig();
        for (uint256 i; i < commands.length; i++) {
            trades[i] = _execute(_baseAsset, commands[i], _config);
        }
    }

    function _estimate(ExecuteTradeParams[] memory commands)
        internal
        virtual
        returns (Trade[] memory trades, int256[] memory profits, ITradingFees.FeeAmount[][] memory feeAmounts)
    {
        trades = new Trade[](commands.length);
        profits = new int256[](commands.length);
        feeAmounts = new ITradingFees.FeeAmount[][](commands.length);
        address _baseAsset = baseAsset();
        IKEIConfig.AssetManagerConfig memory _config = _assetManagerConfig();
        for (uint256 i; i < commands.length; i++) {
            (trades[i], profits[i], feeAmounts[i]) = _estimate(_baseAsset, commands[i], _config);
        }
    }

    function _execute(address baseAsset, ExecuteTradeParams memory params, IKEIConfig.AssetManagerConfig memory config)
        internal
        virtual
        returns (Trade memory trade)
    {
        TradingData memory _data = tradingData(params.fromAsset, params.toAsset);

        if (_data.module == bytes32(0)) {
            revert TradingModuleUnsupportedTrade(params.fromAsset, params.toAsset);
        }

        if (!config.enabled) {
            revert TradingModuleDisabled();
        }

        trade = _createTrade(params.fromAsset, params.toAsset, params.amount, _handleExecute(baseAsset, params, _data));
        int256 _profit = _recordProfit(baseAsset, trade, params.positionIds);
        _handleTradeFees(config, baseAsset, trade, _profit);

        emit ExecuteComplete(trade.id, params.fromAsset, params.toAsset, params, trade, _profit);
    }

    function _estimate(address baseAsset, ExecuteTradeParams memory params, IKEIConfig.AssetManagerConfig memory config)
        internal
        returns (Trade memory trade, int256 profit, ITradingFees.FeeAmount[] memory feeAmounts)
    {
        TradingData memory _data = tradingData(params.fromAsset, params.toAsset);

        if (_data.module == bytes32(0)) {
            revert TradingModuleUnsupportedTrade(params.fromAsset, params.toAsset);
        }

        trade = Trade({
            id: 0,
            fromAsset: params.fromAsset,
            toAsset: params.toAsset,
            fromAmount: params.amount,
            toAmount: _handleEstimate(baseAsset, params, _data)
        });
        profit = _estimateProfit(baseAsset, trade, params.positionIds);
        feeAmounts = _estimateTradeFees(config, baseAsset, trade, profit);
    }

    function _handleExecute(address baseAsset, ExecuteTradeParams memory command, TradingData memory data)
        internal
        virtual
        returns (uint256)
    {
        revert TradingModuleTradingUnsupportedOperation();
    }

    function _handleEstimate(address baseAsset, ExecuteTradeParams memory command, TradingData memory data)
        internal
        virtual
        returns (uint256)
    {
        revert TradingModuleTradingUnsupportedOperation();
    }

    function _updateTradingData(address fromAsset, address toAsset, TradingData memory data) internal virtual {
        TradingModuleStore storage $ = _tradingModuleStore();

        bytes32 _key = keccak256(abi.encodePacked(fromAsset, toAsset));
        TradingData memory _prevData = $.tradingData[_key];

        if (keccak256(abi.encode(_prevData)) == keccak256(abi.encode(data))) {
            revert TradingModuleTradingDataUnchanged();
        }

        $.tradingData[_key] = data;

        emit TradingDataUpdate(fromAsset, toAsset, data.module, data.data);
    }

    function _calculateShares(uint256 totalBaseValue, uint256 addedValue) private view returns (uint256) {
        if (totalBaseValue == 0) {
            return addedValue * 100;
        } else {
            return (addedValue * totalSupply()) / totalBaseValue;
        }
    }

    function _convertSharesToBase(
        address baseAsset,
        uint256 shares,
        uint256[][] memory positionIds,
        bytes[] memory data,
        IKEIConfig.AssetManagerConfig memory config
    ) private returns (uint256 totalBaseValue) {
        uint256 _totalSupply = totalSupply();
        uint256 _totalAssets = totalAssets();

        for (uint256 i; i < _totalAssets; i++) {
            address _asset = assetAt(i);

            ExecuteTradeParams memory _params;

            _params.fromAsset = _asset;
            _params.toAsset = baseAsset;
            _params.amount = (IERC20(baseAsset).balanceOf(address(this)) * shares) / _totalSupply;
            _params.positionIds = positionIds[i];
            _params.data = data[i];

            Trade memory _trade = _execute(baseAsset, _params, config);

            totalBaseValue += _trade.toAmount;
        }
    }

    function _calculateTotalValue(address baseAsset) private returns (uint256 totalBaseValue) {
        uint256 _totalAssets = totalAssets();

        totalBaseValue += IERC20(baseAsset).balanceOf(address(this));

        // here we start from 1 because 0 is the baseAsset
        for (uint256 i = 1; i < _totalAssets; i++) {
            address _asset = assetAt(i);
            uint256 _balance = IERC20(_asset).balanceOf(address(this));
            if (_balance > 0) {
                totalBaseValue += _handleEstimate(
                    baseAsset,
                    ExecuteTradeParams({
                        fromAsset: _asset,
                        toAsset: baseAsset,
                        amount: _balance,
                        data: "",
                        positionIds: new uint256[](0)
                    }),
                    tradingData(_asset, baseAsset)
                );
            }
        }
    }
}

