pragma solidity =0.8.20;

import {IBaseTrading} from "./IBaseTrading.sol";
import {BaseTradingStorage} from "./BaseTradingStorage.sol";

abstract contract BaseTrading is IBaseTrading, BaseTradingStorage {
    function totalTrades() public view returns (uint256) {
        return _baseTradingStore().totalTrades;
    }

    function _createTrade(address fromAsset, address toAsset, uint256 fromAmount, uint256 toAmount)
        internal
        returns (Trade memory trade)
    {
        uint256 _id = ++_baseTradingStore().totalTrades;
        trade = Trade(_id, fromAsset, toAsset, fromAmount, toAmount);
        emit NewTrade(_id, fromAsset, toAsset, fromAmount, toAmount);
    }
}

