pragma solidity ^0.8.0;

import "./TradingRecord.sol";

interface ITradingRecord {
    event EnterPosition(
        uint256 indexed id, uint256 indexed tradeId, address indexed asset, uint256 assetAmount, uint256 baseValue
    );

    event ExitPosition(
        uint256 indexed id,
        uint256 indexed tradeId,
        address indexed asset,
        uint256 assetAmount,
        uint256 exitBaseValue,
        uint256 entryBaseValue,
        int256 profit,
        uint256 newPositionId
    );

    struct OpenPosition {
        address asset;
        uint128 assetAmountRemaining;
        uint128 baseValueRemaining;
    }

