import {ISwapRouter} from "@uniswap/v3-periphery/contracts/interfaces/ISwapRouter.sol";
import {IQuoter} from "@uniswap/v3-periphery/contracts/interfaces/IQuoter.sol";
import {IERC20} from "@openzeppelin/contracts/token/ERC20/IERC20.sol";

import {TradingModule} from "../../lib/TradingModule/TradingModule.sol";
import {HasAssets} from "../../lib/HasAssets/HasAssets.sol";

import {Path, BytesLib} from "./lib/Path.sol";
import {PoolAddress} from "./lib/PoolAddress.sol";

import {IUniswapV3ModuleErrors} from "./IUniswapV3ModuleErrors.sol";
import {IUniswapV3Module} from "./IUniswapV3Module.sol";

abstract contract UniswapV3Module is IUniswapV3Module, IUniswapV3ModuleErrors, TradingModule {
    using Path for bytes;
    using BytesLib for bytes;

    bytes32 public constant UNISWAP_V3_MODULE = bytes32("uniswap-v3");

    ISwapRouter public immutable UNISWAP_V3_SWAP_ROUTER;
    IQuoter public immutable UNISWAP_V3_QUOTER;

    constructor(address router, address quoter) {
        UNISWAP_V3_SWAP_ROUTER = ISwapRouter(router);
        UNISWAP_V3_QUOTER = IQuoter(quoter);
    }

    function uniswapV3PathApproved(bytes memory path) public view override returns (bool) {
        (address _assetA, address _assetB, bytes memory _pathToA, bytes memory _pathToB) = _computePathDetails(path);
        return keccak256(tradingData(_assetA, _assetB).data) == keccak256(_pathToB)
            && keccak256(tradingData(_assetB, _assetA).data) == keccak256(_pathToA);
    }

    function _addUniswapV3Path(bytes memory path) internal {
        if (uniswapV3PathApproved(path)) {
            revert UniswapV3ModulePathAlreadyApproved();
        }

        (address _assetA, address _assetB, bytes memory _pathToA, bytes memory _pathToB) = _computePathDetails(path);

        _updateTradingData(_assetA, _assetB, TradingData(UNISWAP_V3_MODULE, _pathToB));
        _updateTradingData(_assetB, _assetA, TradingData(UNISWAP_V3_MODULE, _pathToA));

        _addAsset(_assetA);
        _addAsset(_assetB);

        IERC20(_assetA).approve(address(UNISWAP_V3_SWAP_ROUTER), type(uint256).max);
        IERC20(_assetB).approve(address(UNISWAP_V3_SWAP_ROUTER), type(uint256).max);

        emit UniswapV3ModuleAddPath(_assetA, _assetB, _pathToA, _pathToB);
    }

    function _removeUniswapV3Path(bytes memory path) internal {
        if (!uniswapV3PathApproved(path)) {
            revert UniswapV3ModulePathNotApproved();
        }

        (address _assetA, address _assetB, bytes memory _pathToA, bytes memory _pathToB) = _computePathDetails(path);

        _removeAsset(_assetA);
        _removeAsset(_assetB);

        _updateTradingData(_assetA, _assetB, TradingData(UNISWAP_V3_MODULE, _pathToB));
        _updateTradingData(_assetB, _assetA, TradingData(UNISWAP_V3_MODULE, _pathToA));

        emit UniswapV3ModuleRemovePath(_assetA, _assetB, _pathToA, _pathToB);
    }

    function _handleExecute(address baseAsset, ExecuteTradeParams memory command, TradingData memory data)
        internal
        virtual
        override
        returns (uint256)
    {
        if (data.module != UNISWAP_V3_MODULE) {
            return super._handleExecute(baseAsset, command, data);
        }

        (uint256 _amountOutMinimum, uint256 _deadline) = abi.decode(command.data, (uint256, uint256));

        ISwapRouter.ExactInputParams memory _params;

        _params.path = data.data;
        _params.recipient = address(this);
        _params.amountIn = command.amount;
        _params.amountOutMinimum = _amountOutMinimum;
        _params.deadline = _deadline;

        return UNISWAP_V3_SWAP_ROUTER.exactInput(_params);
    }

    function _handleEstimate(address baseAsset, ExecuteTradeParams memory command, TradingData memory data)
        internal
        virtual
        override
        returns (uint256)
    {
        if (data.module != UNISWAP_V3_MODULE) {
            return super._handleEstimate(baseAsset, command, data);
        }

        return UNISWAP_V3_QUOTER.quoteExactInput(data.data, command.amount);
    }

    function _computePathDetails(bytes memory path)
        private
        pure
        returns (address assetA, address assetB, bytes memory pathToA, bytes memory pathToB)
    {
        bytes memory _pathReversed = _reversePath(path);

        address _fromAsset = path.toAddress(0);
        address _toAsset = path.toAddress(path.length - Path.ADDR_SIZE);

        if (_fromAsset == _toAsset) {
            revert UniswapV3ModulePathHasSameToAndFrom(_fromAsset);
        }

        (assetA, assetB) = _fromAsset < _toAsset ? (_fromAsset, _toAsset) : (_toAsset, _fromAsset);
        (pathToA, pathToB) = _fromAsset < _toAsset ? (_pathReversed, path) : (path, _pathReversed);
    }

    function _reversePath(bytes memory path) private pure returns (bytes memory inversePath) {
        bool _hasMultiplePools;

        do {
            _hasMultiplePools = path.hasMultiplePools();

            (address _tokenA, address _tokenB, uint24 _fee) = path.decodeFirstPool();

            if (inversePath.length == 0) {
                inversePath = abi.encodePacked(_tokenA);
            }

            // build the inverse of the path
            inversePath = abi.encodePacked(_tokenB, _fee, inversePath);

            if (_hasMultiplePools) {
                path = path.skipToken();
            }
        } while (_hasMultiplePools);
    }
}

