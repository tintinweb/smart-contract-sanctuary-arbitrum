import {BaseAsset} from "../BaseAsset/BaseAsset.sol";

import {IHasAssets} from "./IHasAssets.sol";
import {HasAssetsStorage} from "./HasAssetsStorage.sol";

abstract contract HasAssets is IHasAssets, HasAssetsStorage, BaseAsset {
    using EnumerableSet for EnumerableSet.AddressSet;

    function __HasAssets_init() internal onlyInitializing {
        _hasAssetsStore().assets.add(baseAsset());
    }

    function assets() public view override returns (address[] memory) {
        return _hasAssetsStore().assets.values();
    }

    function totalAssets() public view override returns (uint256) {
        return _hasAssetsStore().assets.length();
    }

    function assetAt(uint256 index) public view override returns (address) {
        return _hasAssetsStore().assets.at(index);
    }

    function _addAsset(address asset) internal virtual {
        if (asset == baseAsset()) return;

        HasAssetsStore storage $ = _hasAssetsStore();

        // we need to keep track of the total assets so that we can add or remove it depending on the total asset pathways listed
        uint256 _count = $.assetCount[asset];

        if (_count == 0) {
            $.assets.add(asset);
            emit AddAsset(asset);
        }

        unchecked {
            $.assetCount[asset] = _count + 1;
        }
    }

    function _removeAsset(address asset) internal virtual {
        if (asset == baseAsset()) return;

        HasAssetsStore storage $ = _hasAssetsStore();

        uint256 _count = $.assetCount[asset];

        if (_count == 1) {
            $.assets.remove(asset);
            emit RemoveAsset(asset);
        }

        unchecked {
            $.assetCount[asset] = _count - 1;
        }
    }
}

