import {IERC20} from "@openzeppelin/contracts/interfaces/IERC20.sol";
import {SafeERC20} from "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";

import {IKEIConfig} from "@kei.fi/config-contracts/IKEIConfig.sol";
import {BaseAsset} from "../BaseAsset/BaseAsset.sol";
import {BaseTrading} from "../BaseTrading/BaseTrading.sol";

import {ITradingFees} from "./ITradingFees.sol";
import {TradingFeesStorage} from "./TradingFeesStorage.sol";

abstract contract TradingFees is ITradingFees, TradingFeesStorage, BaseAsset, BaseTrading {
    using SafeERC20 for IERC20;

    function _handleTradeFees(
        IKEIConfig.AssetManagerConfig memory config,
        address baseAsset,
        Trade memory trade,
        int256 profit
    ) internal returns (FeeAmount[] memory amounts) {
        amounts = _estimateTradeFees(config, baseAsset, trade, profit);

        FeeAmount memory _fee = amounts[0];
        if (_fee.amount > 0) {
            IERC20(_fee.asset).safeTransfer(_fee.recipient, _fee.amount);
        }

        _fee = amounts[1];
        if (_fee.amount > 0) {
            IERC20(_fee.asset).safeTransfer(_fee.recipient, _fee.amount);
        }

        emit Fees(trade.id, amounts);

        return amounts;
    }

    function _estimateTradeFees(
        IKEIConfig.AssetManagerConfig memory config,
        address baseAsset,
        Trade memory trade,
        int256 profit
    ) internal view returns (FeeAmount[] memory amounts) {
        uint256 _fee;
        FeeConfig memory _feeConfig = _tradingFeesStore().config;

        // there should be the same amount of fee items as there are FeeTypes in the enum.
        amounts = new FeeAmount[](2);

        if (baseAsset == trade.toAsset && profit > 0 && config.feeRecipient != address(0) && config.performanceFee != 0)
        {
            _fee = (uint256(profit) * config.performanceFee) / config.basePercent;
            if (_fee > 0) {
                amounts[0] = FeeAmount({asset: baseAsset, amount: _fee, recipient: config.feeRecipient});
            }
        }

        if (_feeConfig.tradeFeePercent > 0) {
            // FIXED fee per transaction - defined by 10_000 as base percent
            // TODO we need to use a percentage library for this contract
            _fee = _feeConfig.tradeFeePercent * trade.toAmount / 10_000;
            if (_fee > 0) {
                amounts[1] = FeeAmount({asset: trade.toAsset, amount: _fee, recipient: config.feeRecipient});
            }
        }

        return amounts;
    }

    function _updateTradingFees(FeeConfig calldata feeConfig) external {}
}

