import {SafeCast} from "@openzeppelin/contracts/utils/math/SafeCast.sol";

import {BaseAsset} from "../BaseAsset/BaseAsset.sol";
import {BaseTrading} from "../BaseTrading/BaseTrading.sol";

import {ITradingRecord} from "./ITradingRecord.sol";
import {TradingRecordStorage} from "./TradingRecordStorage.sol";
import {ITradingRecordErrors} from "./ITradingRecordErrors.sol";

abstract contract TradingRecord is
    ITradingRecord,
    ITradingRecordErrors,
    TradingRecordStorage,
    BaseAsset,
    BaseTrading
{
    using SafeCast for uint256;

    uint256 public constant PRICE_DECIMALS = type(uint64).max;
    uint256 public constant BASE_PERCENT = 10000;

    function openPosition(uint256 id) external view returns (OpenPosition memory) {
        return _tradingRecordStore().openPosition[id];
    }

    function _estimateProfit(address baseAsset, Trade memory trade, uint256[] memory entryPositionIds)
        internal
        view
        returns (int256 totalBaseProfit)
    {
        TradingRecordStore storage $ = _tradingRecordStore();

        if (trade.fromAsset == trade.toAsset) {
            revert TradingRecordInvalidTrade(trade);
        }

        if (trade.toAsset != baseAsset) {
            return 0;
        }

        TradeCache memory _cache;
        _cache.remainingPositionsAmount = trade.fromAsset == baseAsset ? 0 : $.totalPositionsAmount[trade.fromAsset];
        _cache.remainingAssetAmount = trade.fromAmount;

        if (trade.fromAsset == baseAsset) {
            _cache.totalBaseValue = trade.fromAmount;
        } else {
            for (uint256 i = 0; i < entryPositionIds.length; ++i) {
                uint256 _positionId = entryPositionIds[i];
                OpenPosition memory _position = $.openPosition[_positionId];

                if (trade.fromAsset != _position.asset) {
                    revert TradingRecordDifferingAssets(trade, _position);
                }

                uint256 _assetsUsed = _cache.remainingAssetAmount > _position.assetAmountRemaining
                    ? _position.assetAmountRemaining
                    : _cache.remainingAssetAmount;
                uint256 _baseUsed = _cache.remainingAssetAmount > _position.assetAmountRemaining
                    ? _position.baseValueRemaining
                    : (_position.baseValueRemaining * _assetsUsed) / _position.assetAmountRemaining;

                unchecked {
                    _cache.remainingAssetAmount -= _assetsUsed;
                    _cache.totalBaseValue += _baseUsed;
                }

                if (_cache.remainingAssetAmount == 0) break;
            }
        }

        // we do not open any more positions as we are selling back to base
        return int256(trade.toAmount) - int256(_cache.totalBaseValue);
    }

    function _recordProfit(address baseAsset, Trade memory trade, uint256[] memory entryPositionIds)
        internal
        returns (int256 totalBaseProfit)
    {
        TradingRecordStore storage $ = _tradingRecordStore();

        if (trade.fromAmount == 0 || trade.toAmount == 0) {
            revert TradingRecordZeroAmount(trade);
        }

        if (trade.fromAsset == trade.toAsset) {
            revert TradingRecordInvalidTrade(trade);
        }

        TradeCache memory _cache;
        _cache.remainingPositionsAmount = trade.fromAsset == baseAsset ? 0 : $.totalPositionsAmount[trade.fromAsset];
        _cache.remainingAssetAmount = trade.fromAmount;

        if (trade.toAsset != baseAsset) {
            _cache.newPositionId = ++$.currentPositionId;
        }

        if (trade.fromAsset == baseAsset) {
            if (entryPositionIds.length > 0) {
                revert TradingRecordCannotHaveEntriesWhenBuying();
            }
            _cache.totalBaseValue = trade.fromAmount;
        } else {
            for (uint256 i = 0; i < entryPositionIds.length && _cache.remainingAssetAmount > 0; ++i) {
                uint256 _positionId = entryPositionIds[i];
                OpenPosition memory _position = $.openPosition[_positionId];

                if (trade.fromAsset != _position.asset) {
                    revert TradingRecordDifferingAssets(trade, _position);
                }

                uint256 _assetsUsed = _cache.remainingAssetAmount > _position.assetAmountRemaining
                    ? _position.assetAmountRemaining
                    : _cache.remainingAssetAmount;
                uint256 _baseUsed = _cache.remainingAssetAmount > _position.assetAmountRemaining
                    ? _position.baseValueRemaining
                    : (_position.baseValueRemaining * _assetsUsed) / _position.assetAmountRemaining;

                unchecked {
                    _cache.remainingAssetAmount -= _assetsUsed;
                    _cache.totalBaseValue += _baseUsed;

                    // do not let the remaining pending amount go to 0
                    if (_cache.remainingPositionsAmount > _assetsUsed) {
                        _cache.remainingPositionsAmount -= _assetsUsed;
                    } else {
                        _cache.remainingPositionsAmount = 0;
                    }
                }

                if (_position.assetAmountRemaining == _assetsUsed) {
                    // the position has been fully closed
                    delete $.openPosition[_positionId];
                } else {
                    // the position has been partially closed. therefore lets update it
                    unchecked {
                        _position.assetAmountRemaining -= _assetsUsed.toUint128();
                        _position.baseValueRemaining -= _baseUsed.toUint128();
                    }
                    $.openPosition[_positionId] = _position;
                }

                uint256 _exitBaseAmount = _baseUsed;
                if (trade.toAsset == baseAsset) {
                    _exitBaseAmount = (trade.toAmount * _assetsUsed) / trade.fromAmount;
                }

                int256 _profit = (int256(_exitBaseAmount) - int256(_baseUsed));

                emit ExitPosition(
                    _positionId,
                    trade.id,
                    trade.fromAsset,
                    _assetsUsed,
                    _exitBaseAmount,
                    _baseUsed,
                    _profit,
                    _cache.newPositionId
                );
            }
        }

        // if we fail to make all the entries sufficiently, then we should revert. We have to mark all exits with an entry
        if (_cache.remainingPositionsAmount > 0 && _cache.remainingAssetAmount > 0) {
            revert TradingRecordUnsoldEntryPositions(_cache.remainingPositionsAmount, _cache.remainingAssetAmount);
        }

        if (trade.fromAsset != baseAsset) {
            // we want to update the new remaining position amount after closing all positions
            // only if we are updating an existing position. (not buying from the BASE VALUE)
            $.totalPositionsAmount[trade.fromAsset] = _cache.remainingPositionsAmount;
        }

        if (_cache.newPositionId == 0) {
            // we do not open any more positions as we are selling back to base
            totalBaseProfit = int256(trade.toAmount) - int256(_cache.totalBaseValue);
        } else {
            // we opening another position as we are not selling to base
            // if the _cache.remainingAssetAmount is more than 0 here, we dont need to do anything as the exit profit will be
            // calculated at a later date, (when exiting back to base)
            $.openPosition[_cache.newPositionId] =
                OpenPosition(trade.toAsset, trade.toAmount.toUint128(), _cache.totalBaseValue.toUint128());
            // add the amount of open positions here to later keep track of positions that are still pending
            $.totalPositionsAmount[trade.toAsset] += trade.toAmount;
            emit EnterPosition(_cache.newPositionId, trade.id, trade.toAsset, trade.toAmount, _cache.totalBaseValue);
        }

        return totalBaseProfit;
    }
