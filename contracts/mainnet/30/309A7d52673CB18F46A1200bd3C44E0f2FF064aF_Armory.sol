    struct Item {
        uint8 id;
        uint8 itemType;
        string itemName;
        uint8 focus;
        uint8 strength;
        uint8 intellect;
        uint8 spell;
        uint8 endurance;
        string lore;
    }
    
    function getItem(uint256 _id) external view returns(uint8 itemType, string memory itemName, string memory lore, uint8[5] memory stats){
        uint8[5] memory statsToReturn;
        statsToReturn[0] = genesisFocus[_id];
        statsToReturn[1] = genesisStrength[_id];
        statsToReturn[2] = genesisIntellect[_id];
        statsToReturn[3] = genesisSpell[_id];
        statsToReturn[4] = genesisEndurance[_id];
        return (genesisTypes[_id],genesisNames[_id],lores[_id],statsToReturn);
    }

    function getItemType(uint256 _id) external view returns(uint256){
        return genesisTypes[_id];
    }

    function getItemLevel(uint256 _id) external view returns (uint256){
        return genesisLevels[_id];
    }

    function getItemStats(uint256 _id) external view returns (uint8[5] memory){
        uint8[5] memory statsToReturn;
        statsToReturn[0] = genesisFocus[_id];
        statsToReturn[1] = genesisStrength[_id];
        statsToReturn[2] = genesisIntellect[_id];
        statsToReturn[3] = genesisSpell[_id];
        statsToReturn[4] = genesisEndurance[_id];
        return statsToReturn;
    }


    function addLore(string[] memory _lores, uint8[] memory _itemIds) external onlyOwner{
        for(uint i=0;i<_itemIds.length;i++){
            lores[_itemIds[i]]= _lores[i];
        }
    }

    function editItem(uint8 _id, uint8 _level, uint8 _itemType, string memory _itemName, string memory _lore, uint8[5] memory _stats) external onlyOwner{
        if(_id<495){
            genesisTypes[_id]= _itemType;
            genesisNames[_id]=_itemName;
            genesisFocus[_id]= _stats[0];
            genesisStrength[_id]= _stats[1];
            genesisIntellect[_id]=_stats[2];
            genesisSpell[_id]=_stats[3];
            genesisEndurance[_id]=_stats[3];
            lores[_id]= _lore;
            genesisLevels[_id]=_level;
        }else{
            types[_id]= _itemType;
            names[_id]=_itemName;
            focus[_id]= _stats[0];
            strength[_id]= _stats[1];
            intellect[_id]=_stats[2];
            spell[_id]=_stats[3];
            endurance[_id]=_stats[3];
            lores[_id]= _lore;
            levels[_id]=_level;
        }
       
    }

