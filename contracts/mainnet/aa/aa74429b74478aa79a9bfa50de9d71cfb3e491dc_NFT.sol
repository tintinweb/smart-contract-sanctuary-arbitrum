        if (block.number <= pool.lastRewardBlock) {
            return;
        }
        uint256 lpSupply = pool.pairToken.balanceOf(address(this));
        if (lpSupply == 0) {
            pool.lastRewardBlock = block.number;
            return;
        }
        if (totalAllocPoint != 0) {
            int256 blocks = int256(block.number - pool.lastRewardBlock);
            int256 totalReward = int256(lastBalance) *
                blocks +
                (k * blocks ** 2) /
                2 /
                int256(HUNDRED_PERCENTS);
            if (totalReward > 0) {
                uint256 reward = (uint256(totalReward) * pool.allocPoint) /
