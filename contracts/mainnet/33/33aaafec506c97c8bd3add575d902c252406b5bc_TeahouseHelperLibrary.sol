library TeahouseHelperLibrary {
    using EnumerableSet for EnumerableSet.AddressSet;
    using SafeCast for int256;
    using SafeCast for uint256;

    /// @notice Maximum basis points capped to 1%
    uint256 internal constant BPS = 100;

    /// @notice The staleness threshold
    uint256 internal constant STALENESS_THRESHOLD = 24 hours;

    /// @notice The grace period
    uint256 internal constant GRACE_PERIOD_TIME = 60 minutes;

    /// @notice The asset config arguments
    struct AssetConfigArgs {
        /// @notice The asset being swapped from
        address assetIn;
        /// @notice The asset being swapped to
        address assetOut;
        /// @notice True if the price reported from the chainlink
        /// price feed should be inverted
        bool shouldInvert;
        /// @notice The asset's chainlink pricefeed
        address chainlinkPriceFeed;
        /// @notice The assets' fallback price if a Chainlink price feed does not exist.
        /// @dev This should only be used if the conversion between the
        /// asset pair is constant
        uint256 stableFallbackPrice;
        /// @notice The pool fee in BPS
        uint256 feeBps;
        /// @notice The path to swap the assets
        bytes swapPath;
        /// @notice Params that can be used to derive the
        /// value of an asset
        AssetContractOracleParams assetContractOracleParams;
        /// @notice The router address to perform swaps
        /// @dev This is used in case we need to swap on a DEX
        /// that is a Uniswap clone
        address swapRouter;
    }

    /// @notice The swap parameters
    struct SwapConfig {
        /// @notice The pool fee in BPS
        uint256 feeBps;
        /// @notice The path to swap the assets
        bytes swapPath;
        /// @notice The router address to perform swaps
        /// @dev This is used in case we need to swap on a DEX
        /// that is a Uniswap clone
        address swapRouter;
    }

    /// @notice Parameters to value an asset if a Chainlink oracle
    /// does not exist AND the asset is not a stable coin
    struct AssetContractOracleParams {
        /// @notice The address to call the value function at
        address assetContract;
        /// @notice The number of decimals the result of the
        /// value function is denominated in
        uint256 decimals;
        /// @notice Function call to fetch the price of the asset
        bytes assetOracleFnCall;
    }

    /// @notice Parameters packed together to reduce contract size
    struct StorageParams {
        /// @notice Max price difference threshold between the oracle and spot price values
        uint256 priceThreshold;
        /// @notice The address of the L2 Sequencer UptimeFeed
        address sequencerUptimeFeed;
    }

    /// @notice The asset configuration
    struct AssetConfig {
        /// @notice True if the price feed answer should be inverted
        bool shouldInvert;
        /// @notice The asset's chainlink pricefeed
        AggregatorV3Interface chainlinkPriceFeed;
        /// @notice The assets' fallback price if a Chainlink price feed does not exist.
        /// @dev This should only be used if the conversion between the
        /// asset pair is constant
        uint256 stableFallbackPrice;
        /// @notice The configuration to swap between assets
        SwapConfig swapConfig;
        /// @notice Params that can be used to derive the
        /// value of an asset
        /// @dev This can be used if a Chainlink oracle does not exist
        /// for the asset AND the asset is not a stable coin.  An
        /// example of this would be to value GUSD
        /// @dev This assumes that the function called returns 1 DP
        AssetContractOracleParams assetContractOracleParams;
    }

    /// @notice The parameters use to withdraw from a Teahouse vault
    /// and swap it's counter assets
    struct WithdrawAndSwapParams {
        /// @notice The amount of base assets to withdraw
        uint256 withdrawAmount;
        /// @notice The maximum percentage deviation between the reported
        /// spot and twap prices
        uint256 priceThreshold;
        /// @notice The target address to withdraw from
        address withdrawTarget;
        /// @notice Arbitrary swap data
        bytes swapData;
        /// @notice The liquidity warehouse's base asset
        address baseAsset;
        /// @notice The address of the sequencer uptime feed
        address sequencerUptimeFeed;
    }

    /// @notice This event is emitted when the swap path is set
    /// @param assetIn The asset being swapped from
    /// @param assetOut The asset being swapped to
    /// @param chainlinkPriceFeed The chainlink price feed used to compare the Uniswap
    /// price
    /// @param stableFallbackPrice The stable fallback price used
    /// @param path The path to swap the assets
    /// @param feeBps The pool fee in BPS
    event AssetConfigSet(
        address indexed assetIn,
        address indexed assetOut,
        address chainlinkPriceFeed,
        uint256 stableFallbackPrice,
        bytes path,
        uint256 feeBps
    );

    /// @notice This error is thrown whenever trying to swap an asset without the
    /// swap path being set
    /// @param assetIn The asset being swapped from
    /// @param assetOut The asset being swapped to
    error SwapPathNotSet(address assetIn, address assetOut);

    /// @notice This error is thrown whenever the chainlink price feed is stale
    error ChainlinkPriceFeedStale();

    /// @notice This error is thrown whenever the Sequencer is down
    error SequencerDown();

    /// @notice This error is thrown when the grace period after a sequencer
    /// is up is not over
    error GracePeriodNotOver();

    /// @notice This error is thrown whenever an invalid withdraw target is called
    /// @param withdrawTarget The invalid withdraw target address
    error InvalidWithdrawTarget(address withdrawTarget);

    /// @notice This error is thrown whenver the sqrt price reported using twaps and
    /// spot price have a large discrepancy
    /// @param oraclePrice The price reported by the oracle
    /// @param spotPrice The price reported by the Uniswap pool
    /// @param percentDif The percentage difference between the twap and spot sqrt price
    error PriceDiscrepancy(uint256 oraclePrice, uint256 spotPrice, uint256 percentDif);

    /// @notice This error is thrown whenever the asset oracle is not configured correctly
    error InvalidAssetOracle();

    /// @notice This error is thrown whenever the contract fails to determine the value
    /// of an asset
    error FailedToReadAssetValue();

    /// @notice Set the asset configs
    /// @param assetConfigs Array of asset configs to set
    /// @param storedAssetConfigs The asset config storage
    function setAssetConfigs(
        AssetConfigArgs[] memory assetConfigs,
        mapping(address assetIn => mapping(address assetOut => TeahouseHelperLibrary.AssetConfig assetConfig)) storage
            storedAssetConfigs
    ) public {
        for (uint256 i; i < assetConfigs.length; ++i) {
            AssetConfigArgs memory assetConfigArgs = assetConfigs[i];
            storedAssetConfigs[assetConfigArgs.assetIn][assetConfigArgs.assetOut] = TeahouseHelperLibrary.AssetConfig({
                chainlinkPriceFeed: AggregatorV3Interface(assetConfigArgs.chainlinkPriceFeed),
                stableFallbackPrice: assetConfigArgs.stableFallbackPrice,
                swapConfig: TeahouseHelperLibrary.SwapConfig({
                    swapPath: assetConfigArgs.swapPath,
                    feeBps: assetConfigArgs.feeBps,
                    swapRouter: assetConfigArgs.swapRouter
                }),
                shouldInvert: assetConfigArgs.shouldInvert,
                assetContractOracleParams: assetConfigArgs.assetContractOracleParams
            });
            emit AssetConfigSet(
                assetConfigArgs.assetIn,
                assetConfigArgs.assetOut,
                assetConfigArgs.chainlinkPriceFeed,
                assetConfigArgs.stableFallbackPrice,
                assetConfigArgs.swapPath,
                assetConfigArgs.feeBps
            );
        }
    }

    /// @notice Withdraws from a teahouse vault and swaps the counter asset to the base asset
    /// @param params The withdrawal params
    /// @param storedAssetConfigs The asset config storage
    /// @return bool True if the full withdraw amount was withdrawn from the Teahouse vault
    function withdrawAndSwap(
        WithdrawAndSwapParams memory params,
        mapping(address assetIn => mapping(address assetOut => TeahouseHelperLibrary.AssetConfig assetConfig)) storage
            storedAssetConfigs
    ) public returns (bool) {
        (uint256 deadline, uint256 amountOutMinimum, uint256 teahouseAmount0OutMin, uint256 teahouseAmount1OutMin) =
            abi.decode(params.swapData, (uint256, uint256, uint256, uint256));

        address counterAsset = params.baseAsset == ITeahouseVaultV3PairWithPool(params.withdrawTarget).assetToken0()
            ? ITeahouseVaultV3PairWithPool(params.withdrawTarget).assetToken1()
            : ITeahouseVaultV3PairWithPool(params.withdrawTarget).assetToken0();

        uint256 sharesAmt = IERC20(params.withdrawTarget).balanceOf(address(this));
        TeahouseHelperLibrary.AssetConfig storage assetConfig = storedAssetConfigs[counterAsset][params.baseAsset];

        uint256 withdrawAmtShares;
        if (params.withdrawAmount < type(uint256).max) {
            params.withdrawAmount = params.withdrawAmount * assetConfig.swapConfig.feeBps / BPS;

            withdrawAmtShares = convertToTeahouseShares(
                params.withdrawTarget,
                params.withdrawAmount,
                params.baseAsset,
                params.sequencerUptimeFeed,
                params.priceThreshold,
                assetConfig
            );
            // Calculate number of shares to burn which is the minimum of the amount of shares required to withdraw
            // the withdraw amount and the LP balance of the liquidity warehouse
            sharesAmt = Math.min(withdrawAmtShares, sharesAmt);
        } else {
            // This is placed in the else block as _validatePoolPrice is
            // already called in convertToTeahouseShares in the above if block
            _validatePoolPrice(
                ITeahouseVaultV3PairWithPool(params.withdrawTarget).pool(),
                storedAssetConfigs[counterAsset][params.baseAsset],
                params.priceThreshold,
                AggregatorV3Interface(params.sequencerUptimeFeed)
            );
        }

        if (sharesAmt == 0) return false;

        ITeahouseVaultV3PairWithPool(params.withdrawTarget).withdraw(
            sharesAmt, teahouseAmount0OutMin, teahouseAmount1OutMin
        );

        bytes memory swapPath = assetConfig.swapConfig.swapPath;
        if (bytes32(swapPath) == bytes32("")) revert SwapPathNotSet(params.baseAsset, counterAsset);

        uint256 counterAssetSwapAmt = IERC20(counterAsset).balanceOf(address(this));
        if (counterAssetSwapAmt > 0) {
            address swapRouter = assetConfig.swapConfig.swapRouter;
            ISwapRouter(swapRouter).exactInput(
                ISwapRouter.ExactInputParams({
                    path: swapPath,
                    recipient: address(this),
                    deadline: block.timestamp + deadline,
                    amountIn: counterAssetSwapAmt,
                    amountOutMinimum: amountOutMinimum
                })
            );
        }

        return sharesAmt == withdrawAmtShares;
    }

    /// @notice Swaps all the counter asset balances to the liquidity warehouse's base asset
    /// @param data Arbitrary data that can be used to swap
    /// @param baseAsset The liquidity warehouse's base asset
    /// @param withdrawTargets The liquidity warehouse's withdraw targets
    /// @param params Storage params
    /// @param storedAssetConfigs The asset config storage
    function swap(
        bytes memory data,
        address baseAsset,
        EnumerableSet.AddressSet storage withdrawTargets,
        StorageParams memory params,
        mapping(address assetIn => mapping(address assetOut => TeahouseHelperLibrary.AssetConfig assetConfig)) storage
            storedAssetConfigs
    ) public {
        (uint256[] memory deadlines, uint256[] memory amountOutMinimums) = abi.decode(data, (uint256[], uint256[]));

        for (uint256 i; i < withdrawTargets.length(); ++i) {
            ITeahouseVaultV3PairWithPool teahouseVault = ITeahouseVaultV3PairWithPool(withdrawTargets.at(i));

            address counterAsset =
                baseAsset == teahouseVault.assetToken0() ? teahouseVault.assetToken1() : teahouseVault.assetToken0();

            _validatePoolPrice(
                teahouseVault.pool(),
                storedAssetConfigs[counterAsset][baseAsset],
                params.priceThreshold,
                AggregatorV3Interface(params.sequencerUptimeFeed)
            );

            bytes memory swapPath = storedAssetConfigs[counterAsset][baseAsset].swapConfig.swapPath;
            if (bytes32(swapPath) == bytes32("")) {
                revert TeahouseHelperLibrary.SwapPathNotSet(baseAsset, counterAsset);
            }

            uint256 swapAmount = IERC20(counterAsset).balanceOf(address(this));
            if (swapAmount > 0) {
                address swapRouter = storedAssetConfigs[counterAsset][baseAsset].swapConfig.swapRouter;
                ISwapRouter(swapRouter).exactInput(
                    ISwapRouter.ExactInputParams({
                        path: swapPath,
                        recipient: address(this),
                        deadline: block.timestamp + deadlines[i],
                        amountIn: swapAmount,
                        amountOutMinimum: amountOutMinimums[i]
                    })
                );
            }
        }
    }

    /// @notice Calculates the value of the liquidity warehouse's shares in the liquidity
    /// warehouse's base asset
    /// @param teahouseVaultAddr The address of the teahouse vault
    /// @param baseAsset The address of the liquidity warehouse's base asset
    /// @param params Storage params
    /// @param storedAssetConfigs The asset config storage
    function convertToTeahouseAssets(
        address teahouseVaultAddr,
        address baseAsset,
        StorageParams memory params,
        mapping(address assetIn => mapping(address assetOut => TeahouseHelperLibrary.AssetConfig assetConfig)) storage
            storedAssetConfigs
    ) public view returns (uint256) {
        ITeahouseVaultV3PairWithPool teahouseVault = ITeahouseVaultV3PairWithPool(teahouseVaultAddr);
        address counterAsset =
            baseAsset == teahouseVault.assetToken0() ? teahouseVault.assetToken1() : teahouseVault.assetToken0();
        _validatePoolPrice(
            teahouseVault.pool(),
            storedAssetConfigs[counterAsset][baseAsset],
            params.priceThreshold,
            AggregatorV3Interface(params.sequencerUptimeFeed)
        );
        uint256 baseAssetValue = _getBaseAssetValue(teahouseVault, baseAsset);
        return IERC20(teahouseVaultAddr).balanceOf(address(this)) * baseAssetValue
            / IERC20(teahouseVaultAddr).totalSupply();
    }

    /// @notice Calculates the amount of shares representing a given asset amount in a
    /// teahouse vault
    /// @param teahouseVaultAddr The address of the teahouse vault
    /// @param assetAmt The amount of assets to convert
    /// @param baseAsset The address of the liquidity warehouse's base asset
    /// @param priceThreshold The price threshold between the reported twap and spot square root price values
    /// @param assetConfig  The asset's configuration
    function convertToTeahouseShares(
        address teahouseVaultAddr,
        uint256 assetAmt,
        address baseAsset,
        address sequencerUptimeFeed,
        uint256 priceThreshold,
        AssetConfig storage assetConfig
    ) public view returns (uint256) {
        ITeahouseVaultV3PairWithPool teahouseVault = ITeahouseVaultV3PairWithPool(teahouseVaultAddr);
        _validatePoolPrice(
            teahouseVault.pool(), assetConfig, priceThreshold, AggregatorV3Interface(sequencerUptimeFeed)
        );
        uint256 baseAssetValue = _getBaseAssetValue(teahouseVault, baseAsset);
        return assetAmt * IERC20(teahouseVaultAddr).totalSupply() / baseAssetValue;
    }

    /// @notice Returns the total base asset value in the Teahouse vault
    /// @param teahouseVault The teahouse vault
    /// @param baseAsset The address of the liquidity warehouse's base asset
    /// @return uint256 The total value of the teahouse vault denominated in the base asset
    function _getBaseAssetValue(ITeaVaultV3Pair teahouseVault, address baseAsset) internal view returns (uint256) {
        if (baseAsset == teahouseVault.assetToken0()) {
            return teahouseVault.estimatedValueInToken0();
        }
        return teahouseVault.estimatedValueInToken1();
    }

    /// @notice Validates that the price of an oracle has not been manipulated
    /// @param pool The Uniswap pool to fetch the price from
    /// @param assetConfig The asset's configuration
    /// @param priceThreshold The maximum percentage deviation between the spot and twap prices
    /// @param sequencerUptimeFeed The sequencer uptime feed
    function _validatePoolPrice(
        IUniswapV3Pool pool,
        AssetConfig memory assetConfig,
        uint256 priceThreshold,
        AggregatorV3Interface sequencerUptimeFeed
    ) private view {
        uint256 spotPrice = _getSpotPrice(pool);
        uint256 anchorPrice = _getAnchorPrice(assetConfig, sequencerUptimeFeed);
        uint256 absPriceDif = anchorPrice < spotPrice ? spotPrice - anchorPrice : anchorPrice - spotPrice;

        uint256 percentPriceDif = absPriceDif * WadRayMath.WAD / anchorPrice;
        if (percentPriceDif > priceThreshold) {
            revert PriceDiscrepancy(anchorPrice, spotPrice, percentPriceDif);
        }
    }

    function _getSpotPrice(IUniswapV3Pool pool) private view returns (uint256) {
        (uint160 sqrtPriceX96,,,,,,) = pool.slot0();
        uint256 spotPrice = FullMath.mulDiv(
            10 ** (18 + IERC20Metadata(pool.token0()).decimals()),
            FullMath.mulDiv(sqrtPriceX96, sqrtPriceX96, FixedPoint96.Q96),
            FixedPoint96.Q96
        );
        return spotPrice / 10 ** IERC20Metadata(pool.token1()).decimals();
    }

    function _getAnchorPrice(AssetConfig memory assetConfig, AggregatorV3Interface sequencerUptimeFeed)
        private
        view
        returns (uint256)
    {
        if (assetConfig.assetContractOracleParams.assetContract != address(0)) {
            (bool success, bytes memory data) = assetConfig.assetContractOracleParams.assetContract.staticcall(
                assetConfig.assetContractOracleParams.assetOracleFnCall
            );
            if (!success) revert FailedToReadAssetValue();

            (uint256 rate) = abi.decode(data, (uint256));

            /// @dev Scale to 18 DP
            uint256 assetOraclePrice = rate * 10 ** (18 - assetConfig.assetContractOracleParams.decimals);
            if (assetConfig.shouldInvert) {
                return WadRayMath.WAD * WadRayMath.WAD / assetOraclePrice;
            }
            return assetOraclePrice;
        }
        return _getOraclePrice(assetConfig, sequencerUptimeFeed);
    }

    function _getOraclePrice(AssetConfig memory assetConfig, AggregatorV3Interface sequencerUptimeFeed)
        private
        view
        returns (uint256)
    {
        if (address(assetConfig.chainlinkPriceFeed) == address(0)) return assetConfig.stableFallbackPrice;
        (, int256 oraclePrice,, uint256 updatedAt,) = assetConfig.chainlinkPriceFeed.latestRoundData();
        if (block.timestamp - updatedAt > STALENESS_THRESHOLD) revert ChainlinkPriceFeedStale();

        (, int256 answer, uint256 startedAt,,) = sequencerUptimeFeed.latestRoundData();

        if (answer == 1) {
            revert SequencerDown();
        }
        if (block.timestamp - startedAt <= GRACE_PERIOD_TIME) {
            revert GracePeriodNotOver();
        }

        uint256 scaledOraclePrice = oraclePrice.toUint256() * 10 ** (18 - assetConfig.chainlinkPriceFeed.decimals());
        if (assetConfig.shouldInvert) {
            scaledOraclePrice = WadRayMath.WAD * WadRayMath.WAD / scaledOraclePrice;
        }
        return scaledOraclePrice;
    }
}

