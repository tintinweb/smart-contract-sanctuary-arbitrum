interface IPriceEstimator {
    function setOracleParams(
        address _dataFeed, 
        bool _useOracle
    ) external;

    function getEstimatedETHforERC20(
        uint256 erc20Amount,
        address token
    ) external view returns (uint256[] memory);

    function getEstimatedERC20forETH(
        uint256 etherAmountInWei,
        address tokenAddress
    ) external view returns (uint256[] memory);

