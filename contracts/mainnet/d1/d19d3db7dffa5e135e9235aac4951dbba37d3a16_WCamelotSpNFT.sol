/**
 * @dev for book keeping manual harvest yield where the LP may still auto-compound
 */
contract HybridYieldBookKeepingModule is Initializable {
    using EnumerableSet for EnumerableSet.AddressSet;

    struct PoolInfo {
        uint lpGrowthFactor;
        EnumerableSet.AddressSet manualRewardTokens;
        mapping(address => uint256) accRewardPerShare;
        mapping(uint256 => mapping(address => uint256)) rewardDebt;
    }

    mapping(address => mapping(address => uint256)) private _rewardBalance;
    mapping(uint256 => PoolInfo) private _poolInfo;

    /********************************************************/
    /****************** INTERNAL FUNCTIONS ******************/
    /********************************************************/

    /**
     * @param poolId poolId in CamelotMaster
     * @param posId SingelBank posId
     * @param originalLpAmt original LP balance of the position,
     * 0 if the position is newly created
     * @param toAddLpAmt LP amount to add to the position
     */
    function _depositLp(
        uint256 poolId,
        uint256 posId,
        uint256 originalLpAmt,
        uint256 toAddLpAmt,
        address user
    ) internal {
        uint256 totalLpAmt = originalLpAmt + toAddLpAmt;

        PoolInfo storage poolInfo = _poolInfo[poolId];
        uint256 length = poolInfo.manualRewardTokens.length();
        uint lpGrowthFactor = poolInfo.lpGrowthFactor;
        require(
            lpGrowthFactor != 0,
            "lpGrowthFactor is 0, try `_updateLpGrowthFactor(pid, 0)`"
        );

        for (uint256 index = 0; index < length; ++index) {
            address rewardToken = poolInfo.manualRewardTokens.at(index);

            uint pendingReward = pendingRewardToken(
                poolId,
                posId,
                rewardToken,
                originalLpAmt
            );
            _rewardBalance[rewardToken][user] += pendingReward;

            uint accRewardPerShare = poolInfo.accRewardPerShare[rewardToken];
            poolInfo.rewardDebt[posId][rewardToken] =
                (accRewardPerShare * ((totalLpAmt * 1e18) / lpGrowthFactor)) /
                1e18;
        }
    }

    /**
     * @param poolId poolId in CamelotMaster
     * @param posId SingelBank posId
     * @param originalLpAmt original LP balance of the position
     */
    function _withdrawLp(
        uint256 poolId,
        uint256 posId,
        uint256 originalLpAmt,
        uint256 toWithdrawLpAmt,
        address user
    ) internal {
        uint newLpAmt = originalLpAmt - toWithdrawLpAmt;

        PoolInfo storage poolInfo = _poolInfo[poolId];
        uint256 length = poolInfo.manualRewardTokens.length();

        for (uint256 index = 0; index < length; ++index) {
            address rewardToken = poolInfo.manualRewardTokens.at(index);
            uint accRewardPerShare = poolInfo.accRewardPerShare[rewardToken];
            uint lpGrowthFactor = poolInfo.lpGrowthFactor;

            uint pendingReward = pendingRewardToken(
                poolId,
                posId,
                rewardToken,
                originalLpAmt
            );

            _rewardBalance[rewardToken][user] += pendingReward;

            poolInfo.rewardDebt[posId][rewardToken] =
                (accRewardPerShare * ((newLpAmt * 1e18) / lpGrowthFactor)) /
                1e18;
        }
    }

    function _harvest(
        uint256 poolId,
        uint256 posId,
        address rewardToken,
        uint userLpAmount,
        address user
    ) internal {
        PoolInfo storage poolInfo = _poolInfo[poolId];
        uint accRewardPerShare = poolInfo.accRewardPerShare[rewardToken];
        uint lpGrowthFactor = poolInfo.lpGrowthFactor;

        uint pendingReward = pendingRewardToken(
            poolId,
            posId,
            rewardToken,
            userLpAmount
        );

        _rewardBalance[rewardToken][user] += pendingReward;
        poolInfo.rewardDebt[posId][rewardToken] =
            (accRewardPerShare * ((userLpAmount * 1e18) / lpGrowthFactor)) /
            1e18;
    }

    /**
     * @param poolId poolId in CamelotMaster
     * @param rewardToken rewardToken e.g. xGRAIL address
     * @param lpSupply total LP supply
     * @param amount amount of reward token to add
     */
    function _addYieldToPool(
        uint256 poolId,
        address rewardToken,
        uint lpSupply,
        uint amount
    ) internal validateRewardToken(poolId, rewardToken) {
        require(lpSupply != 0, "lpSupply is 0");
        PoolInfo storage poolInfo = _poolInfo[poolId];
        uint256 accRewardPerShare = poolInfo.accRewardPerShare[rewardToken];

        uint lpGrowthFactor = poolInfo.lpGrowthFactor;
        uint lpSupplyNormalizedWithGrowthFactor = (lpSupply * 1e18) /
            lpGrowthFactor;
        accRewardPerShare +=
            (amount * 1e18) /
            lpSupplyNormalizedWithGrowthFactor;
        poolInfo.accRewardPerShare[rewardToken] = accRewardPerShare;
    }

    function _addManualRewardToken(
        uint256 poolId,
        address rewardToken
    ) internal {
        PoolInfo storage poolInfo = _poolInfo[poolId];
        poolInfo.manualRewardTokens.add(rewardToken);
    }

    function _removeManualRewardToken(
        uint256 poolId,
        address rewardToken
    ) internal {
        PoolInfo storage poolInfo = _poolInfo[poolId];
        poolInfo.manualRewardTokens.remove(rewardToken);
    }

    /**
     * @param poolId poolId in CamelotMaster
     * @param addedLpRatio formula: added_LP_from_auto-compound * 1e18 / original LP amount
     */
    function _updateLpGrowthFactor(uint256 poolId, uint addedLpRatio) internal {
        PoolInfo storage poolInfo = _poolInfo[poolId];
        uint lpGrowthFactor = poolInfo.lpGrowthFactor;

        if (lpGrowthFactor == 0) {
            poolInfo.lpGrowthFactor = 1e18 + addedLpRatio;
            return;
        }

        poolInfo.lpGrowthFactor =
            (lpGrowthFactor * (1e18 + addedLpRatio)) /
            1e18;
    }

    /**************************************************/
    /****************** PUBLIC VIEWS ******************/
    /**************************************************/

    function getAccRewardPerShare(
        uint256 poolId,
        address rewardToken
    ) public view returns (uint) {
        return _poolInfo[poolId].accRewardPerShare[rewardToken];
    }

    function getLpGrowthFactor(uint256 poolId) public view returns (uint) {
        return _poolInfo[poolId].lpGrowthFactor;
    }

    function getManualRewardTokensLength(
        uint256 poolId
    ) external view returns (uint length) {
        return _poolInfo[poolId].manualRewardTokens.length();
    }

    function manualRewardTokensAtIndex(
        uint256 poolId,
        uint256 index
    ) external view returns (address rewardToken) {
        return _poolInfo[poolId].manualRewardTokens.at(index);
    }

    function posRewardDebt(
        uint256 poolId,
        uint256 posId,
        address rewardToken
    ) public view returns (uint) {
        return _poolInfo[poolId].rewardDebt[posId][rewardToken];
    }

    function pendingRewardToken(
        uint256 poolId,
        uint256 posId,
        address rewardToken,
        uint userLpAmount
    ) public view returns (uint) {
        PoolInfo storage poolInfo = _poolInfo[poolId];
        uint accRewardPerShare = poolInfo.accRewardPerShare[rewardToken];
        uint lpGrowthFactor = poolInfo.lpGrowthFactor;
        uint rewardDebt = poolInfo.rewardDebt[posId][rewardToken];

        uint accu = (accRewardPerShare *
            ((userLpAmount * 1e18) / lpGrowthFactor)) / 1e18;
        if (accu < rewardDebt) {
            revert Underflow(accu, rewardDebt);
        }
        return accu - rewardDebt;
    }

    function rewardBalance(
        address rewardToken,
        address user
    ) public view returns (uint) {
        return _rewardBalance[rewardToken][user];
    }

    /***********************************************/
    /****************** MODIFIERS ******************/
    /***********************************************/

    modifier validateRewardToken(uint256 poolId, address rewardToken) {
        require(
            _poolInfo[poolId].manualRewardTokens.contains(rewardToken),
            "no such manual reward token"
        );
        _;
    }
}






