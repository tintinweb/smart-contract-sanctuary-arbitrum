    function _getInfoByPid(
        uint pid
    )
        internal
        view
        returns (address lpToken, INFTPool spNFT, uint spNftTokenId)
    {
        spNFT = INFTPool(camelotMaster.getPoolAddressByIndex(pid));
        (lpToken, , , , , , , ) = spNFT.getPoolInfo();
        return (lpToken, spNFT, _getSpNftTokenId(spNFT, pid));
    }

    /**
     * @dev Returns the token ID associated with `spNFT`. Returns 0 if no
     * token ID is associated. Currently only support 1 spNFT per pool.
     */
    function _getSpNftTokenId(
        INFTPool spNFT,
        uint pid
    ) internal view returns (uint256) {
        if (stakedNitroPool[pid] != address(0)) {
            uint length = INitroPool(stakedNitroPool[pid]).userTokenIdsLength(
                address(this)
            );
            require(
                length == 1,
                "currently only support 1 spNFT per NitroPool"
            );
            return
                INitroPool(stakedNitroPool[pid]).userTokenId(address(this), 0);
        }
        uint balOf = spNFT.balanceOf(address(this));
        if (balOf == 0) {
            return 0;
        }
        if (balOf == 1) {
            return
                IERC721EnumerableUpgradeable(address(spNFT)).tokenOfOwnerByIndex(
                    address(this),
                    0
                );
        }
        revert("currently only support 1 spNFT per pool");
    }

    function _recordAutoCompoundRewards(
        uint256 pid,
        address _rewardToken
    ) internal returns (uint newReward) {
        AutoCompoundInfo storage acReward = autoCompoundRewards[_rewardToken];
        newReward = _rewardToken.myBalance().sub(acReward.total);
        if (newReward > 0) {
            acReward.poolRewards[pid] = acReward.poolRewards[pid].add(
                newReward
            );
            acReward.total = acReward.total.add(newReward);
            emit HarvestMultiyield(pid, address(_rewardToken), newReward);
        }
    }

    function _recordXGrail(
        uint256 pid,
        INFTPool spNFT,
        uint256 spNftTokenId,
        uint totalLpSupply,
        uint oldXGrailBal
    ) internal returns (uint rewardBalance) {
        uint xGrailBal = address(xGRAIL).myBalance();
        if (xGrailBal < oldXGrailBal)
            revert Underflow(xGrailBal, oldXGrailBal);
        rewardBalance = xGrailBal - oldXGrailBal;
        if (rewardBalance > 0) {
            _addYieldToPool(pid, address(xGRAIL), totalLpSupply, rewardBalance);
            emit HarvestMultiyield(pid, address(xGRAIL), rewardBalance);
        }
    }

    /**
     * @notice save some gas by approve max allowance once,
     * make sure the spender is trusted
     */
    function _makeSureXGrailUsageIsApproved(
        uint amt,
        IXGrailTokenUsage usageAddress
    ) internal {
        if (
            xGRAIL.getUsageApproval(address(this), address(usageAddress)) >= amt
        ) {
            return;
        }
        xGRAIL.approveUsage(usageAddress, type(uint256).max);
    }

    /// @dev harvest reward token to worker.
    /// @param pid pool id
    function _harvestReward(uint256 pid) internal {
        uint xGrailBalSnapshot = address(xGRAIL).myBalance();
        (, INFTPool spNFT, uint spNftTokenId) = _getInfoByPid(pid);

        spNFT.harvestPosition(spNftTokenId);
        (uint lpTotal, , , , , , , ) = spNFT.getStakingPosition(spNftTokenId);

        _recordAutoCompoundRewards(pid, GRAIL);
        for (
            uint256 index = 0;
            index < autoCompoundRewardTokens[pid].length();
            ++index
        ) {
            _recordAutoCompoundRewards(
                pid,
                autoCompoundRewardTokens[pid].at(index)
            );
        }
        _recordXGrail(
            pid,
            spNFT,
            spNftTokenId,
            lpTotal,
            xGrailBalSnapshot
        );

        uint toAllocate = address(xGRAIL).myBalance() - xGrailBalSnapshot;

        if (toAllocate > 0) {
            address yieldBooster = camelotMaster.yieldBooster();
            _makeSureXGrailUsageIsApproved(
                toAllocate,
                IXGrailTokenUsage(yieldBooster)
            );
            xGRAIL.allocate(
                yieldBooster,
                toAllocate,
                abi.encode(address(spNFT), spNftTokenId)
            );
        }
    }

