    function initializePool(uint poolId) external onlyOwner {
        _updateLpGrowthFactor(poolId, 0); // initialize lpGrowthFactor to 1e18
        _addManualRewardToken(poolId, address(xGRAIL)); // assume xGRAIL always exists as a manual harvest reward token
        autoCompoundRewardTokens[poolId].add(GRAIL);
    }

    function setDexHelper(IDynamicDexHelper helper) external onlyOwner {
        dexHelper = helper;
    }

    function setDedicatedWorkers(
        address[] calldata workers,
        bool ok
    ) public onlyOwner {
        for (uint8 i = 0; i < workers.length; i++) {
            dedicatedWorkers[workers[i]] = ok;
            emit SetWorkers(workers[i], ok);
        }
    }

    function stakeNFTToNitroPool(
        uint poolId,
        INitroPool nitroPool
    ) external onlyOwner {
        (, INFTPool spNFT, uint spNftTokenId) = _getInfoByPid(poolId);
        require(
            address(nitroPool.nftPool()) == address(spNFT),
            "wrong nitro pool"
        );
        (, uint endTime, , uint depositEndTime, , , , , ) = nitroPool
            .settings();
        require(
            depositEndTime == 0 || depositEndTime > block.timestamp,
            "nitro pool deposit ended"
        );
        require(endTime > block.timestamp, "nitro pool ended");
        spNFT.safeTransferFrom(address(this), address(nitroPool), spNftTokenId);
        stakedNitroPool[poolId] = address(nitroPool);
        // @TODO: emit event
    }

    function unstakeNFTFromNitroPool(uint poolId) external onlyOwner {
        address nitroPool = stakedNitroPool[poolId];
        require(address(nitroPool) != address(0), "no nitro pool");
        (, , uint spNftTokenId) = _getInfoByPid(poolId);

        INitroPool(nitroPool).withdraw(spNftTokenId);
        stakedNitroPool[poolId] = address(0);
        // @TODO: emit event
    }

