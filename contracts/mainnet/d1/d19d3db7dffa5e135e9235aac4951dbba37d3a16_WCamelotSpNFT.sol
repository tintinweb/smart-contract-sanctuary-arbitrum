    /// @dev Decode ERC1155 token id to pid
    /// @param id Token id
    /// @return pid
    function decodeId(uint id) external pure override returns (uint pid) {
        (pid, ) = CollIdLibrary.decodeId(id);
    }

    function collSizeToLpBalance(
        uint256 collId,
        uint256 collSize
    ) public view virtual override returns (uint256 balance) {
        if (collSize == 0) return 0;
        (uint pid, ) = CollIdLibrary.decodeId(collId);
        (, INFTPool spNFT, uint spNftTokenId) = _getInfoByPid(pid);
        (uint lpTotal, , , , , , , ) = spNFT.getStakingPosition(spNftTokenId);
        balance = totalSupplyByPid[pid] > 0
            ? collSize.mul(lpTotal).div(totalSupplyByPid[pid])
            : collSize;
    }

    // FIXME change totalSupply to totalCollSize of the PID
    function lpBalanceToCollSize(
        uint256 collId,
        uint256 balance
    ) public view virtual returns (uint256 collSize) {
        (uint pid, ) = CollIdLibrary.decodeId(collId);
        (, INFTPool spNFT, uint spNftTokenId) = _getInfoByPid(pid);
        (uint lpTotal, , , , , , , ) = spNFT.getStakingPosition(spNftTokenId);
        collSize = totalSupplyByPid[pid] == 0
            ? balance
            : balance.mul(totalSupplyByPid[pid]).div(lpTotal);
    }
    
