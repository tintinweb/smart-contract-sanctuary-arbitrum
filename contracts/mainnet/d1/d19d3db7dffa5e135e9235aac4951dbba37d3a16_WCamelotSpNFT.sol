library CollIdLibrary {
    function encodeId(uint pid, uint posId) internal pure returns (uint id) {
        require(pid < (1 << 16), "bad pid");
        require(posId < (1 << 240), "bad posId");
        id = (pid << 240) | posId;
    }

    function decodeId(uint id) internal pure returns (uint pid, uint posId) {
        pid = id >> 240; // First 16 bits
        posId = id & ((1 << 240) - 1); // Last 240 bits
    }
}