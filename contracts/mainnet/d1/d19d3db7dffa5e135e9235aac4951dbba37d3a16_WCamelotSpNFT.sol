    event Stake(uint256 indexed pid, uint256 lpAmount);
    event SetWorkers(address workers, bool ok);
    event Mint(uint256 indexed pid, uint256 collAmount, uint256 lpAmount);
    event Burn(uint256 indexed pid, uint256 collAmount, uint256 lpAmount);

    /****************************************************************/
    /****************** EXTERNAL PUBLIC FUNCTIONS  ******************/
    /****************************************************************/

    /// @dev Mint ERC1155 token for the given pool id.
    /// @param id CollId
    /// @param amount Token amount to wrap
    /// @return The token id that got minted.
    function mint(
        uint id,
        uint amount,
        address user
    )
        external
        virtual
        override
        onlyDedicatedWorker
        nonReentrant
        returns (uint)
    {
        uint xGrailBalSnapshot = address(xGRAIL).myBalance();
        (uint pid, uint posId) = CollIdLibrary.decodeId(id);
        (address lpToken, INFTPool spNFT, uint spNftTokenId) = _getInfoByPid(
            pid
        );

        lpToken.safeTransferFrom(msg.sender, address(this), amount);
        IERC20Upgradeable(lpToken).safeApprove(address(spNFT), amount);

        uint originalLpAmount = collSizeToLpBalance(id, totalSupply(id));
        uint size = lpBalanceToCollSize(id, amount);

        _depositLp(pid, posId, originalLpAmount, amount, user); // manual harvest tokens yield bookkeeping/ harvesting
        totalSupplyByPid[pid] += size;
        _mint(msg.sender, id, size, "");

        if (spNftTokenId == 0 /* no spNFT associated */) {
            spNFT.createPosition(amount, 0);
        } else {
            spNFT.addToPosition(spNftTokenId, amount);
        }

        (uint lpTotal, , , , , , , ) = spNFT.getStakingPosition(spNftTokenId);
        _recordAutoCompoundRewards(pid, GRAIL);

        for (
            uint256 index = 0;
            index < autoCompoundRewardTokens[pid].length();
            ++index
        ) {
            _recordAutoCompoundRewards(
                pid,
                autoCompoundRewardTokens[pid].at(index)
            );
        }
        _recordXGrail(
            pid,
            spNFT,
            spNftTokenId,
            lpTotal,
            xGrailBalSnapshot
        );

        uint toAllocate = address(xGRAIL).myBalance() - xGrailBalSnapshot;

        if (toAllocate > 0) {
            address yieldBooster = camelotMaster.yieldBooster();
            _makeSureXGrailUsageIsApproved(
                toAllocate,
                IXGrailTokenUsage(yieldBooster)
            );
            xGRAIL.allocate(
                yieldBooster,
                toAllocate,
                abi.encode(address(spNFT), spNftTokenId)
            );
        }

        emit Mint(pid, size, amount);
        return id;
    }

    /// @dev Burn ERC1155 token to redeem LP ERC20 token back.
    /// @param id Token id
    /// @param amount Token amount to burn
    /// @return The pool id that that you will receive LP token back.
    function burn(
        uint id,
        uint amount,
        address user
    )
        external
        virtual
        override
        // uint posId
        onlyDedicatedWorker
        nonReentrant
        returns (uint)
    {
        if (amount == type(uint256).max) {
            amount = balanceOf(msg.sender, id);
        }

        uint originalLpAmt = collSizeToLpBalance(id, totalSupply(id));

        (uint pid, uint posId) = CollIdLibrary.decodeId(id);
        (, INFTPool spNFT, uint spNftTokenId) = _getInfoByPid(pid);

        uint balance = collSizeToLpBalance(id, amount);
        totalSupplyByPid[pid] = amount > totalSupplyByPid[pid]
            ? 0
            : totalSupplyByPid[pid].sub(amount);

        (uint lpTotal, , , , , , , ) = spNFT.getStakingPosition(spNftTokenId);
        _burn(msg.sender, id, amount);
        {
            uint xGrailBalSnapshot = address(xGRAIL).myBalance();
            uint deductAmountFromYieldBooster = lpTotal ==
                balance /* NFT is burnt */
                ? xGRAIL.getUsageAllocation(
                    address(this),
                    camelotMaster.yieldBooster()
                ) * (10000 - xGRAIL.usagesDeallocationFee(camelotMaster.yieldBooster()))
                / 10000
                : 0;
            address nitroPool = stakedNitroPool[pid];

            // cannot withdraw if staked to NitroPool
            if (nitroPool != address(0)) {
                INitroPool(nitroPool).withdraw(spNftTokenId);
            }
            spNFT.withdrawFromPosition(spNftTokenId, balance);
            if (
                nitroPool != address(0) &&
                (lpTotal - balance) > 0 /* NFT not burnt */
            ) {
                spNFT.safeTransferFrom(
                    address(this),
                    address(nitroPool),
                    spNftTokenId
                );
            }

            _recordAutoCompoundRewards(pid, GRAIL);
            for (
                uint256 index = 0;
                index < autoCompoundRewardTokens[pid].length();
                ++index
            ) {
                _recordAutoCompoundRewards(
                    pid,
                    autoCompoundRewardTokens[pid].at(index)
                );
            }

            _recordXGrail(
                pid,
                spNFT,
                spNftTokenId,
                lpTotal,
                xGrailBalSnapshot + deductAmountFromYieldBooster
            );
        }

        uint _xGrailRewardBal = rewardBalance(address(xGRAIL), user);
        _withdrawLp(pid, posId, originalLpAmt, balance, user);
        uint xGrailRewardBal = rewardBalance(address(xGRAIL), user) -
            _xGrailRewardBal;

        reservedXGrail += xGrailRewardBal;
        uint _xGrailBal = address(xGRAIL).myBalance();

        if (
            reservedXGrail > _xGrailBal &&
            lpTotal !=
            balance /* withdraw all LP, NFT will be burnt, all xGRAIL will be deallocated already */
        ) {
            uint toDeallocate = reservedXGrail - _xGrailBal;

            xGRAIL.deallocate(
                camelotMaster.yieldBooster(),
                toDeallocate,
                abi.encode(address(spNFT), spNftTokenId)
            );
        }

        if (reservedXGrail < _xGrailBal && lpTotal != balance) {
            uint toAllocate = _xGrailBal - reservedXGrail;
            address yieldBooster = camelotMaster.yieldBooster();
            _makeSureXGrailUsageIsApproved(
                toAllocate,
                IXGrailTokenUsage(yieldBooster)
            );
            xGRAIL.allocate(
                yieldBooster,
                toAllocate,
                abi.encode(address(spNFT), spNftTokenId)
            );
        }

        (address lpToken, , , , , , , ) = spNFT.getPoolInfo();
        lpToken.safeTransfer(msg.sender, balance);
        emit Burn(pid, amount, balance);
        return pid;
    }

    /// @dev stake without minting (reinvest purpose)
    /// @param pid pool id
    /// @param amount amount of LP to stake
    function stake(uint256 pid, uint256 amount) external virtual override {
        require(msg.sender == address(dexHelper), "only dexHelper");
        (address lpToken, INFTPool spNFT, uint spNftTokenId) = _getInfoByPid(
            pid
        );

        IERC20Upgradeable(lpToken).safeTransferFrom(
            msg.sender,
            address(this),
            amount
        );

        (uint lpTotal, , , , , , , ) = spNFT.getStakingPosition(spNftTokenId);
        IERC20Upgradeable(lpToken).safeApprove(address(spNFT), amount);
        spNFT.addToPosition(spNftTokenId, amount);

        _updateLpGrowthFactor(pid, (amount * 1e18) / lpTotal);

        emit Stake(pid, amount);
    }

    function reinvest(
        uint pid,
        address[] memory path,
        uint lpMin
    ) external onlyDedicatedWorker nonReentrant {
        _harvestReward(pid);
        AutoCompoundInfo storage info = autoCompoundRewards[path[0]];
        uint rewardAmount = info.poolRewards[pid]; 
        info.poolRewards[pid] = 0;
        require(info.total >= rewardAmount, "reward balance < rewardAmount");
        info.total = info.total.sub(rewardAmount);
        path[0].safeApprove(address(dexHelper), rewardAmount);
        uint256 stakeAmount = dexHelper.reinvestLP(pid, path, rewardAmount);
        require(stakeAmount >= lpMin, "reinvest: lpMin");
    }

