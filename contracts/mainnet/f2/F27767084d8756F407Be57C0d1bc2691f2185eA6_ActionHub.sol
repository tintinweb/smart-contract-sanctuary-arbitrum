pragma solidity 0.8.25;

import "../Traits/extras/recovery/BlackHolePrevention.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

/*
    GLP Balance in the sky contract

    -- 18 decimals
*/
contract GLP is Ownable, BlackHolePrevention {
    
    enum GlobalBalanceType {
        NONE,
        DEV_ID,
        ARTIST_ID,
        EXIT_ID,
        COMMUNITY_ID,
        COMMUNITY_ID_DISCOUNT_POOL
    }

    enum CommunityLevelBalanceType {
        NONE,
        TOKEN,
        SPLITTER,
        VAULT
    }

    string      public name = "GLP";
    string      public symbol = "GLP";
    uint256     public decimals = 18;
    uint256     public totalSupply = 0;

    // balance type =>  uid => uint value
    mapping(GlobalBalanceType => mapping( uint256 => uint256) ) balance;

    //                   TYPE         =>     communityId =>         collectionId     => tokenId  => value
    //                   TYPE         =>     communityId =>         zero             => entityId => value
    mapping(CommunityLevelBalanceType => mapping(uint256 => mapping( uint256 => mapping( uint256 => uint256)))) communityLevelBalanceType;

    event TotalSupplyChangedTo(uint256 indexed _amount);
    event CommunityLevelBalanceChange(CommunityLevelBalanceType _type, uint256 _communityId, uint256 _entityA, uint256 _id, uint256 _newAmount);
    event GlobalTypeBalanceChange(GlobalBalanceType _type, uint256 _id, uint256 _newAmount);

    function UpdateTotal(bool add, uint256 _amount) internal {
        if(add) {
            totalSupply+=_amount;
        } else {
            totalSupply-=_amount;
        }
        emit TotalSupplyChangedTo(totalSupply);
    }

    function addToCLevelBalance(CommunityLevelBalanceType _type, uint256 communityId, uint256 entityA, uint256 _id, uint256 _amount) public onlyRole {
        communityLevelBalanceType[_type][communityId][entityA][_id]+=_amount;
        emit CommunityLevelBalanceChange(_type, communityId, entityA, _id, communityLevelBalanceType[_type][communityId][entityA][_id]);
        UpdateTotal(true, _amount);
    }

    function subFromCLevelBalance(CommunityLevelBalanceType _type, uint256 communityId, uint256 entityA, uint256 _id, uint256 _amount) public onlyRole {
        communityLevelBalanceType[_type][communityId][entityA][_id]-=_amount;
        emit CommunityLevelBalanceChange(_type, communityId, entityA, _id, communityLevelBalanceType[_type][communityId][entityA][_id]);
        UpdateTotal(false, _amount);
    }

    function addToGTypeBalance(GlobalBalanceType _type, uint256 _id, uint256 _amount) public onlyRole {
        balance[_type][_id]+=_amount;
        emit GlobalTypeBalanceChange(_type, _id, balance[_type][_id]);
        UpdateTotal(true, _amount);
    }

    function subFromGTypeBalance(GlobalBalanceType _type, uint256 _id, uint256 _amount) public onlyRole {
        balance[_type][_id]-=_amount;
        emit GlobalTypeBalanceChange(_type, _id, balance[_type][_id]);
        UpdateTotal(false, _amount);
    }

    // views

    function getTokenBalancesFor(uint256 communityId, uint256 collectionId, uint256 tokenId) public view returns ( uint256 result) {
        return communityLevelBalanceType[CommunityLevelBalanceType.TOKEN][communityId][collectionId][tokenId];
    }

    function getCLevelBalancesFor(
        CommunityLevelBalanceType[] memory _type,
        uint256[] memory communityId,
        uint256[] memory entityA,
        uint256[] memory ids
    ) public view returns ( uint256[] memory results) {
        results = new uint256[](ids.length);
        for(uint256 i = 0; i < ids.length; i++ ) {
            results[i] = 
            communityLevelBalanceType[_type[i]][communityId[i]][entityA[i]][ids[i]];
        }
    }

    function getGTypeBalancesFor(GlobalBalanceType[] memory _type, uint256[] memory ids) public view returns ( uint256[] memory results) {
        results = new uint256[](ids.length);
        for(uint256 i = 0; i < ids.length; i++ ) {
            results[i] = balance[ _type[i] ][ ids[i] ];
        }
    }

    modifier onlyRole() {
        // check role
        _;
    }
}

