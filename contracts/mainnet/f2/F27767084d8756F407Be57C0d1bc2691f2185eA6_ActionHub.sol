interface IPaymentMatrix {
    function getArtistIDAndAmountForCollection(uint32 _communityId, uint32 _collectionId) external view returns(uint256 artistId, uint256 amount);
}


contract ActionHub is IActionHub, IERC1155Receiver, UsesGalaxisRegistry {
    using Strings for uint256;

    function version() public pure returns (uint256) {
        return 2024060701; 
    }

    IERC20          public GLXContractInstance;
    GLP             public GLPContractInstance;
    address         public GLX_TO_GLP_CONVERTER;
    IPaymentMatrix  public PaymentMatrixContractInstance;
    TokenDepositor  public TokenDepositorContractInstance;
    uint256         public TokenMembershipTier1;

    error RegistryNotLoaded();
    error CommunityListNotLoaded(address);
    error TraitRegistryNotLoaded(uint32,string,address);
    error CommunityRegistryNotLoaded(address);

    constructor(address _galaxisRegistry) UsesGalaxisRegistry(_galaxisRegistry){
        refreshRegistries();
    }

    function refreshRegistries() public {
        GLXContractInstance             = IERC20(galaxisRegistry.getRegistryAddress("GALAXIS_TOKEN"));
        TokenDepositorContractInstance  = TokenDepositor(galaxisRegistry.getRegistryAddress("GALAXIS_DEPOSIT"));
        TokenMembershipTier1           =  galaxisRegistry.getRegistryUINT("MEMBERSHIP_TIER_1");

        GLPContractInstance             = GLP(galaxisRegistry.getRegistryAddress("GLP"));
        GLX_TO_GLP_CONVERTER            = galaxisRegistry.getRegistryAddress("GLX_TO_GLP_CONVERTER");
        PaymentMatrixContractInstance   = IPaymentMatrix(galaxisRegistry.getRegistryAddress("PAYMENT_MATRIX"));

        require(address(GLXContractInstance) != address(0), "ActionHub: Key GALAXIS_TOKEN needs to be != address(0)");
        require(address(TokenDepositorContractInstance) != address(0), "ActionHub: Key GALAXIS_DEPOSIT needs to be != address(0)");
        require(TokenMembershipTier1 > 0, "ActionHub: Key MEMBERSHIP_TIER_1 needs to be higher than 1");

        // require(address(GLPContractInstance) != address(0), "ActionHub: Key GLP needs to be != address(0)");
    }

    function takeMyGLX(uint256 amount, bytes calldata userData) external {
        if(userData.length == 0) {
            revert("ActionHub: userData is required.");
        }

        // userData needs to specify which 721 Id we're depositing for.
        (
            uint32 communityId,
            uint32 collectionId,
            uint32 tokenId
        ) = abi.decode(userData, (uint32,uint32,uint32));

        // validate userData
        require(communityId > 0,     "ActionHub: userData communityId must be higher than 0");
        require(collectionId > 0,    "ActionHub: userData collectionId must be higher than 0");
        require(tokenId > 0,         "ActionHub: userData tokenId must be higher than 0");

        uint256 GLPAmount = GLXToGLPConverter(GLX_TO_GLP_CONVERTER).getGLXToGLPConvertedAmount(amount);
        // allocate GLP to a token id
        GLPContractInstance.addToCLevelBalance(GLP.CommunityLevelBalanceType.TOKEN, communityId, collectionId, tokenId, GLPAmount);

        // whole thing should just fail if we don't have enough balance anyway.. no need to check allowance or balances..
        address GLXVault = galaxisRegistry.getRegistryAddress("GLX_VAULT");
        GLXContractInstance.transferFrom(msg.sender, GLXVault, amount);
    }

    function getExpandedTokenContracts(
        uint32 _communityId,
        uint32 _collectionId,
        uint16 _traitId
    ) internal view returns (ExpandedTokenContracts memory) {
        ICommunityList COMMUNITY_LIST = ICommunityList(galaxisRegistry.getRegistryAddress("COMMUNITY_LIST"));
        (,address _communityRegistryContract,) = COMMUNITY_LIST.communities(_communityId);
        if (_communityRegistryContract.code.length == 0) {
            revert CommunityRegistryNotLoaded(_communityRegistryContract);
        }

        string memory TOKEN_NUMBER_KEY = string.concat("TOKEN_",uint256(_collectionId).toString());
        string memory TRAIT_REGISTRY_KEY = string.concat("TRAIT_REGISTRY_",uint256(_collectionId).toString());

        CommunityRegistry _tokenCommunityRegistry  = CommunityRegistry(_communityRegistryContract);
        address _tokenContract = _tokenCommunityRegistry.getRegistryAddress(TOKEN_NUMBER_KEY);
        address _traitRegistryContract = _tokenCommunityRegistry.getRegistryAddress(TRAIT_REGISTRY_KEY);

        IECRegistry TR = IECRegistry(_traitRegistryContract);
        address _traitImplementerContract = TR.getImplementer(_traitId);

        // implementer of first trait so we can check owner
        address _traitImplementerContractFirstTrait = TR.getImplementer(0);

        // get membership token contract
        address _membershipTokenContract;
        if(_tokenCommunityRegistry.getRegistryBool("IS_HOME_CHAIN")) {
            _membershipTokenContract = _tokenCommunityRegistry.getRegistryAddress("TOKEN_1");
        } else {
            _membershipTokenContract = _tokenCommunityRegistry.getRegistryAddress("CHILD_TOKEN_1");
        }

        return ExpandedTokenContracts(
            _communityRegistryContract,
            _tokenContract,
            _traitRegistryContract,
            _traitImplementerContract,
            _traitImplementerContractFirstTrait,
            _membershipTokenContract,
            address(TokenDepositorContractInstance)
        ); 
    }

    function payTraitUsageCost(IGenericTrait _Trait, UserAction memory _msg) internal {

        bytes32[] memory propNames = new bytes32[](3);
        propNames[0] = "royalty_id";
        propNames[1] = "royalty_amount";
        propNames[2] = "status";

        bytes[] memory props = _Trait.getProperties(_msg.tokenId, propNames);

        uint256 royalty_id      = uint256(bytes32(props[0]));
        uint256 royalty_amount  = uint256(bytes32(props[1]));
        uint8 status            = abi.decode(props[2], (uint8));

        // TraitStatus.ACTIVE
        require(status == 1, "ActionHub: Trait status != TraitStatus.ACTIVE");

        uint256 tokenGLPBalance = GLPContractInstance.getTokenBalancesFor(
            _msg.communityId,
            _msg.collectionId,
            _msg.tokenId
        );

        require(tokenGLPBalance >= royalty_amount, "ActionHub: GLP balance lower than trait usage cost");

        // subtract usage cost from token's balance
        GLPContractInstance.subFromCLevelBalance(GLP.CommunityLevelBalanceType.TOKEN, _msg.communityId, _msg.collectionId, _msg.tokenId, royalty_amount); 

        // do payment splitting
        uint256 amountRemaining = _processPaymentInternals(royalty_amount, _msg.communityId);

        // send remaining payment to Dev
        GLPContractInstance.addToGTypeBalance(GLP.GlobalBalanceType.DEV_ID, royalty_id, amountRemaining);
    }

    function _processPaymentInternals(uint256 royalty_amount, uint256 _communityId) internal returns(uint256) {
        // TODO: maybe use fractions so we don't loose too much precision?
        uint256 amountRemaining = royalty_amount;

        // we burn 10%
        uint256 GLP_BURN_PERCENTAGE = galaxisRegistry.getRegistryUINT("GLP_BURN_PERCENTAGE");   // in millis
        // sub burned amount
        amountRemaining -= (royalty_amount / 1000) * GLP_BURN_PERCENTAGE;

        // GLAXIS gets 10%
        // uint256 GLP_GALAXIS_TAKE_PERCENTAGE = galaxisRegistry.getRegistryUINT("GLP_GALAXIS_TAKE_PERCENTAGE");
        // if(GLP_GALAXIS_TAKE_PERCENTAGE > 0) {
        //     uint256 galaxisTenPercent = (royalty_amount / 100) * GLP_GALAXIS_TAKE_PERCENTAGE;
        //     amountRemaining -= galaxisTenPercent;
        //     GLPContractInstance.addToTypeBalance(GLP.BalanceType.DEV_ID, 1, galaxisTenPercent);
        // }

        // first look at per community setting
        uint256 GLP_COMMUNITY_TAKE_PERCENTAGE = galaxisRegistry.getRegistryUINT(
            string.concat("GLP_COMMUNITY_TAKE_PERCENTAGE_",uint256(_communityId).toString())
        );
        if(GLP_COMMUNITY_TAKE_PERCENTAGE == 0) {
            // default fallback
            GLP_COMMUNITY_TAKE_PERCENTAGE = galaxisRegistry.getRegistryUINT("GLP_COMMUNITY_TAKE_PERCENTAGE");
        }
        uint256 CommunityBalance = (royalty_amount / 1000) * GLP_COMMUNITY_TAKE_PERCENTAGE; // in millis

        uint256 GLP_COMMUNITY_DISCOUNT_POOL_P = galaxisRegistry.getRegistryUINT(
            string.concat("GLP_COMMUNITY_DISCOUNT_POOL_P")
        );
        uint256 CommunityDiscountAmount = (CommunityBalance / 1000) * GLP_COMMUNITY_DISCOUNT_POOL_P; // in millis 

        // sub community cut
        amountRemaining -= CommunityBalance;

        // add to the community discount pool balance
        GLPContractInstance.addToGTypeBalance(GLP.GlobalBalanceType.COMMUNITY_ID_DISCOUNT_POOL, _communityId, CommunityDiscountAmount);

        // add to the community balance
        GLPContractInstance.addToGTypeBalance(GLP.GlobalBalanceType.COMMUNITY_ID, _communityId, (CommunityBalance - CommunityDiscountAmount) );

        return amountRemaining;
    }


    function royaltiesPaid(
        uint32 _communityId,
        uint32 _collectionId,
        uint32 _tokenId
    ) external view returns (bool) {
        ExpandedTokenContracts memory etc = getExpandedTokenContracts(
            _communityId,
            _collectionId,
            0   // first trait
        );
        return _royaltiesPaid(etc._traitImplementerContractFirstTrait, _tokenId);
    }

    function _royaltiesPaid(
        address _traitImplementerContract,
        uint32 _tokenId
    ) internal view returns (bool) {

        require(_traitImplementerContract != address(0), "ActionHub: trait implementer cannot be address(0)");
        IGenericTrait Trait = IGenericTrait(_traitImplementerContract);

        bytes32[] memory propNames = new bytes32[](4);
        propNames[0] = "movement_permission";
        propNames[1] = "owner_stored";
        propNames[2] = "owner_current";
        propNames[3] = "status";

        bytes[] memory props = Trait.getProperties(_tokenId, propNames);
        uint8   movement_permission = abi.decode(props[0], (uint8));
        address owner_stored        = abi.decode(props[1], (address));
        address owner_current       = abi.decode(props[2], (address));
        uint8   status              = abi.decode(props[3], (uint8));

        //  MovementPermission.LOCKED
        require(movement_permission == 2, "ActionHub: first trait implementer has to have MovementPermission.LOCKED!");

        // TraitStatus.ACTIVE
        require(status == 1, "ActionHub: Trait status != TraitStatus.ACTIVE");

        if(owner_stored == address(0)) {
            return true;
        }

        if(owner_stored == owner_current) {
            return true;
        }

        return false;

    }

    function payRoyalties(
        uint32 _communityId,
        uint32 _collectionId,
        uint32 _tokenId,
        address _traitImplementerContract
    ) internal {

        // get how much we need to pay
        (uint256 royalty_id, uint256 royalty_amount) = PaymentMatrixContractInstance.getArtistIDAndAmountForCollection(_communityId, _collectionId);

        IGenericTrait _Trait = IGenericTrait(_traitImplementerContract);
        uint8 status            = abi.decode( _Trait.getProperty("status", _tokenId), (uint8));

        // TraitStatus.ACTIVE
        require(status == 1, "ActionHub: Trait status != TraitStatus.ACTIVE");

        if(royalty_amount > 0) {

            uint256 tokenGLPBalance = GLPContractInstance.getTokenBalancesFor(
                _communityId,
                _collectionId,
                _tokenId
            );

            require(tokenGLPBalance >= royalty_amount, "ActionHub: GLP balance lower than trait usage cost");

            // subtract usage cost from token's balance
            GLPContractInstance.subFromCLevelBalance(GLP.CommunityLevelBalanceType.TOKEN, _communityId, _collectionId, _tokenId, royalty_amount); 

            // do payment splitting
            uint256 amountRemaining = _processPaymentInternals(royalty_amount, _communityId);

            // send remaining payment to Dev
            GLPContractInstance.addToGTypeBalance(GLP.GlobalBalanceType.ARTIST_ID, royalty_id, amountRemaining);
        }

        // we increment the counter each time royalties are paid, this also updates current owner of the token
        _Trait.incrementCounter(_tokenId);
    }

    function doAction(UserAction[] memory messages) public {

        require(galaxisRegistry.getRegistryAddress("ACTION_HUB") == address(this), "ActionHub: Not current Action Hub.");

        require(messages.length > 0, "ActionHub: messages.length at least 1");
        for(uint8 i = 0; i < messages.length; i++) {
            UserAction memory _msg = messages[i];

            ExpandedTokenContracts memory etc = getExpandedTokenContracts(
                _msg.communityId,
                _msg.collectionId,
                _msg.traitId
            );

            // royalties DISABLED in favour of membershipCardBalance
            // if(!_royaltiesPaid(etc._traitImplementerContractFirstTrait, _msg.tokenId)) {
            //     // try to pay them
            //     payRoyalties(
            //         _msg.communityId,
            //         _msg.collectionId,
            //         _msg.tokenId,
            //         etc._traitImplementerContractFirstTrait
            //     );
            // }

            require(IERC721(etc._membershipTokenContract).ownerOf(_msg.membershipCardId) == msg.sender, "ActionHub: Membership Card not owned by msg.sender");
            uint256 membershipCardBalance = TokenDepositorContractInstance.balances(etc._membershipTokenContract, _msg.membershipCardId);
            require(membershipCardBalance >= TokenMembershipTier1, "ActionHub: Membership Card balance too low");

            // load this tokens' and traits' contract addresses
            require(IERC721(etc._tokenContract).ownerOf(_msg.tokenId) == msg.sender, "ActionHub: Token not owned by msg.sender");
            require(etc._traitImplementerContract != address(0), "ActionHub: trait implementer cannot be address(0)");
            IGenericTrait Trait = IGenericTrait(etc._traitImplementerContract);

            require(Trait.hasTrait(_msg.tokenId), "ActionHub: Token does not have specified trait");

            // if(_msg._type == ActionType.USE_TRAIT ) {
            //     payTraitUsageCost(Trait, _msg);
            // }

            IGenericTraitConsumer(_msg.consumer).doAction(msg.sender, etc, _msg);
        }
    }


    function handle1155Received(
        address from,
        uint256 received1155TokenId,
        uint256 messageIndex,
        bytes calldata userData
    ) internal {
        
        // do trait extraction ( in dave's branch )
    }

    /**
     * @dev Receive any ERC1155 using safeTransfer
     */
    function onERC1155Received(
        address operator,
        address from,
        uint256 id,
        uint256 value,
        bytes calldata userData
    ) external returns (bytes4) {
        if(userData.length == 0) {
            // just store tokens.. do nothing
        } else {
            require(operator == address(0), "ActionHub: no operator allowed");
            require(value == 1, "ActionHub: Can only transfer one per tokenId!");
            // should we only accept ERC1155 TeleportToken ( communityTeleportAddress ) for this community ??
            handle1155Received(from, id, 0, userData);
        }
        return this.onERC1155Received.selector;
    }

    /**
     * @dev Receive any ERC1155 Batch using safeTransfer
     */
    function onERC1155BatchReceived(
        address operator, 
        address from, 
        uint256[] calldata ids, 
        uint256[] calldata values, 
        bytes calldata userData
    ) external returns (bytes4) {
        if(userData.length == 0) {
            // just store tokens.. do nothing
        } else {
            require(operator == address(0), "ActionHub: no operator allowed");
            for (uint256 idpos = 0; idpos < ids.length; idpos++) {
                uint256 value = values[idpos];
                require(value == 1, "ActionHub: Can only transfer one per tokenId!");
                handle1155Received(from, ids[idpos], idpos, userData);
            }
        }
        return this.onERC1155BatchReceived.selector;
    }

    function supportsInterface(bytes4 interfaceId) external pure returns (bool) {
        return (
            interfaceId == type(IERC1155Receiver).interfaceId ||
            interfaceId == type(IERC721Receiver).interfaceId ||
            interfaceId == type(IERC165).interfaceId
        );
    }

}

